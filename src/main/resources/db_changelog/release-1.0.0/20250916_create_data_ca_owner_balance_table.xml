<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="create_table_data_ca_owner_balance" author="borisov-tyur">
        <preConditions>
            <not>
                <tableExists tableName="data_ca_owner_balance" />
            </not>
        </preConditions>
        <createTable tableName="data_ca_owner_balance">
            <column name="owner_security_id" type="BIGINT">
                <constraints primaryKey="true"/>
            </column>
            <column name="bal" type="BIGINT"/>
            <column name="create_date_time" type="TIMESTAMP WITH TIME ZONE"/>
            <column name="caid" type="BIGINT"/>
            <column name="cftid" type="BIGINT"/>
        </createTable>
    </changeSet>
</databaseChangeLog>