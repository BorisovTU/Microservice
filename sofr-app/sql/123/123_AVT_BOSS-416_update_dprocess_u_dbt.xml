  <changeSet id="123_AVT_BOSS-416_update_dprocess_u_dbt.xml" author="Nikolaev">
    <comment>Освобождение номеров 22,23 для новых пунктов биржевого автомата с T_TYPE = 1</comment>
    
    <update tableName="dprocess_u_dbt">
      <column name="T_NUM" valueComputed="T_NUM + 2"/>
      <where>T_TYPE = 1 AND T_NUM > 21</where>
    </update>
    
  </changeSet>
