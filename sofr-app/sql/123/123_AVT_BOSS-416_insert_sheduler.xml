<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd
    http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.6.xsd ">
   <changeSet id="123_AVT_BOSS-416_insert_sheduler" author="Nikolaev" >
      <insert tableName="DSIMPLESERVICE_DBT">
          <column name="T_ID" valueNumeric="10121" />
          <column name="T_NAME" value="Запуск генерации и удерж. НДФЛ по мат. выгоде (БА)" />
          <column name="T_DESCRIPTION" value="Запуск генерации и удерж. НДФЛ по мат. выгоде (БА)" />
          <column name="T_ISACTIVE" valueComputed="CHR(0)" />
      </insert>

      <insert tableName="DSS_FUNC_DBT">
          <column name="T_ID" valueNumeric="100121" />
          <column name="T_SERVICE" valueNumeric="10121" />
          <column name="T_LEVEL" value="1" />
          <column name="T_NAME" value="Запуск генерации и удерж. НДФЛ по мат. выгоде (БА)" />
          <column name="T_TYPE" valueNumeric="1" />
          <column name="T_EXECUTORNAME" value="AutoProcess_is_dss" />
          <column name="T_EXECUTORFUNC" value="RunProcess_dss" />
          <column name="T_STARTDELAY" valueNumeric="0" />
          <column name="T_PERIOD" valueNumeric="0" />
          <column name="T_MAXATTEMPT" valueNumeric="1" />
          <column name="T_PARAMETERS" valueComputed="TO_CLOB('611')" />
      </insert>

      <insert tableName="DSS_SHEDULER_DBT">
          <column name="T_ID" valueNumeric="10121" />
          <column name="T_NAME" value="Запуск генерации и удерж. НДФЛ по мат. выгоде (БА)" />
          <column name="T_DESCRIPTION" value="Запуск генерации и удерж. НДФЛ по мат. выгоде (БА)" />
          <column name="T_SERVICE" valueNumeric="10121" />
          <column name="T_MODULE" valueNumeric="1" />
          <column name="T_SHEDULERTYPE" valueNumeric="0" />
          <column name="T_WORKSTARTTIME" valueComputed="TO_DATE('01/01/0001 06:00:01', 'DD/MM/YYYY HH24:MI:SS')" />
          <column name="T_WORKENDTIME" valueComputed="TO_DATE('01/01/0001 06:49:59', 'DD/MM/YYYY HH24:MI:SS')" />
          <column name="T_STARTTIME" valueComputed="TO_DATE('01/01/0001 06:00:01', 'DD/MM/YYYY HH24:MI:SS')" />
          <column name="T_PERIODTYPE" valueNumeric="0" />
          <column name="T_PERIOD" valueNumeric="0" />
          <column name="T_NEXTSTAMP" valueComputed="trunc(sysdate) + 1 + NUMTODSINTERVAL(6, 'HOUR')" />
          <column name="T_PARAMETERS" valueComputed="TO_CLOB('-subtypes:{611} -date:{curdate} -fixdate:{1} -processtype:1')" />
      </insert>
   </changeSet>
</databaseChangeLog>


