<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd
    http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.6.xsd ">
  <changeSet id="123_AVT_CCBO-90_ins_llv_3_1" author="Ponomareva">
      <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                select count(1) from dllvalues_dbt where t_list = 5019 and t_element = 5
            </sqlCheck>
      </preConditions>
      <insert tableName="dllvalues_dbt">
          <column name="t_list" value="5019" />
          <column name="t_element" value="5" />
          <column name="t_code" value="КорАсОтР" />
          <column name="t_name" value="Доход от корректировок стоимости, отсроченная разница по собств.облиг." />
          <column name="t_flag" value="5" />
          <column name="t_note" value="Доход от корректировок стоимости, отсроченная разница по собств.облиг." />
          <column name="t_reserve" valueComputed="chr(1)" />
      </insert>
      
  </changeSet>

  <changeSet id="123_AVT_CCBO-90_ins_llcl_3_2" author="Ponomareva">
    <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                select count(1) from dllclsval_dbt where t_classificator = 5004 and t_list = 5019 and t_element = 5
            </sqlCheck>
      </preConditions>
     <insert tableName="dllclsval_dbt">
        <column name="t_classificator" valueNumeric="5004" />
        <column name="t_element" valueNumeric="5" />
        <column name="t_list" valueNumeric="5019" />
        <column name="t_issys" valueComputed="CHR(0)" />
        <column name="t_sort" valueNumeric="0" />
    </insert>
  </changeSet>

</databaseChangeLog>
