<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd
                            http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.6.xsd ">

    <changeSet id="AVT_BOSS-544-create_table_sofr_ccppricerange" author="Simanov">
        <createTable tableName="dccppricerange_dbt">
            <column name="id" type="number(12)" remarks="ИД">
                <constraints primaryKey="true" primaryKeyName="pk_ccppricerange"/>
            </column>

            <column name="t_seccode" type="varchar2(12)" remarks="Код инструмента">
                <constraints nullable="false"/>
            </column>

            <column name="t_loaddate" type="date" remarks="Дата торговой сессии">
                <constraints nullable="false"/>
            </column>

            <column name="t_Discount_CCP" type="number(19, 8)" remarks="Рыночный риск, %.">
                <constraints nullable="false"/>
            </column>

            <column name="t_created_at" type="timestamp" remarks="Время создания записи" defaultValueComputed="systimestamp">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>