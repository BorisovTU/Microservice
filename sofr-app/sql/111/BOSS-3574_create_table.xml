<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd
    						http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.6.xsd ">

    <changeSet id="BOSS-3574_create_table" author="Shishkin">
        <createTable tableName="DNPTXOP_INVESTMENTS_TMP">
            <column name="t_id" type="number(3)">
                <constraints nullable="true"></constraints>
            </column>
            <column name="t_name" type="varchar2(100)">
                <constraints nullable="true"></constraints>
            </column>
            <column name="t_client" type="number(10)">
                <constraints nullable="true"></constraints>
            </column>
            <column name="t_number" type="varchar2(64)">
                <constraints nullable="true"></constraints>
            </column>
            <column name="t_type_board" type="varchar2(8)">
                <constraints nullable="true"></constraints>
            </column>
            <column name="t_amount" type="number(32,12)">
                <constraints nullable="true"></constraints>
            </column>
            <column name="t_currency" type="varchar2(3)">
                <constraints nullable="true"></constraints>
            </column>
            <column name="t_timestamp" type="date">
                <constraints nullable="true"></constraints>
            </column>
            <column name="t_status" type="char(1)">
                <constraints nullable="true"></constraints>
            </column>
            <column name="t_send_time" type="date">
                <constraints nullable="true"></constraints>
            </column>
            <column name="t_guid" type="varchar2(32)">
                <constraints nullable="true"></constraints>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>