<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:pro="http://www.liquibase.org/xml/ns/pro" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd
    http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.6.xsd ">
   <changeSet id="DEF-83757_create_table" author="SennikovIV">

      <createTable tableName="DMSESDATA_DBT" remarks="Таблица межсессионных данных">
         <column name="T_PROCESSID" type="VARCHAR2(40 BYTE)" remarks="Идентификатор процесса"/>
         <column name="T_CLASSNAME" type="VARCHAR2(100 BYTE)" remarks="Имя класса"/>
         <column name="T_STRID" type="VARCHAR2(100 BYTE)" remarks="Строковое уникальное ID"/>
         <column name="T_DATA" type="CLOB" remarks="Данные структуры"/>
      </createTable>

      <createIndex indexName="DMSESDATA_DBT_IDX0" tableName="DMSESDATA_DBT" tablespace="INDX" unique="false">
         <column name="T_PROCESSID"/>
      </createIndex>

      <createIndex indexName="DMSESDATA_DBT_IDX1" tableName="DMSESDATA_DBT" tablespace="INDX" unique="true">
         <column name="T_STRID"/>
         <column name="T_CLASSNAME"/>
         <column name="T_PROCESSID"/>
      </createIndex>

      <createIndex indexName="DMSESDATA_DBT_IDX2" tableName="DMSESDATA_DBT" tablespace="INDX" unique="false">
         <column name="T_PROCESSID"/>
         <column name="T_CLASSNAME"/>
      </createIndex>

   </changeSet>

</databaseChangeLog>