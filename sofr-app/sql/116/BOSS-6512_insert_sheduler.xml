<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd
    http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.6.xsd ">
   <property name="now" value="sysdate" dbms="oracle"/>
   <changeSet id="BOSS-6512_insert_sсheduler" author="SennikovIV" >
   
      <insert tableName="DSIMPLESERVICE_DBT">
          <column name="T_ID" valueNumeric="10108" />
          <column name="T_NAME" value="Урег. остат. на счет. НДФЛ к возвр. в дату резер." />
          <column name="T_DESCRIPTION" value="Урегулирование остатков на счетах НДФЛ к возврату в дату резервирования" />
          <column name="T_ISACTIVE" value="X" />
      </insert>

      <insert tableName="DSS_FUNC_DBT">
          <column name="T_ID" valueNumeric="100108" />
          <column name="T_SERVICE" valueNumeric="10108" />
          <column name="T_LEVEL" value="1" />
          <column name="T_NAME" value="Урег. остат. на счет. НДФЛ к возвр. в дату резер." />
          <column name="T_TYPE" valueNumeric="1" />
          <column name="T_EXECUTORNAME" value="nptxadjres" />
          <column name="T_EXECUTORFUNC" value="AdjResRestSchedRun" />
          <column name="T_STARTDELAY" valueNumeric="0" />
          <column name="T_PERIOD" valueNumeric="0" />
          <column name="T_MAXATTEMPT" valueNumeric="1" />
      </insert>

      <insert tableName="DSS_SHEDULER_DBT">
          <column name="T_ID" valueNumeric="10108" />
          <column name="T_NAME" value="Урег. остат. на счет. НДФЛ к возвр. в дату резер." />
          <column name="T_DESCRIPTION" value="" />
          <column name="T_SERVICE" valueNumeric="10108" />
          <column name="T_MODULE" valueNumeric="1" />
          <column name="T_SHEDULERTYPE" valueNumeric="2" />
          <column name="T_WORKSTARTTIME" valueComputed="TO_DATE('01/01/0001 00:00:01', 'DD/MM/YYYY HH24:MI:SS')" />
          <column name="T_WORKENDTIME" valueComputed="TO_DATE('01/01/0001 23:59:59', 'DD/MM/YYYY HH24:MI:SS')" />
          <column name="T_STARTTIME" valueComputed="TO_DATE('01/01/0001 20:20:01', 'DD/MM/YYYY HH24:MI:SS')" />
          <column name="T_PERIODTYPE" valueNumeric="4" />
          <column name="T_PERIOD" valueNumeric="1" />
          <column name="T_NEXTSTAMP" valueComputed="trunc(sysdate) + 1 + NUMTODSINTERVAL(20, 'HOUR') + NUMTODSINTERVAL(20, 'MINUTE')" />
      </insert>

   </changeSet>

</databaseChangeLog>
