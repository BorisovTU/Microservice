  <changeSet id="118_DEF-93419_upd_opr_stp_blc" author="Ponomareva">
    <update tableName="DOPROSTEP_DBT">
        <column name="T_SYMBOL" value="ะต"/>
        <where>T_BLOCKID = 20000956 AND T_NUMBER_STEP = 10</where>
    </update>
    <update tableName="DOPROBLCK_DBT">
        <column name="T_SYMBOLSFORINSERTION" valueComputed="T_SYMBOLSFORINSERTION || 'ะต'"/>
        <where>T_BLOCKID = 20000173 and INSTR(T_SYMBOLSFORINSERTION, 'ะต') = 0</where>
    </update>
    <update tableName="DOPRSTEP_DBT">
        <column name="T_SYMBOL" value="ะต"/>
        <where>T_BLOCKID = 20000956 AND T_NUMBER_STEP = 10 AND T_ISEXECUTE = 'R'</where>
    </update>
  </changeSet>
</databaseChangeLog>

