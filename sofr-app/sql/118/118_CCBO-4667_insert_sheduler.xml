<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd
    http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.6.xsd ">
   <changeSet id="118_CCBO-4667_insert_sheduler.xml" author="Shestakov" >
      <insert tableName="DSIMPLESERVICE_DBT">
          <column name="T_ID" valueNumeric="10111" />
          <column name="T_NAME" value="Импорт ИН из RuData" />
          <column name="T_DESCRIPTION" value="Импорт ИН из RuData" />
          <column name="T_ISACTIVE" value="X" />
      </insert>

      <insert tableName="DSS_FUNC_DBT">
          <column name="T_ID" valueNumeric="100111" />
          <column name="T_SERVICE" valueNumeric="10111" />
          <column name="T_LEVEL" value="1" />
          <column name="T_NAME" value="Импорт ИН из RuData" />
          <column name="T_TYPE" valueNumeric="1" />
          <column name="T_EXECUTORNAME" value="ws_check_job" />
          <column name="T_EXECUTORFUNC" value="ws_RuData_FaceValue" />
          <column name="T_STARTDELAY" valueNumeric="0" />
          <column name="T_PERIOD" valueNumeric="0" />
          <column name="T_MAXATTEMPT" valueNumeric="1" />
          <column name="T_TIMEOUT" valueNumeric="7200" />
      </insert>

      <insert tableName="DSS_SHEDULER_DBT">
          <column name="T_ID" valueNumeric="10111" />
          <column name="T_NAME" value="Импорт ИН из RuData" />
          <column name="T_DESCRIPTION" value="Импорт ИН из RuData" />
          <column name="T_SERVICE" valueNumeric="10111" />
          <column name="T_MODULE" valueNumeric="1" />
          <column name="T_SHEDULERTYPE" valueNumeric="2" />
          <column name="T_WORKSTARTTIME" valueComputed="TO_DATE('01/01/0001 02:30:00', 'DD/MM/YYYY HH24:MI:SS')" />
          <column name="T_WORKENDTIME" valueComputed="TO_DATE('01/01/0001 23:59:59', 'DD/MM/YYYY HH24:MI:SS')" />
          <column name="T_STARTTIME" valueComputed="TO_DATE('01/01/0001 02:30:00', 'DD/MM/YYYY HH24:MI:SS')" />
          <column name="T_PERIODTYPE" valueNumeric="4" />
          <column name="T_PERIOD" valueNumeric="1" />
          <column name="T_NEXTSTAMP" valueComputed="TRUNC(SYSDATE) + 1 + NUMTODSINTERVAL(2, 'HOUR') + NUMTODSINTERVAL(30, 'MINUTE') + NUMTODSINTERVAL(1, 'SECOND')" />
      </insert>

   </changeSet>

</databaseChangeLog>