file alg ( "namealg.dbt", "bank.def" );

/**********************************************************/

macro GetNameAlg (TypeAlg,NumberAlg)
  var NameAlg;
  KeyNum (alg,0);
  alg.iTypeAlg = TypeAlg;
  alg.iNumberAlg = NumberAlg;
  if (GetEQ(alg))
    NameAlg = alg.szNameAlg;
  else
    NameAlg = "";
  end;
  return NameAlg;
end;
