    /**
     * Получить значение. Для балансовых первого порядка явно указываем какой тип остатка получать.
     * @param balanceAttribute TBalanceAttribute Атрибут
     * @param balance String Балансовый счет.
     * @return value Значение.
     */
    private macro getValueByTypeOfAccount(balanceAttribute: TBalanceAttribute, balance: String)
        var value = null;
        if ((balance == "102") OR (balance == "107"))
            value = balanceAttribute.getOutRestPassive();
        else
            value = balanceAttribute.getOutRest();
        end;
        return value;
    end;

    /**
     * Установить значение узла по балансовому счету.
     * При формировании узла по балансовому счету первого порядка,
     * необходимо учитывать (функция getValueByTypeOfAccount()),
     * какой тип остатка обрабатывать - активный или пассивный.
     * @param balance Балансовый счет
     * @return balanceNode Узел
     */
    private macro setBalanceNode(balance: String)
        if (m_balanceRcbReport == null)
            return null;
        end;

        var balanceAttribute = TBalanceAttribute("БАЛАНС",  m_balanceRcbReport);
        var isBalanceAttribute = balanceAttribute.getBalanceAttribute(1, balance, false);

        var balanceNode;
        var value = TValue();
            value.exact  = 0;
            value.scaled = 0;

        if (balanceAttribute == null)
            m_protocolView.printLine("Исходящему остатку по счету " + balance + " присвоено нулевое значение, т.к. соответствующая переменная не найдена.");

            balanceNode = initializeNode(this, "Бн" + balance, value);            return null;
        end;

        if (isBalanceAttribute == NULL)
            m_protocolView.printLine("Переменная для балансового счета " + balance + " не определена.");

            balanceNode = initializeNode(this, "Бн" + balance, Value);
        elif (balanceAttribute.getOutRest().isUndefined())
            m_protocolView.printLine("Переменная для балансового счета " + balance + " не определена.");

            balanceNode = initializeNode(this, "Бн" + balance, Value);
        else
            balanceNode = initializeNode(this, "Бн" + balance, getValueByTypeOfAccount(balanceAttribute, balance));
        end;

        balanceNode.setDef(0.5);
        return balanceNode;
    end;

