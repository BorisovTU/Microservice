class (TGlobalBase) F118Global()
    initTGlobalBase();

    private var m_rcbReport = rcbApplication.currentReport;

    macro getRcbReport() : RcbReport
        return m_rcbReport;
    end;

    initializeRepDataPackage(m_rcbReport);
end;

macro global()
    if (v_global == null)
        v_global = F118Global();
    end;
    return v_global;
end;