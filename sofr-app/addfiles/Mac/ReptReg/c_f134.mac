    macro EqДоп()
        m_protocol.printAttributeHeader("EqДоп");

        var value = TValue();
        var eq3_5 = TValue();
        var eqOsnValue    = TValue("EqОсн");
        var eqIstDopValue = TValue("EqИстДоп");
        var eq3_5DannyeValue;
        var eq3_5Dannye10letValue;
        var protocolStr = TArray(0);

        if (eqOsnValue.getExact() > 0)
            if (eqIstDopValue.getExact() > 0)

                if (eqOsnValue.getExact() <= eqIstDopValue.getExact())

                    m_protocol.printLine("Переменная EqОсн (\"Основной капитал\") = " + eqOsnValue.getExact() + " <= переменной EqИстДоп = " + eqIstDopValue.getExact() + ".");
                    m_protocol.printLine("Значение перменной равно " + eqOsnValue.getExact());
                    value = eqOsnValue;

                else

                    value = TValue("Eq3_1").plus(TValue("Eq3_3")).plus(TValue("Eq3_4"));

                    eq3_5DannyeValue = TValue("Eq3_5данные");
                    eq3_5Dannye10letValue = TValue("Eq3_5данные10лет");

                    if (eq3_5DannyeValue.getExact() <= 0.5*eqOsnValue.getExact())
                        protocolStr(0) = "m_protocol.printAttributeLine(\"+Eq3_5данные\", string(eq3_5DannyeValue.getExact() : 0 : 2) + \" руб.\")";
                        protocolStr(1) = "m_protocol.printAttributeLine(\"+Eq3_5данные10лет\", string(eq3_5Dannye10letValue.getExact() : 0 : 2) + \" руб.\")";
                        eq3_5 = TValue(eq3_5DannyeValue.getExact() + eq3_5Dannye10letValue.getExact());
                    else
                        protocolStr(0) = "m_protocol.printAttributeLine(\"+EqОсн * 0.5\", string((0.5*eqOsnValue.getExact()) : 0 : 2) + \" руб.\")";
                        protocolStr(1) = "m_protocol.printAttributeLine(\"+Eq3_5данные10лет\", string(eq3_5Dannye10letValue.getExact() : 0 : 2) + \" руб.\")";
                        eq3_5 = TValue(0.5*eqOsnValue.getExact() + eq3_5Dannye10letValue.getExact());
                    end;

                    if (eqOsnValue.getExact() <= eq3_5.getExact())
                        protocolStr(0) = "m_protocol.printAttributeLine(\"+EqОсн\", string(eqOsnValue.getExact() : 0 : 2) + \" руб.\");";
                        protocolStr.size = 1;
                        eq3_5 = TValue(eqOsnValue.getExact());
                    end;

                    value.plus(eq3_5);
                    for (var i, protocolStr)
                       execExp(i);
                    end;

                    value.plus(TValue("Eq3_6")).plus(TValue("Eq3_7")).plus(TValue("Eq3_9")).minus(TValue("Eq3_10"));

                end;
            else
                m_protocol.printLine("Переменная EqИстДоп <= 0, значение перменной равно нулю");
            end;
        else
            m_protocol.printLine("Переменная EqОсн (\"Основной капитал\") <= 0, значение перменной равно нулю");
        end;

        setAttributeValue(value, "EqДоп");
    end;

