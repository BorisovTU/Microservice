    macro Eq2_1_6()
        m_protocol.printAttributeHeader("Eq2_1_6");

        var value = TValue();

        if (m_parameters.ПрибыльТекущегоГодаПодтверждена)
            m_protocol.PrintLine("Прибыль текущего года подтверждена.");
