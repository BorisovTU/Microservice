        end;

        var sign = "";
        if ((isMinus != NULL) and isMinus)
            sign = "-";
        end;

        var rest = $0.0;
        var dataSet = NULL;
        dataSet = TRsbDataSet("SELECT account.t_account, account.t_rest"
            + "\n" +          "  FROM " + getAccountDataSourceQuery(masks) + " account"
            + "\n" +          " WHERE t_rest <> 0.0"
