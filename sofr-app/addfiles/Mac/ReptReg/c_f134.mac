        m_protocol.printLine("Вычисляем ОС:");
        var ocValue = summarizeAccountRests("60401*, 60404*", getAccountCategoryFilter("5.2 искл", CATEGORY_NOT_EXIST_CONDITION));
        ocValue.plus(summarizeAccountRests("60701*", getAccountCategoryFilter("НМА", CATEGORY_NOT_EXIST_CONDITION)));
