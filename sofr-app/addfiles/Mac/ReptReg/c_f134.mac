    macro Eq2_2_6()
        m_protocol.printAttributeHeader("Eq2_2_6");

        var isMinus = true;
        var reserve = $0.0;
        var rest    = $0.0;

        private macro printAccountAndReserve(account, rest, reserve, isReserve)
            if (isReserve)
               m_protocol.printAccountAndReserve(account, rest, reserve);
            else
               m_protocol.printAccount(account, rest, reserve);
            end;
        end;

        private macro addValues(dataSet, isMinus)
            if ((isMinus != null) and isMinus)
                while (dataSet.moveNext())
                    rest    = rest    - dataSet.rest;
                    reserve = reserve + dataSet.reserve;
                    printAccountAndReserve("-" + dataSet.account, dataSet.rest, dataSet.reserve, dataSet.isReserve);
                end;
            else
                while (dataSet.moveNext())
                    rest    = rest    + dataSet.rest;
                    reserve = reserve + dataSet.reserve;
                    printAccountAndReserve(dataSet.account, dataSet.rest, dataSet.reserve, dataSet.isReserve);
                end;
            end;
        end;

        var selectClause =
                     "SELECT account.t_account, account.t_rest,"
            + "\n" + "       CASE WHEN SUBSTR(account.t_account, 1, 5) IN ('50705', '50718', '60101', '60102', '60103', '60104', '60201', '60202', '60203', '60204')"
            + "\n" + "            THEN 1"
            + "\n" + "            ELSE 0"
            + "\n" + "       END t_isReserve,"
            + "\n" + "       NVL(reserveAccount.t_endDateReserveAmount, 0) t_reserve";
        var commonWhereClause =
              "\n" +        "account.t_rest <> 0.0"
            + "\n" + "   AND account.t_accountId = reserveAccount.t_connectAccountId(+)";

        var acquisitionPurposeClause = "(" + getAccountCategoryFilter("ИП", CATEGORY_EXIST_CONDITION) + ")";
        var contragentAcPortionClause = "(rep_note.readShareInCapital(account.t_contractorId, " + getSqlDate(m_period.endDate()) + ") > 20.0)";
        var contragentCode47Clause = "(" + getCategoryFilter(RCB_OBJTYPE_PARTY ,RCB_OBJGROUP_PT_OKOPF, "47", CATEGORY_EXIST_CONDITION) + ")";
        var contragentCode67Clause = "(" + getCategoryFilter(RCB_OBJTYPE_PARTY ,RCB_OBJGROUP_PT_OKOPF, "67", CATEGORY_EXIST_CONDITION) + ")";
        var contragentIsSubsidiaryClause = "(party.t_isSubsidiary = 1)";
        var account226CategoryClause = "(" + getAccountCategoryFilter(CATEGORY2_2_6искл, CATEGORY_NOT_EXIST_CONDITION) + ")";

        var dataSet;
        //=========================================================================================
        dataSet = TRsbDataSet(selectClause
            + "\n" + "  FROM " + getAccountDataSourceQuery("60101*") + " account,"
            + "\n" + "       drepReserveAccount_vw reserveAccount"
            + "\n" + " WHERE " + commonWhereClause
            + "\n" + "   AND (   (" + acquisitionPurposeClause + " AND " + contragentAcPortionClause + ")"
            + "\n" + "        OR " + contragentCode67Clause
            + "\n" + "        OR (" + contragentCode47Clause + " AND " + account226CategoryClause + ")"
            + "\n" + "       )");

        addValues(dataSet);
        //=========================================================================================
        dataSet = TRsbDataSet(selectClause
            + "\n" + "  FROM " + getAccountDataSourceQuery("60102*-60104*") + " account,"
            + "\n" + "       drepReserveAccount_vw reserveAccount"
            + "\n" + " WHERE " + commonWhereClause
            + "\n" + "   AND " + acquisitionPurposeClause + " AND " + contragentAcPortionClause);

        addValues(dataSet);
        //=========================================================================================
        dataSet = TRsbDataSet(selectClause
            + "\n" + "  FROM " + getAccountDataSourceQuery("60201*") + " account,"
            + "\n" + "       drepReserveAccount_vw reserveAccount"
            + "\n" + " WHERE " + commonWhereClause);

        addValues(dataSet);
        //=========================================================================================
        dataSet = TRsbDataSet(selectClause
            + "\n" + "  FROM " + getAccountDataSourceQuery("60202*, 60203*, 60204*") + " account,"
            + "\n" + "       drepparty_vw party,"
            + "\n" + "       drepReserveAccount_vw reserveAccount"
            + "\n" + " WHERE account.t_contractorId = party.t_id"
            + "\n" + "   AND " + commonWhereClause            + " AND " + acquisitionPurposeClause
            + "\n" + "   AND " + contragentIsSubsidiaryClause + " AND " + contragentAcPortionClause);

        addValues(dataSet);
        //=========================================================================================
        dataSet = TRsbDataSet(selectClause
            + "\n" + "  FROM " + getAccountDataSourceQuery("50605*, 50618*, 50705*, 50718*, 50621*, 50721*") + " account,"
            + "\n" + "       drepparty_vw party,"
            + "\n" + "       drepReserveAccount_vw reserveAccount"
            + "\n" + " WHERE account.t_contractorId = party.t_id"
            + "\n" + "   AND " + commonWhereClause
            + "\n" + "   AND (   (" + acquisitionPurposeClause + " AND " + contragentIsSubsidiaryClause + " AND " + contragentAcPortionClause + ")"
            + "\n" + "        OR " + contragentCode67Clause
            + "\n" + "        OR (" + contragentCode47Clause + " AND " + account226CategoryClause + ")"
            + "\n" + "       )");

        addValues(dataSet);
        //=========================================================================================
        dataSet = TRsbDataSet(selectClause
            + "\n" + "  FROM " + getAccountDataSourceQuery("50620*, 50720*") + " account,"
            + "\n" + "       drepparty_vw party,"
            + "\n" + "       drepReserveAccount_vw reserveAccount"
            + "\n" + " WHERE account.t_contractorId = party.t_id"
            + "\n" + "   AND " + commonWhereClause
            + "\n" + "   AND (   (" + acquisitionPurposeClause + " AND " + contragentIsSubsidiaryClause + " AND " + contragentAcPortionClause + ")"
            + "\n" + "        OR " + contragentCode67Clause
            + "\n" + "        OR (" + contragentCode47Clause + " AND " + account226CategoryClause + ")"
            + "\n" + "       )");

        addValues(dataSet, isMinus);
        //=========================================================================================

        setAttributeValue(TValue(rest-reserve).plus(TValue("Eq2_2_6ввод")), "Eq2_2_6");
    end;

