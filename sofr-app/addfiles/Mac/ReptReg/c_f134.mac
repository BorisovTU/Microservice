    private macro dbgIsEqualLoops(one : RcbArray, another : RcbArray)
        if (one.getCount() != another.getCount())
            return false;
        end;

        one.moveFirst();
        another.moveFirst();
        another.moveNext();
        while (one.moveNext())
            if (one.getCurrentItem() == another.getCurrentItem())
                while (one.moveNext())
                    another.moveNext();
                    if (one.getCurrentItem() != another.getCurrentItem())
                        return false;
                    end;
                end;
                one.moveFirst();
                while (another.moveNext())
                    one.moveNext();
                    if (one.getCurrentItem() != another.getCurrentItem())
                        return false;
                    end;
                end;
                return true;
            end;
        end;
    end;

    private macro dbgAddLoop(tail : RcbArray)
        var loop : RcbArray = RcbArray();
        while (tail.moveNext())
            loop.push_back(tail.getCurrentItem());
        end;

        loops.moveFirst();
        while (loops.moveNext())
            if (dbgIsEqualLoops(loops.getCurrentItem(), loop))
                return;
            end;
        end;
        loops.push_back(loop);
    end;

    /**
     * Искать циклы начиная с указанного атрибута
     *
     * @param     calculationAttributeData
     */
    private macro dbgFindLoopFrom(calculationAttributeData : T134CalculationAttributeData, tail : RcbArray)
        if ((calculationAttributeData == null) or calculationAttributeData.isCalculated())
            tail.pop_back();
            return;
        end;

        tail.moveFirst();
        while (tail.moveNext())
            if (    (tail.getCurrentItem() == calculationAttributeData.getAttribute())
                and not ((tail.getCurrentIndex()+1) == tail.getCount()))

                dbgAddLoop(tail);
                tail.pop_back();
                return;
            end;
        end;

        var dependenceAttributePool = calculationAttributeData.getDependencesData();

        dependenceAttributePool.moveFirst();
        while (dependenceAttributePool.moveNext())
            tail.push_back(dependenceAttributePool.getCurrentItem());
            dbgFindLoopFrom(getCalculationAttributeData(dependenceAttributePool.getCurrentItem()), tail);
        end;

        tail.pop_back();
        calculationAttributeData.setCalculated();
    end;

    /**
     * Поиск циклов
     */
    private macro dbgFindLoopsInCalculationDataPool()
        var tail : RcbArray = RcbArray();
        var saviour : Integer;
        m_calculationDataPool.moveFirst();
        while (m_calculationDataPool.moveNext())
            tail.push_back(m_calculationDataPool.getCurrentItem().getAttribute());
            dbgFindLoopFrom(m_calculationDataPool.getCurrentItem(), tail);
        end;

        dbgClearCalculationStatus();

        loops.moveFirst();
        while (loops.moveNext())
            tail = loops.getCurrentItem(); // повторно используем переменную
            tail.moveFirst();
            print("Loop: ");
            while (tail.moveNext())
                print(string(tail.getCurrentItem():15));
            end;
            println();
        end;

    end;

    /**
     * Пометить для пересчета атрибут, зависящий от пересчитываемых после свода
     *
     * @param     calculationAttributeData
     */
    private macro dbgUpdateRecalculateKind(calculationAttributeData : T134CalculationAttributeData)
        if ((calculationAttributeData == null) or calculationAttributeData.isAfterSummarizeRecalculate())
            return true;
        end;

        var dependenceAttributePool = calculationAttributeData.getDependencesData();
        var needRecalculationAfterSummarize = false;

        dependenceAttributePool.moveFirst();
        while (dependenceAttributePool.moveNext())
            if (dbgUpdateRecalculateKind(getCalculationAttributeData(dependenceAttributePool.getCurrentItem())))
                needRecalculationAfterSummarize = true;
            end;
        end;

        if (needRecalculationAfterSummarize)
            calculationAttributeData.setAfterSummarizeRecalculate();
        end;

        return calculationAttributeData.isAfterSummarizeRecalculate();
    end;

    /**
     * Переработка инициализированных данных о пересчете
     */
    private macro dbgUpdateCalculationDataPool()
        var saviour : Integer;
        m_calculationDataPool.moveFirst();
        while (m_calculationDataPool.moveNext())
            dbgUpdateRecalculateKind(m_calculationDataPool.getCurrentItem());
        end;
    end;

    /**
     * Рассчитать один атрибут
     *
     * @param     calculationAttributeData
     */
    private macro calculateAttribute(calculationAttributeData : T134CalculationAttributeData)
        if ((calculationAttributeData == null) or (calculationAttributeData.isCalculated() and
            /* если мы не в режиме СВОД, или переменные являются перераcсчитываемые после СВОДА */
            ((not m_report.context.isSummaryMode) OR calculationAttributeData.isAfterSummarizeRecalculate()))
           )
            return;
        end;

        var dependenceAttributePool = calculationAttributeData.getDependencesData();

        dependenceAttributePool.moveFirst();
        while (dependenceAttributePool.moveNext())
            calculateAttribute(getCalculationAttributeData(dependenceAttributePool.getCurrentItem()));
        end;

        calculationAttributeData.getCalculationData().calculate();

        calculationAttributeData.setCalculated();
    end;

    /**
     * Рассчитать все атрибуты отчетной формы
     */
    private macro calculateCalculationDataPool()
        m_calculationDataPool.moveFirst();
        while (m_calculationDataPool.moveNext())
            calculateAttribute(m_calculationDataPool.getCurrentItem());
        end;
    end;

    /**
     * Операция расчета отчета.
     */
    macro calculate(period : RcbPeriod)
        initializeCalculationDataPool();
        //dbgUpdateCalculationDataPool();      //debug
        //dbgFindLoopsInCalculationDataPool(); //debug
        calculateCalculationDataPool();
    end;

    initTCalculator2133_2150(protocol);
end;

