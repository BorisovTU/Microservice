            + "\n" +              "           CASE"
            + "\n" +              "               WHEN t_operationDate <> TO_DATE('01-01-0001', 'DD-MM-YYYY')"
            + "\n" +              "                   THEN t_daysToEnd + t_operationDate"
            + "\n" +              "               WHEN t_daysToEnd <> 0"
            + "\n" +              "                   THEN t_daysToEnd + " + getSqlDate(m_period.endDate())
            + "\n" +              "               ELSE"
