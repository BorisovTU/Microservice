    /**
     */
    private macro dbgClearCalculationStatus()
        m_calculationDataPool.moveFirst();
        while (m_calculationDataPool.moveNext())
            m_calculationDataPool.getCurrentItem().setNotCalculated();
        end;
    end;

    /**
     * Добавить набор данных для расчета одного атрибута в список расчитываемых атрибутов
     *
     * @param     calculationAttributeData
     */
    private macro addCalculationAttributeData(calculationAttributeData : T134CalculationAttributeData)
       m_calculationDataPool.push_back(calculationAttributeData);
    end;

    /**
     * Заменить или добавить набор данных для расчета одного атрибута в списке расчитываемых атрибутов
     *
     * @param     calculationAttributeData
     */
    private macro addOrReplaceCalculationAttributeData(calculationAttributeData : T134CalculationAttributeData)
        private var isAttributeFound = false;

        m_calculationDataPool.moveFirst();
        while (m_calculationDataPool.moveNext())
            if (m_calculationDataPool.getCurrentItem().getAttribute() == calculationAttributeData.getAttribute())
                m_calculationDataPool.setCurrentItem(calculationAttributeData);
                isAttributeFound = true;
                break;
            end;
        end;

        if (not isAttributeFound)
            addCalculationAttributeData(calculationAttributeData);
        end;
    end;

    /**
     * @param     attribute
     */
    private macro getCalculationAttributeData(attribute : String) : T134CalculationAttributeData
        var i = 0;
        while (i < m_calculationDataPool.size)
            if(m_calculationDataPool[i].getAttribute() == attribute)
                return m_calculationDataPool[i];
            end;
            i = i + 1;
        end;
        msgbox("Ошибка программирования: атрибут \"" + attribute + "\"\n не инициализирован в initializeCalculationDataPool().");
        return null;
    end;

