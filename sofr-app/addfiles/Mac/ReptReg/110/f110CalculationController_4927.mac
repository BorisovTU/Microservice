    macro calculateAttributeA_5_2(attribute, number)
        var data;
        var dataProtocol;
        TBoAccounts().clearAccounts();

        data = m_dataSource.dataMbkDeals();
        data.setUseBoAccounts(true);
        dataProtocol = m_protocol.getDataProtocol(attribute.getPart(), attribute.getName(), number);
        attribute.addValue(data.getRealRvpsAndRvpsDelayed(data.getFilter().hasKindDeals(DEAL_KIND_PLACEMENT)
                                                        + data.getFilter().isOpenedAtEndDate(),
                                                          m_tuneTable.getPrimaryAccountMasks(attribute.getName(), BOIBC, 1),
                                                          dataProtocol),
                           BOIBC);

        data = m_dataSource.dataLoansCredit();
        data.setUseBoAccounts(true);
        attribute.setValue(data.getRvpsSum(data.getFilter().isSatisfiesCreditAccountMasks(m_tuneTable.getPrimaryAccountMasks(attribute.getName(),BOLoans, 1))
                                         + data.getFilter().isOpened()
                                         + data.getFilter().hasNotType(LO_DEPOSIT),
                                           dataProtocol),
                           BOLoans);

        attribute.addValue(data.getBadRvpsSum(data.getFilter().isSatisfiesCreditAccountMasks(m_tuneTable.getPrimaryAccountMasks(attribute.getName() + "_1", BOLoans, 1))
                                            + data.getFilter().isOpened()
                                            + data.getFilter().hasNotType(LO_DEPOSIT),
                                              dataProtocol),
                           BOLoans);

        data = m_dataSource.dataGkboReserves();
        attribute.setValue(data.getReserveRvps(data.getFilter().isSatisfiesToAccountMasks(m_tuneTable.getPrimaryAccountMasks(attribute.getName(), BOCB, 1))
                                               +
                                               data.getFilter().isReserveAccountForAccountWithKindReserve(RESERVE_KIND_RVPS)
                                               +
                                               data.getFilter().isNotCalcBo("reserveacc.t_reserveAccount"),
                                               dataProtocol),
                           BOCB);
    end;

