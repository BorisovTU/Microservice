            +"\n"+ "                   FROM drepaccount_vw repaccount"
            +"\n"+ "                  WHERE (" + convertMaskToSqlFormat(m_tuneTable.setKey("3").getMasks(),"repaccount.t_balance") +  ")"
            +"\n"+ "                    AND repaccount.t_isopened = 1"
            +"\n"+ "                    AND EXISTS(SELECT 1"
            +"\n"+ "                                FROM ddp_dep_dbt ddp_dep"
            +"\n"+ "                               WHERE ddp_dep.t_code != repAccount.t_departmentId "
            +"\n"+ "                                 AND ddp_dep.t_partyId = TO_NUMBER(repAccount.t_contragentId) ))"
            +"\n"+ "      ORDER BY t_row, t_account, t_category, t_fromDate";
    end;

    initTBaseInputControlDataSource();
end;

private macro getSymbolCount(str : String, symbol : String)
    var count = 0;
    var idx = index(str, symbol);
    while (idx != 0)
        count = count + 1;
        idx = index(str, symbol, idx+1);
    end;
    return count;
end;

