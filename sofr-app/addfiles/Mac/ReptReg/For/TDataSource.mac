private macro makeBalanceQueryStandalone(applicationName : String, rowCode : String, balanceGroup : String, balanceField : String, restsQuery : String, condition : String, filter : Object)
    return "SELECT /*+ UNNEST*/"
    +"\n"+ "       " + getSqlString(applicationName) + " t_applicationName,"
    +"\n"+ "       " + BOCB + " t_backOffice,"
    +"\n"+ "       " + getSqlString(rowCode) + " t_rowCode,"
    +"\n"+ "       " + balanceField + " t_balance,"
    +"\n"+ "       t_date,"
    +"\n"+ "       SUM(NVL(t_rest, 0)) t_rest, "
    +"\n"+ "       ROUND(SUM(NVL(t_rest, 0)) / " + RcbApplication().currentReport.multiplier + ") t_scaledRest,"
    +"\n"+ "       NVL(t_isCurrency, CHR(0)) t_isCurrency, "
    +"\n"+ "       " + getSqlString(balanceGroup) + " t_balanceGroup"
    +"\n"+ "  FROM (SELECT t_balance,"
    +"\n"+ "               t_date,"
    +"\n"+ "               t_rest*t_rate AS t_rest,"
    +"\n"+ "               t_isCurrency"
    +"\n"+ "          FROM (" + restsQuery + ") account"
    +"\n"+ "         UNION ALL"
    +"\n"+ "        SELECT t_balance,"
    +"\n"+ "               t_date,"
    +"\n"+ "               0 t_rest,"
    +"\n"+ "               cur.t_isCurrency"
    +"\n"+ "          FROM dbalance_dbt,"
    +"\n"+ "               dffordates_tmp,"
    +"\n"+ "               (SELECT 'X' t_isCurrency FROM DUAL"
    +"\n"+ "                 UNION ALL"
    +"\n"+ "                SELECT CHR(0) t_isCurrency FROM DUAL"
    +"\n"+ "               ) cur"
    +"\n"+ "         WHERE " + condition
    +"\n"+ "           AND t_chapter = 1"
    +"\n"+ "           AND " + filter.isSatisfiesToBalancesMasks("t_balance")
    +"\n"+ "       ) account"
    +"\n"+ " GROUP BY t_balance,"
    +"\n"+ "          t_date,"
    +"\n"+ "          t_isCurrency"
end;

