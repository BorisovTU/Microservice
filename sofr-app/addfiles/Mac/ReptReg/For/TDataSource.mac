    private macro getQerry()

        return     "SELECT * FROM ("
            +"\n"+ "    SELECT accounts.t_account AS t_account,"
            +"\n"+ "           CASE "
            +"\n"+ "                WHEN accounts.t_maturityDate < accounts.t_openDate + INTERVAL '3' YEAR"
