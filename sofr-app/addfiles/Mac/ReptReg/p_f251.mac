MACRO Pr( ИмяПеременной )
var val;
RECORD buf (cy_varsd, "cy_files.def" );

  СвойстваПеременной ( {Название отчета}, ИмяПеременной, buf ); 

  if ( ИмяПеременной == "" )
    return "";
  end;

  if ( not ПрочитатьПеременную( val,
                                ДатаОтчета, ПредДатаОтчета,
                                {Название отчета}, ИмяПеременной)
     )
    if( MeanUndefByZero != 0 ) /* Если установлен флаг "При печати и сведении полагать
                                неопределенные значения равными 0"*/
      return "0";
    else
      MsgBox( "Не задано значение переменной \"", ИмяПеременной, "\" за период с ",
            ПредДатаОтчета, " по ", ДатаОтчета, "|", buf.szcomment );
      Exit(1);
    end;
  end;

  if ( index(ИмяПеременной , "К" ) > 0 ) 
       return string( val:6:0:r );
  else
       return string( val:15:0:r );
  end;

END;


