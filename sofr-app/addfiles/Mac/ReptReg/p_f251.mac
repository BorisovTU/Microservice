VAR
  err,
  NPage,
  Nbl,
  Port,
  ZagLen,
  Page,
  Nom;

GetRegistryValue( "REPTREG\\REP_GROUPS\\ФОРМА 251\\ШИРИНА ОТЧЕТА",
            V_INTEGER, RealWide, err );
if(err != 0)
  msgbox("Ошибка при определении настройки: |REPTREG\\REP_GROUPS\\ФОРМА 251\\ШИРИНА ОТЧЕТА");
  RealWide = 310;
end;


/*  i = 1;
  While( i < 6 )
    NBlPage( i ) = 0;
    i = i + 1;
  end;*/

  if ( RealWide < MINWIDE ) 
    if ( not getTrue( TRUE, "Ширина отчета не может быть меньше "+string(MINWIDE)+".| Установить ширину "+string(MINWIDE)+"?" ) )
      exit(1);
    end;
    RealWide = MinWide;
  end;

  if ( RealWide >= MaxWide )
    NPage = 1; NBlPage( 1 ) = 5;
  else 
    NPage = 0; NBl = 5;
    while (  Nbl > 0 )
       NPage = NPage + 1;
       if ( NPage == 1 ) Port = WideNs + WideNp;
       else              Port = WideNs;
       end;
       NBlPage( NPage ) = min( Floor ( (RealWide - Port)/ WideBlock ), NBl ) ;
       NBl = NBl - NBlPage( NPage ); 
    end;
  end;

  ZagLen = WideNs + WideNp + NBlPage( 1 )*WideBlock;  

  PrintHeader( ZagLen );

  NBlPage( 1 ) = NBlPage( 1 ) - 1;

  page = 1; Nom = 1;
  while ( page < NPage + 1 )

     if ( page == 1 )
          PrintStI( TRUE,  Nom, NBlPage( page) );
     else 
          Nom = Nom + NBlPage( page-1 );
          PrintStI( FALSE, Nom, NBlPage( page) );
     end;
     page = page + 1;
     println ("\f"); /* переход к новой странице  */
  end;

  Подпись();

