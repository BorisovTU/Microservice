/**
 *  Базовое печатное представление подразделов раздела 2
 *  @author ABP
 *  @since 6.00.020.29
 */
private class (TPartView) TSection2PartView()

    private var m_part : TArray;

    macro getPart(partNumber)
        return m_part[partNumber - 1];
    end;

    macro printFooter()
    end;

    macro getWidth()

        var i;

        var width = 0;

        i = 0;
        while (i < m_part.size)
            width = max(width, m_part[i].getWidth());
            i = i + 1;
        end;

        return width;

    end;

    /**
     *  Конструктор
     */
    initTPartView();
    local var value;
    local var i;
    m_part = TArray();
    for (i, 1, parmCount())
        getParm(i, value);
        m_part[i-1] = value;
    end;

end;

