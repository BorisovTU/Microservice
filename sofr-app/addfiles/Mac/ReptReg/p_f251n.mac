    private macro printValue(p_name : String) : String
      var value = RcbApplication.currentReport.attributeValue(p_name);

      if (value != null)
        return ternary(value.isDefined(), value.currentAsString, "0");
      end;

      return "0";
    end;

    /* 24.08.2007 Malakhova  */
    /*Функция определяет исполнителя в соответствии с ТЗ_Требования к печатным формам.doc:*/
    /*ФИО исполнителя из панели задания характеристик формы
    По умолчанию - имя пользователя, выпускающего отчет
    */
    private macro getExecutorName()
        var executorName;

        if (Исполнитель != "")
            executorName = Исполнитель;
        else
            executorName = ИмяОперациониста();
        end;

        return executorName;
    end;

    private macro printHead();
        var m_view;
        m_view = RcbReportView("СВЕДЕНИЯ О СЧЕТАХ КЛИЕНТОВ И ПЛАТЕЖАХ, ПРОВЕДЕННЫХ ЧЕРЕЗ КРЕДИТНУЮ ОРГАНИЗАЦИЮ (ЕЁ ФИЛИАЛ)",
                                "0409251", "Квартальная", DATE_IN_PERIOD_FORMAT);
        m_view.setReportWidth(135);

        m_view.printLendingAgencyCodeZone();
        m_view.getNamesZone().excludeAttribute(AC_FORM_UNIT);

        m_view.printNamesZone();
    end;

