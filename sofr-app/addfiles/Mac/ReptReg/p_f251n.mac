class (TPrintController_4212) TPrintController_4637()
    initTPrintController_4212();

    private var m_departamentListSum = RcbArray();  //список подразделений, вошедших в сводный отчет
    private var m_departamentList    = RcbArray();  //список подразделений, не вошедших в сводный отчет
    private var m_report;                           //текущий отчет для печати
    private var m_isSummaryMode;                    //печатаем сводный отчет?

    private macro printValue(p_name : String) : String
        var value = m_report.attributeValue(p_name);

        if (value != null)
          return ternary(value.isDefined(), value.currentAsString, "0");
        end;

        return "0";
    end;

    private macro printHead();
        var m_view;
        m_view = RcbReportView("СВЕДЕНИЯ О СЧЕТАХ КЛИЕНТОВ И ПЛАТЕЖАХ, ПРОВЕДЕННЫХ ЧЕРЕЗ КРЕДИТНУЮ ОРГАНИЗАЦИЮ (ЕЁ ФИЛИАЛ)",
                               "0409251", "Квартальная", DATE_IN_PERIOD_FORMAT);

        m_view.setReportWidth(135);
        m_view.printLendingAgencyCodeZone();
        m_view.getNamesZone().excludeAttribute(AC_FORM_UNIT);
        m_view.printNamesZone("Полное или сокращенное фирменное наименование кредитной организации ", "Адрес (место нахождения) кредитной организации ");
    end;

    macro printReport()
        m_report = RcbApplication().currentReport;

        printHead();

        if (m_isSummaryMode)
            if (isEmptyReport())
                printNoData();
            else
                m_departamentListSum.moveFirst();
                while (m_departamentListSum.moveNext())
                    if (m_departamentListSum.getCurrentItem().m_isMainOffice == 1)
                        println("Регистрационный номер КО\t\t" + m_departamentListSum.getCurrentItem().m_regNum);
                    else
                        println("Регистрационный номер филиала\t" + m_departamentListSum.getCurrentItem().m_regNum);
                    end;
                end;
                println();
                printBody();

                m_departamentList.moveFirst();
                while (m_departamentList.moveNext())
                    if (    (m_departamentList.getCurrentItem().m_report != NULL)
                        and (m_departamentList.getCurrentItem().m_report.isCalculated))

                        m_report = m_departamentList.getCurrentItem().m_report;
                        println();
                        println("Регистрационный номер филиала\t" + m_departamentList.getCurrentItem().m_regNum);
                        println();
                        printBody();
                    end;
                end;
            end;

            printBottom();
        else
            if (isEmptyReport())
                printNoData();
            else
                printBody();
            end;

            printBottom();
        end;
    end;

    private macro constructor()
        var doop = RcbApplication().currentReport.context.period.endDate;
        var context;
        var report;
        var dataSet;
        var organizationStructure = RcbApplication().currentReport.context.organizationStructure;
        var departmentSource;

        if (   (organizationStructure == null)
            or (organizationStructure == TERRITORIAL))
            departmentSource = "drepdpdep_vw";
        else
            departmentSource = "drepdpreg_vw";
        end;

        m_isSummaryMode = RcbApplication().currentReport.context.isSummaryMode;

        if (m_isSummaryMode == true)
            var query = "WITH                                                                                                                                                                                "
               + "\n" + "departments as                                                                                                                                                                      "
               + "\n" + "(                                                                                                                                                                                   "
               + "\n" + "SELECT deps.t_code                                                                                                                                                  t_code,         "
               + "\n" + "       deps.t_partyId                                                                                                                                               t_partyId,      "
               + "\n" + "       rcb_objAttr.makePartyId(deps.t_partyId)                                                                                                                      t_objId,        "
               + "\n" + "       NVL(rcb_objAttr.getObjAttr(" + OBJTYPE_PARTY + ", " + RCB_OBJGROUP_PT_OKATO + ", rcb_objAttr.makePartyId(deps.t_partyId), " + getSqlDate(doop) + "), CHR(1)) t_okatoCategory,"
               + "\n" + "       NVL(address.t_okato, CHR(1))                                                                                                                                 t_okatoAddress  "
               + "\n" + "  FROM " + departmentSource + " deps,                                                                                                                                               "
               + "\n" + "       dAdress_dbt address                                                                                                                                                          "
               + "\n" + " WHERE address.t_partyId = deps.t_partyId                                                                                                                                           "
               + "\n" + "   AND address.t_type    = " + RCB_PTADDR_LEGAL
               + "\n" + "),                                                                                                                                                                                  "
               + "\n" + "departmentCodes AS                                                                                                                                                                  "
               + "\n" + "(                                                                                                                                                                                   "
               + "\n" + "SELECT deps.t_code                                                                                   t_code,                                                                        "
               + "\n" + "       deps.t_partyId                                                                                t_partyId,                                                                     "
               + "\n" + "       deps.t_objId                                                                                  t_objId,                                                                       "
               + "\n" + "       CASE                                                                                                                                                                         "
               + "\n" + "           WHEN deps.t_okatoCategory != CHR(1)                                                                                                                                      "
               + "\n" + "               THEN RPAD(SUBSTR(deps.t_okatoCategory, 1, 2), 5, '0')                                                                                                                "
               + "\n" + "           WHEN deps.t_okatoAddress != CHR(1)                                                                                                                                       "
               + "\n" + "               THEN RPAD(SUBSTR(deps.t_okatoAddress, 1, 2), 5, '0')                                                                                                                 "
               + "\n" + "           ELSE '99999'                                                                                                                                                             "
               + "\n" + "       END                                                                                           t_okato,                                                                       "
               + "\n" + "       rsb_rep_pt.get_partyCode(" + RCB_PTCK_BANKREGNUM + ", deps.t_objId, " + getSqlDate(doop) + ") t_regNum                                                                       "
               + "\n" + "  FROM departments deps                                                                                                                                                             "
               + "\n" + ")                                                                                                                                                                                   "
               + "\n" + " SELECT deps.t_nodeId,                                                                                                                                                              "
               + "\n" + "        deps.t_parentNodeId,                                                                                                                                                        "
               + "\n" + "        deps.t_nodeType,                                                                                                                                                            "
               + "\n" + "        deps.t_code,                                                                                                                                                                "
               + "\n" + "        deps.t_codeName,                                                                                                                                                            "
               + "\n" + "        deps.t_partyId,                                                                                                                                                             "
               + "\n" + "        deps.t_name,                                                                                                                                                                "
               + "\n" + "        deps.t_shortName,                                                                                                                                                           "
               + "\n" + "        deps.t_status,                                                                                                                                                              "
               + "\n" + "        deps.t_branchType,                                                                                                                                                          "
               + "\n" + "        codes.t_okato,                                                                                                                                                              "
               + "\n" + "        codes.t_regNum,                                                                                                                                                             "
               + "\n" + "        CASE                                                                                                                                                                        "
               + "\n" + "            WHEN codes.t_okato = (SELECT c.t_okato                                                --ОКАТО как у выпускающего подразделения                                          "
               + "\n" + "                                    FROM departmentCodes c                                                                                                                          "
               + "\n" + "                                   WHERE c.t_code = " + RcbApplication().parameters.departmentCode + ")"
               + "\n" + "                THEN 1                                                                                                                                                              "
               + "\n" + "            ELSE 0                                                                                                                                                                  "
               + "\n" + "        END t_inSummaryReport,                                                                    --подразделение вошло в сводный отчет                                             "
               + "\n" + "        CASE                                                                                                                                                                        "
               + "\n" + "            WHEN (SELECT count(t.t_code) FROM " + departmentSource + " t WHERE t.t_code = deps.t_parentNodeId) = 0"
               + "\n" + "                THEN 1                                                                                                                                                              "
               + "\n" + "            ELSE 0                                                                                                                                                                  "
               + "\n" + "        END t_isMainOffice                                                                        --подразделение ТС являюется ГО                                                   "
               + "\n" + "   FROM dRepDpDep_vw    deps,                                                                                                                                                       "
               + "\n" + "        departmentCodes codes                                                                                                                                                       "
               + "\n" + "  WHERE codes.t_code    = deps.t_nodeId                                                                                                                                             "
               + "\n" + "    AND deps.t_status   = 2                                                                       --только активные подразделения                                                   "
               + "\n" + "    AND deps.t_nodeType = 1                                                                       --только филиалы                                                                  "
               + "\n" + "    AND level < 3                                                                                 --не глубже 2-го уровня                                                           "
               + "\n" + "  START WITH deps.t_code = " + RcbApplication().parameters.departmentCode
               + "\n" + "CONNECT BY PRIOR deps.t_code = deps.t_parentNodeId                                                                                                                                  "
               + "\n" + "  ORDER BY deps.t_nodeId                                                                                                                                                            "
               ;

            dataSet = TRsbDataSet(query);
            while (dataSet.moveNext())
                context = RcbReportContext(RcbApplication().currentReport.context.period, dataSet.nodeId, BANK, organizationStructure, false);
                report  = RcbApplication().objectFactory.createReport(RcbApplication().currentReport.form.id, context);

                if (dataSet.inSummaryReport == 1)
                    m_departamentListSum.push_back(CReportsByOkato(dataSet.nodeId, dataSet.regNum, dataSet.okato, dataSet.name, dataSet.isMainOffice, NULL));
                else
                    m_departamentList.push_back(CReportsByOkato(dataSet.nodeId, dataSet.regNum, dataSet.okato, dataSet.name, 0, report));
                end;
            end;
        end;
    end;

    constructor();
end;

