class (RcbReportPrintController) TF123ReportPrintController
    initRcbReportPrintController();

    m_reportWidth = 120;

    private macro printString(rowNumber : String, description : String, calculated : Bool)
        var valueAsString : String = "";

        if (calculated == false)
            valueAsString = "X";
        else
            valueAsString = m_report.getPart("").getAttribute(rowNumber).getValueAsString();
        end;

        getReportView().getPartView("").printString(RcbArray().initialize(nvl(rowNumber, ""), description, valueAsString));
    end;

