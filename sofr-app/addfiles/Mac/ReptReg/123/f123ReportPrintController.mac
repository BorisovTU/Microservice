 */
class (TF123ReportPrintController) TF123ReportPrintController_XXXX
    initTF123ReportPrintController();

    /**
     * Заголовок дополнительных строк отчета
     */
    const STRING_REFERENCE_HEAD = "Справочно:";
    /**
     * Суффик дополнительных строк
     */
    const STRING_REFERENCE_SUFFIX = "тыс. руб.";

    /**
     * Печать заголовока раздела "Справочно"
     *
     * @return Печать заголовка
     */
    private macro printReferenceHeadString()
        printLn(STRING_REFERENCE_HEAD);
    end;

    /**
     * Печать строк "Справочно"
     *
     * @param rowNumber Номер строки
     * @param description Описание строки
     *
     * @return Печать строки
     */
    private macro printReferenceString(rowNumberForPrint: String, rowNumberForGetValue: String, description: String)
        /**
         * Строковое значение атрибута
         */
        var valueAsString: String = "0";

        valueAsString = m_report.getPart("").getAttribute(rowNumberForGetValue).getValueAsString();

        /**
         * Печать суффикса строки
         */
        var valueSuffix: String = STRING_REFERENCE_SUFFIX;

        printLn(rowNumberForPrint, ". ", description, " ", valueAsString, " ", valueSuffix);
    end;

