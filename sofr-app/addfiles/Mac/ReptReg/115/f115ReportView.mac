/**
 * Класс представления отчетной формы.
 */
private class TPartView(name : String,reportView : RcbReportView)
    /**
     * Название раздела.
     */
    private var m_name;

    /**
     * Ссылка не представление отчетной формы.
     */
    private var m_reportView;

    /**
     * Конструктор.
     */
    private macro constructor(name : String,reportView : RcbReportView)
        m_name       = name;
        m_reportView = reportView;
    end;

    /**
     * Заголовок раздела.
     */
    macro printPartName()
        println();
        println(m_name);
    end;

    /**
     * Разделитель.
     */
    macro printSeparator()
    end;

    /**
     * Строка отчета.
     */
    macro printString()
    end;

    /**
     * Закрывашка.
     */
    macro printBottom()
        println();
    end;

    /**
     * Ширина раздела.
     */
    macro getWidth()
        return 80;
    end;

    constructor(name,reportView);
end;

/**
 * Класс представления таблицы отчетной формы.
 */
private class (TPartView) TPartTableView(name : String,reportView : RcbReportView)
    /**
     * Таблица.
     */
    private var m_table = CTableReport();

    /**
     * Описатель строк.
     */
    private var m_stringDescriptor : TArray;

    /**
     * Конструктор.
     */
    private macro constructor(name : String,reportView : RcbReportView)
        initTPartView(name,reportView);
        m_table = CTableReport();
        m_stringDescriptor = TArray();
    end;

    /**
     * Заголовок раздела.
     */
    macro printHeader()
        m_table.printHead();
    end;

    /**
     * Заголовок раздела без валюты.
     */
    macro printHeaderWithoutUnitName()
        m_table.printHead();
        m_table.setAllCenter(true);
        m_table.printString("1", "2", "3", "4");
        m_table.setAllCenter(false);
    end;

    /**
     * Разделитель.
     */
    macro printSeparator()
        m_table.printSeparator();
    end;

    /**
     * Строка отчета.
     */
    macro printString()
    end;

    /**
     * Закрывашка.
     */
    macro printBottom()
        m_table.printBottom();
        println();
    end;

    /**
     * Ширина раздела.
     */
    macro getWidth()
        return  m_table.getSumLen();
    end;

    /**
     * Добавить описание строки.
     */
    private macro addStringDescriptor(name, descriptor)
        m_stringDescriptor[m_stringDescriptor.size] = ArrCreate(name, descriptor);
    end;

    /**
     * Получить описание строки.
     */
    private macro getStringDescriptor(name)
        var i = 0;
        while (i < m_stringDescriptor.size)
            if (m_stringDescriptor[i][0] == name)
                return m_stringDescriptor[i][1];
            end;
            i = i + 1;
        end;
    end;

    constructor(name, reportView);
end;

