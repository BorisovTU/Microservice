/**
 * Класс представления отчетной формы для раздела 2,3.
 */
private class (TPartTableView) TPart23View2061(name : String, reportView : RcbReportView)

    private macro createColumns()
        m_table.addColumn("Номер|п/п",                                   6, AL_CENTER);
        m_table.addColumn("Наименование статьи",                        40, AL_LEFT);
        m_table.addColumn("Сумма требований",                           20, AL_RIGHT);
        m_table.addColumn("Сформированный резерв|на возможные потери",  20, AL_RIGHT);
    end;

    private macro constructor(name : String, reportView : RcbReportView)
        initTPartTableView(name, reportView);
        createColumns();
    end;

    macro printString(number, value3, value4)
        var i = 2;/*т.к. первые два параметра обязательны*/
        var temp;

        while (GetParm(i, temp))
             if ((temp == stringUndefined) or (temp == null) or (temp == ""))
                ExecExp("value"+(i+1)+"= \"X\"");
             end;
             i = i + 1;
        end;

        m_table.printStringTransferByWord(number, getStringDescriptor(number), value3, value4);
    end;

    macro printHeader()
        m_reportView.printZone(AC_FORM_UNIT);
        m_table.printHead();
        m_table.setAllCenter(true);
        m_table.printString("1", "2", "3", "4");
        m_table.setAllCenter(false);
    end;

    constructor(name, reportView);
end;

