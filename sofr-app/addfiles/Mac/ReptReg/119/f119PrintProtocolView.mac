/*
$Name:          f119PrintProtocolView.mac
$Module:        Регламентируемая отчетность
$Description:   Форма 119. Печатное представление протокола печати.
*/

/**
 * Печатное представление протокола печати
 *
 * @param protocolName String Наименование протокола печати
 */
class (RcbPrintProtocolView) TF119PrintProtocolView(protocolName : String)

    /**
     * Печать строки
     *
     * @param stringValue String Строковое значение для печати
     */
    macro printString(stringValue: String)

        if (stringValue == NULL)
            throw(TUnspecifiedValueException("TF119PrintProtocolView::printString", "stringValue"));
        end;

        printLn(stringValue);
    end;

    /**
     * Предварительные действия
     */
    private macro initialize()
    end;

    /**
     * Конструктор
     *
     * @param protocolName String Наименование протокола печати
     */
    private macro constructorTF119PrintProtocolView(protocolName: String)

        if (protocolName == NULL)
            // throw(TUnspecifiedValueException("TF119PrintProtocolView::constructorTF119PrintProtocolView", "protocolName"));
            // в данной реализации ситуация корректная, исключение не выбрасываем
        end;

        initRcbPrintProtocolView(null, protocolName);
    end;

    constructorTF119PrintProtocolView(protocolName);
end;
