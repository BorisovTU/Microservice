import RcbCoreINter;
import rcbimport;class FBalanceChangePrecision()        private var m_precision;    private var m_balanceReport;            class TFilter()        macro isSuitable(v : Object)            return (     in(subStr(v.attribute.name,3,5), "98000",                                                          "98010",                                                                               "98015",                                                          "98020",                                                          "98030",                                                          "98035",                                                          "98040",                                                          "98050",                                                          "98053",                                                          "98055",                                                          "98060",                                                          "98065",                                                          "98070",                                                          "98080",                                                          "98090")                      or                          in(subStr(v.attribute.name,4,3), "ÑàÄ",                                                          "Ñàè"));        end;    end;    private macro initialize()        m_balanceReport = rcbApplication.currentReport;                getRegistryValue("REPTREG/REP_GROUPS/BALANCE_ACCOUNTS/íéóçéëíú Ñãü ÉãÄÇõ Ñ", V_INTEGER,  m_precision, null);        
    end;            private macro getBalanceAttribute(name)        return m_balanceReport.attributeValue(name);    end;    macro change()        var iterator;         iterator = m_balanceReport.createAttributeValueIterator();        iterator.setFilter(TFilter());        var isExist = false;        iterator.moveFirst();        while (not iterator.isDone())            if (iterator.currentItem.attribute.precision == m_precision)                return;            end;            isExist = true;            iterator.currentItem.attribute.precision = m_precision;                        iterator.moveNext();        end;            if (isExist)            RcbApplication.TransactionManager.commit();            RcbApplication.currentReport.reload();        end;    end;
    initialize();end;FBalanceChangePrecision().change();„·‚†≠Æ¢®‚Ïî´†£ÇÆß¢‡†‚†(OK_MACRO_FLAG);exit(1);