/*
   Получить связанный c валютой счет
*/

import CTInter;

macro FindFILinkedAccount(kind,FIID,Acc);

   var stat = GetLinkedObject(
                            kind,
                            2,
                            strsubst(string(FIID:10)," ","0"),
                            4,
                            Acc
                         );
   if ( stat == 0 )
      setparm(2,substr(Acc,10));
   end;

   return stat;
end;
