/*
   Отчет о массовой обработке созданных по исполнению платежей
*/

import u_rmtls;

macro Печать_ОтчетСканирования (Вызов,firstdoc,Статус,Сообщение)
   PaymReportMassPmLink (
                 Вызов,
                 firstdoc,
                 Статус,
                 Сообщение,
                 "Отчет о массовой обработке платежей",
                 "Обработка выполнена успешно",
                 ПечататьСообщениеОбУспешномВыполнении
              );
end;
