private class RG_ReutersImportBO(_SeanceID, _RecordID)
  private var
      prm_SeanceID,
      prm_RecordID;

  private var
      prm_ObjID,
      prm_Comment;

  private var
      RG;

  private var
      prm_tick,
      prm_legBase,
      prm_legRev;

  private var
      prm_deferOp;

  private macro CreateTick(_BofficeKind)
    var
        type     = 0,
        DateP1C1 = date(0, 0, 0),
        DateP1C2 = date(0, 0, 0),
        DateP2C1 = date(0, 0, 0),
        DateP2C2 = date(0, 0, 0);

      prm_tick.rec.DealID      = 0;
      prm_tick.rec.BofficeKind = _BofficeKind;

      //Статические параметры   
      prm_tick.rec.TradeSystem = PTCK_REUTER;
      prm_tick.rec.DealStatus  = DL_PREPARING;
      prm_tick.rec.RegDate     = {curdate};
      prm_tick.rec.Oper        = {oper};   
      prm_tick.rec.ClientID    = ALLFININSTR;
      prm_tick.rec.DepositID   = ALLFININSTR;
      prm_tick.rec.MarketID    = ALLFININSTR;
      prm_tick.rec.PortfolioID = 0;
      prm_tick.rec.IndocID     = 0;
      prm_tick.rec.NumberPack  = 0;
      prm_tick.rec.Department  = {OperDprt};
      prm_tick.rec.OriginID    = GT_REUTERS;
      prm_tick.rec.Netting     = 3;
      prm_tick.rec.LinkChannel = 1;

      //Номер сделки в торговой системе   
      if((GetParmByName(RG, "RGREU_501", @prm_tick.rec.DealCodeTS, @type) == NULL)or(type != V_STRING))
          RunError("Не найден параметр RGREU_501 объекта " + String(prm_RecordID));
      end;

      //Дата заключения сделки   
      if((GetParmByName(RG, "RGREU_502", @prm_tick.rec.DealDate, @type) == NULL)or(type != V_DATE))
          RunError("Не найден параметр RGREU_502 объекта " + String(prm_RecordID));
      end;

      //Время заключения сделки   
      if((GetParmByName(RG, "RGREU_503", @prm_tick.rec.DealTime, @type) == NULL)or(type != V_TIME))
          RunError("Не найден параметр RGREU_503 объекта " + String(prm_RecordID));
      end;

      //Диллер   
      if((GetParmByName(RG, "RGREU_504", @prm_tick.rec.TraderID, @type) == NULL)or(type != V_INTEGER))
          RunError("Не найден параметр RGREU_504 объекта " + String(prm_RecordID));
      end;

      //Идентификатор контрагента   
      if((GetParmByName(RG, "RGREU_508", @prm_tick.rec.PartyID, @type) == NULL)or(type != V_INTEGER))
          RunError("Не найден параметр RGREU_508 объекта " + String(prm_RecordID));
      end;

      if (prm_tick.rec.PartyID == -1)
          RunError( "Ошибка при создании новой сделки: не задан контрагент." );
      end;

      //Брокер   
      if((GetParmByName(RG, "RGREU_510", @prm_tick.rec.BrokerID, @type) == NULL)or(type != V_INTEGER))
          RunError("Не найден параметр RGREU_510 объекта " + String(prm_RecordID));
      end;

      //Системный тип документа   
      if((GetParmByName(RG, "RGREU_514", @prm_tick.rec.TypeDoc, @type) == NULL)or(type != V_STRING))
          RunError("Не найден параметр RGREU_514 объекта " + String(prm_RecordID));
      end;

      //Комментарий
      if((GetParmByName(RG, "RGREU_553", @prm_tick.rec.Comment, @type) == NULL)or(type != V_STRING))
          RunError("Не найден параметр RGREU_553 объекта " + String(prm_RecordID));
      end;

      //тип сделки
      if((GetParmByName(RG, "RGREU_DealType", @prm_tick.rec.DealType, @type) == NULL)or(type != V_INTEGER))
          RunError("Не найден параметр RGREU_DealType объекта " + String(prm_RecordID));
      end;

      //Группа сделки
      prm_tick.rec.DealGroup = GetOperationGroup (prm_tick.rec.DealType);

      if (prm_tick.rec.BofficeKind == DL_FOREXDOC)
          //генерим номер сделки
          if (not DL_GenRefByTypeDoc(OBJTYPE_CONVDEAL, prm_tick.rec.DealCode))
              RunError("Ошибка при генерации номера сделки");
          end;

          //Признак обратной котировки   
          if((GetParmByName(RG, "RGREU_524", @prm_tick.rec.RevRate, @type) == NULL)or(type != V_STRING))
              RunError("Не найден параметр RGREU_524 объекта " + String(prm_RecordID));
          end;

          if((GetParmByName(RG, "RGREU_525", @DateP1C1, @type) == NULL)or(type != V_DATE))
              RunError("Не найден параметр RGREU_525 объекта " + String(prm_RecordID));
          end;

          if((GetParmByName(RG, "RGREU_526", @DateP1C2, @type) == NULL)or(type != V_DATE))
              RunError("Не найден параметр RGREU_526 объекта " + String(prm_RecordID));
          end;

          if (DateP1C1 > DateP1C2)
              prm_tick.rec.Flag1 = "X";
          else
              prm_tick.rec.Flag1 = "";
          end;

          if (IsSwap(prm_tick.rec.DealGroup))
              if((GetParmByName(RG, "RGREU_527", @DateP2C1, @type) == NULL)or(type != V_DATE))
                  RunError("Не найден параметр RGREU_527 объекта " + String(prm_RecordID));
              end;

              if((GetParmByName(RG, "RGREU_528", @DateP2C2, @type) == NULL)or(type != V_DATE))
                  RunError("Не найден параметр RGREU_528 объекта " + String(prm_RecordID));
              end;

              if (DateP2C1 > DateP2C2)
                  prm_tick.rec.Flag2 = "X";
              else
                  prm_tick.rec.Flag2 = "";
              end;
          end;
      elif (prm_tick.rec.BofficeKind == DL_IBCDOC)
          //генерим номер сделки
          if (not DL_GenRefByTypeDoc(OBJTYPE_MMARKDEAL, prm_tick.rec.DealCode))
              RunError("Ошибка при генерации номера сделки");
          end;
      end;

      if (not prm_tick.insert)
          RunError( "Ошибка при создании новой сделки." );
      end;
  end;

  private macro GetDaysInYear(basis, year)
    if (basis == basis_360Act)
      return 360;
    elif (basis == basis_365Act)
      return 365;
    end;
    
    if( (mod(year, 4) == 0) AND ((mod(year,100) != 0) OR (mod(year,400) == 0 )))
     return 366;
    else
     return 365;
    end;     
  end;
  
  private macro CreateLeg()
    var
        DataSet = NULL,
        type     = 0,
        decimals = "",
        DateP1C1 = date(0, 0, 0),
        DateP1C2 = date(0, 0, 0),
        DateP2C1 = date(0, 0, 0),
        DateP2C2 = date(0, 0, 0),
        year = 0;

      prm_legBase.rec.DepositID    = ALLFININSTR;
      prm_legBase.rec.Registrar    = ALLFININSTR;
      prm_legBase.rec.Pitch        = 1;
      prm_legBase.rec.Mode         = 1;
      prm_legBase.rec.PeriodNumber = 10000;
      prm_legBase.rec.PeriodType   = 3;
      prm_legBase.rec.Diff         = 0;
      prm_legBase.rec.PayDay       = 0;
      prm_legBase.rec.TablePercent = "";
      prm_legBase.rec.Caption      = 0;
      prm_legBase.rec.TypeDate     = 0;
      prm_legBase.rec.CountDay     = 0;
      prm_legBase.rec.Correct      = 0;
      prm_legBase.rec.DealID       = prm_tick.rec.DealID;

      //валюта
      if((GetParmByName(RG, "RGREU_517", @prm_legBase.rec.PFI, @type) == NULL)or(type != V_INTEGER))
          RunError("Не найден параметр RGREU_517 объекта " + String(prm_RecordID));
      end;

      //сумма
      if((GetParmByName(RG, "RGREU_519", @prm_legBase.rec.Principal, @type) == NULL)or(type != V_MONEYL) )
          RunError("Не найден параметр RGREU_519 объекта " + String(prm_RecordID));
      end;

      prm_legBase.rec.LegNumber = prm_tick.rec.DealCode;	  

      if (prm_tick.rec.BofficeKind == DL_FOREXDOC)
          //статические параметры    
          prm_legBase.rec.LegID = 0;
          prm_legBase.rec.Scale = 1;
          prm_legBase.rec.Basis = 4;

          if((GetParmByName(RG, "RGFX_Points", @decimals, @type) == NULL)or(type != V_STRING)or(decimals == ""))
              prm_legBase.rec.Point = 4; 
          else
              prm_legBase.rec.Point = int(decimals);
          end;

          //валюта 2
          if((GetParmByName(RG, "RGREU_518", @prm_legBase.rec.CFI, @type) == NULL)or(type != V_INTEGER))
              RunError("Не найден параметр RGREU_518 объекта " + String(prm_RecordID));
          end;

          //курс 
          if((GetParmByName(RG, "RGREU_522", @prm_legBase.rec.Price, @type) == NULL)or(type != V_DOUBLE))
              RunError("Не найден параметр RGREU_522 объекта " + String(prm_RecordID));
          else
              prm_legBase.rec.Price = prm_legBase.rec.Price * pow(10, prm_legBase.rec.Point);
          end;

          //сумма 2 валюты
          if (prm_tick.rec.RevRate == "")
              prm_legBase.rec.Cost = prm_legBase.rec.Principal * (prm_legBase.rec.Price/pow(10, prm_legBase.rec.Point));
          else
              prm_legBase.rec.Cost = prm_legBase.rec.Principal / (prm_legBase.rec.Price/pow(10, prm_legBase.rec.Point));
          end;

          //даты заключения, начала, окончания сделки
          prm_legBase.rec.Start = prm_tick.rec.DealDate;   

          if((GetParmByName(RG, "RGREU_525", @DateP1C1, @type) == NULL)or(type != V_DATE))
              RunError("Не найден параметр RGREU_525 объекта " + String(prm_RecordID));
          end;

          if((GetParmByName(RG, "RGREU_526", @DateP1C2, @type) == NULL)or(type != V_DATE))
              RunError("Не найден параметр RGREU_526 объекта " + String(prm_RecordID));
          end;

          if (prm_tick.rec.Flag1 == "")
              prm_legBase.rec.Maturity = DateP1C1;
              prm_legBase.rec.Expiry   = DateP1C2;
          else
              prm_legBase.rec.Maturity = DateP1C2;
              prm_legBase.rec.Expiry   = DateP1C1;
          end;

          prm_legBase.rec.Duration     = prm_legBase.rec.Maturity - prm_legBase.rec.Start;

          if (not prm_legBase.insert)
              RunError( "Ошибка при создании новой сделки." );
          end;

          //А если это сделка Своп? Необходимо еще запись для обратной сделки
          if (IsSwap(prm_tick.rec.DealGroup))
              copy(prm_legRev, prm_legBase);

              prm_legRev.rec.ID = 0;

              //статические параметры    
              prm_legRev.rec.LegID = 1;
              //курс 
              if((GetParmByName(RG, "RGREU_523", @prm_legRev.rec.Price, @type) == NULL)or(type != V_DOUBLE))
                  RunError("Не найден параметр RGREU_523 объекта " + String(prm_RecordID));
              else
                  prm_legRev.rec.Price = prm_legRev.rec.Price * pow(10, prm_legRev.rec.Point);
              end;

              //сумма 2 валюты
              if (prm_tick.rec.RevRate == "")
                  prm_legRev.rec.Cost = prm_legRev.rec.Principal * (prm_legRev.rec.Price/pow(10, prm_legRev.rec.Point));
              else
                  prm_legRev.rec.Cost = prm_legRev.rec.Principal / (prm_legRev.rec.Price/pow(10, prm_legRev.rec.Point));
              end;

              //даты заключения, начала, окончания обратной сделки
              if((GetParmByName(RG, "RGREU_527", @DateP2C1, @type) == NULL)or(type != V_DATE))
                  RunError("Не найден параметр RGREU_527 объекта " + String(prm_RecordID));
              end;

              if((GetParmByName(RG, "RGREU_528", @DateP2C2, @type) == NULL)or(type != V_DATE))
                  RunError("Не найден параметр RGREU_528 объекта " + String(prm_RecordID));
              end;

              if (prm_tick.rec.Flag2 == "")
                  prm_legRev.rec.Maturity = DateP2C1;
                  prm_legRev.rec.Expiry   = DateP2C2;
              else
                  prm_legRev.rec.Maturity = DateP2C2;
                  prm_legRev.rec.Expiry   = DateP2C1;
              end;

              prm_legRev.rec.Duration = prm_legRev.rec.Maturity - prm_legRev.rec.Start;

              if (not DL_GenRefByTypeDoc(OBJTYPE_CONVDEAL, prm_legRev.rec.LegNumber))
                  RunError("Ошибка при генерации номера сделки");
              end;

              if (not prm_legRev.insert)
                  RunError( "Ошибка при создании новой сделки." );
              end;
          end;
      elif (prm_tick.rec.BofficeKind == DL_IBCDOC)
          //статические параметры    
          prm_legBase.rec.LegID = 1;
          prm_legBase.rec.Scale = 100;

          DataSet = TRsbDataSet("select * from DPRCRATE_DBT where T_NUMBER=100 and (T_ISUSER IS NULL or T_ISUSER <> 'X')");

          if (DataSet.MoveNext())
              prm_legBase.rec.Point = DataSet.Point;
          else
              prm_legBase.rec.Point = 4;
          end;

          prm_legBase.rec.CFI = prm_legBase.rec.PFI;

          //Действующая ставка  
          if((GetParmByName(RG, "RGREU_520", @prm_legBase.rec.Price, @type) == NULL)or(type != V_DOUBLE))
              RunError("Не найден параметр RGREU_520 объекта " + String(prm_RecordID));
          else
              prm_legBase.rec.Price = prm_legBase.rec.Price * pow(10, prm_legBase.rec.Point);
          end;

          // Даты валютирования
          if((GetParmByName(RG, "RGREU_525", @prm_legBase.rec.Start, @type) == NULL)or(type != V_DATE))
              RunError("Не найден параметр RGREU_525 объекта " + String(prm_RecordID));
          end;

          if((GetParmByName(RG, "RGREU_527", @prm_legBase.rec.Maturity, @type) == NULL)or(type != V_DATE))
              RunError( "Не найден параметр RGREU_527 объекта " + String(prm_RecordID) );
          end;

          //Количество дней в сделке для расчета процентов
          if((GetParmByName(RG, "RGREU_571", @prm_legBase.rec.Duration, @type) == NULL)or(type != V_DOUBLE))
              RunError("Не найден параметр RGREU_571 объекта " + String(prm_RecordID));
          end;

          //Базис расчета процентов
          if( (GetParmByName(RG, "RGREU_572", @prm_legBase.rec.Basis, @type) == NULL)or(type != V_INTEGER))
              RunError("Не найден параметр RGREU_572 объекта " + String(prm_RecordID));
          end;
          
          //Сумма процентов
          DateSplit(prm_legBase.rec.Start, null, null, year);
          prm_legBase.rec.Cost = Floor( Money( prm_legBase.rec.Principal * (prm_legBase.rec.Price/pow(10, prm_legBase.rec.Point+2)) * prm_legBase.rec.Duration / GetDaysInYear(prm_legBase.rec.Basis, year) ) );

          if (not prm_legBase.insert)
              RunError( "Ошибка при создании новой сделки." );
          end;
      end;
  end;

