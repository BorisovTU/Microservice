//вызываем транзакцию, возвращаем нужные параметры	
macro _ForexDeal_CreateNew(_RecordID:INTEGER, _SeanceID:INTEGER, _NewID:@STRING, _Comment:@STRING):INTEGER
  var 
      Obj = RG_ReutersImportBO(_SeanceID, _RecordID),
      retVal = 0;

    WarningText = "";

    retVal = Obj.CreateDeal();

    _NewID    = Obj.Get_ObjID();
    _Comment = Obj.Get_Comment(); 

    if ((_Comment == "")and(WarningText != ""))
        _Comment = WarningText;
    end;

    return retVal;
end;

//вызываем транзакцию импорта сделки МБК, возвращаем нужные параметры	
macro _MoneyMarketDeal_CreateNew(_RecordID:INTEGER, _SeanceID:INTEGER, _NewID:@STRING, _Comment:@STRING):INTEGER
  var 
      Obj = RG_ReutersImportBO(_SeanceID, _RecordID),
      retVal = 0;

    WarningText = "";

    retVal = Obj.CreateDeal();

    _NewID    = Obj.Get_ObjID();
    _Comment = Obj.Get_Comment(); 

    if ((_Comment == "")and(WarningText != ""))
        _Comment = WarningText;
    end;

    return retVal;
end;
