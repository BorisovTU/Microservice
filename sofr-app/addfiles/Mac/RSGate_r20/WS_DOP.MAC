import BankInter,funk;

macro fprintln_debug(
  outFileName : String
,str         : String
)
  FILE outFile() txt append;
 
  if( Open( outFile, GetTxtFileName( outFileName ) ) )
    Insert( outFile, (str  + "  " + string( + Time() ) ) );
    Close( outFile );
  end;
end;
 
 
