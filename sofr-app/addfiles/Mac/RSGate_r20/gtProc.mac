    InstLoadModule("gtImpPaym.mac");
    status = ExecMacro2("_SecDeal_Paym_CreateNew", RecordID, SeanceID, @NewID, @Comment, 1);
    if (status == 0) 
        return ExecMacro2("_SecDeal_Paym_CreateNew", RecordID, SeanceID, @NewID, @Comment, 2);
    end;
    return status;
