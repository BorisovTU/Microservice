macro PartyObj_GetParams(ID:STRING, Buff:@VARIANT, Comment:@STRING):INTEGER
   record Party(party) KEY 0;

   Party.PartyID = int(ID);

   if( GetEQ(Party) )
     Buff = Party;
   end;

   return 0;
end;

macro Account_GetParams(ID:STRING, Buff:@VARIANT, Comment:@STRING):INTEGER
   record f_account ("account.dbt" ) KEY 0;

   var fileAccount;

   var Chapter = 0;
   var FIID = 0;
   var Account = "";

   Chapter = int(substr(ID, 1, 2));
   FIID    = int(substr(ID, 3, 7));
   Account = substr(ID, 10);

   fileAccount = f_account;

   fileAccount.Chapter       = Chapter;
   fileAccount.Code_Currency = FIID;
   fileAccount.Account       = Account;

   if( GetEQ(fileAccount) )
     Buff = fileAccount;
   end;

   return 0;
end;




