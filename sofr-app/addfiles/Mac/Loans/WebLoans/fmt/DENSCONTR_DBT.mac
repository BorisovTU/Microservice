    VAR IDInPantry             : integer = 0;          // Ссылка на запись в desk_aux.dbt
    VAR EnsContractStatusPantry: integer = 0;          // Статус обеспечения в кладовой
    VAR Discount               : double  = 0.0;        // Дисконт
    VAR Deposit                : string  = "";         // Заклад
    VAR QualityID_Ref          : integer = 0;          // Категория качества
    VAR Flag_RVPS              : string  = "";         // Флаг учета для РВПС
    VAR EnsCategory            : integer = 0;          // Категория обеспечения (основное\дополнителное)
    VAR FNCash                 : integer = 0;          // Ссылка на филиал
    VAR CreditTypeID           : integer = 0;          // Ссылка на вид кредита (ВК)
    VAR IsGuarantee            : string  = "";         // Признак банковской гарантии ДО
    VAR Guarantor              : integer = 0;          // Гарант
    VAR NotInclude             : string  = "";         // Не учитывать по поручителю
END;