CLASS DENSCONTR_DBT()
    VAR EnsContractID          : integer = 0;          // ID договора обеспечения
    VAR EnsContractNumber      : string  = "";         // Номер договора обеспечения
    VAR EnsSysType             : integer = 0;          // Тип обязательства
    VAR EnsContractPersonID_Ref: integer = 0;          // Ссылка на ID контрагента
    VAR EnsContractFirstDate   : Date    = Date(0,0,0);// Дата начала договора
    VAR EnsContractLastDate    : Date    = Date(0,0,0);// Дата окончания договора
    VAR EnsContractSum         : Money   = $0;         // Сумма договора
    VAR EnsContractCur         : integer = 0;          // ID валюты договора
    VAR EnsContractStatus      : integer = 0;          // Статус договора обеспечения
    VAR ClaimID_Ref            : integer = 0;          // Ссылка на ID заявки
