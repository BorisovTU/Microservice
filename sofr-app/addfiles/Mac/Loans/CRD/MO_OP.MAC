/*Печать мемориальных ордеров документов операции*/
record DocAcc ("doc_acc.dbt", "loans.def");

macro ПечатьОрдера (DocBuf)
  SetBuff (DocAcc, DocBuf);
  
  VAR MAC = ExecMacroFile ("mo_203", "ПечатьМемориальногоОрдера", DocBuf);

  PRIVATE var {GROUP_MODE};
   
  IF (NOT {GROUP_MODE})
    ViewFile(MAC);
    exit(1);
  END;
   
  return MAC;
end;