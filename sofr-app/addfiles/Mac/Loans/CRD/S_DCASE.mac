IMPORT "caselib.mac";

MACRO RecFilter(curRecord)
   RECORD credit_c ("credit_c.dbt");

   setBuff(credit_c, curRecord);
   return true;
END;

MACRO Фильтр(creditrec)
   if(CheckRecord(creditrec) != true)
      IF (GetTrue(TRUE, "Ссуды не соответствуют признакам однородности выбранного портфеля! Выполнить перевод ?") == TRUE)
         return -1;
      else
         return 1;
      end;
   end;
   return 0;
end;
