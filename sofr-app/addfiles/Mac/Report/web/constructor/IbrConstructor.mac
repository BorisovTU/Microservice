/**
 * Конструктор
 */
class TIbrConstructor()

    /**
     * Отчет
     */
    var report: Object = NULL;

    /**
     * Получить данные отчета
     *
     * @report Variant Идентификатор или наименование отчета
     */
    macro getReportData(report: Variant)

        report = TIbrReport().getData(report);

    end;

end;
