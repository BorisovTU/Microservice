/*
$Name:        d_allrjstep.mac
$Module:      ББ
$Description: Печать отчета о групповом отвержении шагов "Контроль" и "Визирования"
*/
import "d_inter.mac";

MACRO Печать_ОтчетСканирования(Вызов,firstdoc,Статус,Сообщение)

  if(Вызов==Ошибка)
    if(Статус == 0)
      Сообщение = "Шаг отвержен успешно";
    end;
  end;
  
  ПечатьОтчета(Вызов,firstdoc,Статус,Сообщение,"Отчет о групповом отвержении шага.",false);
  
END;