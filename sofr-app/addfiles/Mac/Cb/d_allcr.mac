/*
$Name:           d_allcr.mac
$Module:         Banking
$Description:    Печать отчета о групповом выполнении операций для отложенных мемориальных ордеров
*/  

import "d_inter.mac";

MACRO Печать_ОтчетСканирования(Вызов,firstdoc,Статус,Сообщение)

if(Вызов==Ошибка)
  if(Статус == 0)
    Сообщение = "";
  end;
end;
ПечатьОтчета(Вызов,firstdoc,Статус,Сообщение,"Отчет о групповом выполнении операций для мемориальных ордеров.",true);
END;