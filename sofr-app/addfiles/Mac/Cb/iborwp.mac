      GetParm(0,p1);
      GetParm(1,p2);
      GetParm(2,p3);
      ret=remoteRSL.host.call(func,p1,@p2,@p3);
      SetParm(1,p2);
      SetParm(2,p3);
    elif(ParmCount() == 4)
      GetParm(0,p1);
      GetParm(1,p2);
      GetParm(2,p3);
      GetParm(3,p4);
      ret=remoteRSL.host.call(func,p1,@p2,@p3,@p4);
      SetParm(1,p2);
      SetParm(2,p3);
      SetParm(3,p4);
    else
      GetParm(0,p1);
      GetParm(1,p2);
      GetParm(2,p3);
