    if(ParmCount() < 1)
      ret=remoteRSL.host.call(func);
    elif(ParmCount() == 1)
      GetParm(0,p1);
      ret=remoteRSL.host.call(func,p1);
    elif(ParmCount() == 2)
      GetParm(0,p1);
      GetParm(1,p2);
      ret=remoteRSL.host.call(func,p1,@p2);
      SetParm(1,p2);
