import BankInter;

macro GenerateError(stat)
  if (stat)
    var Err = AL_GetErrorInfo(false);
    if ((Err.Stat != stat) and (stat != 1))
      MemoryError(stat);
      Err = AL_GetErrorInfo(true);
    end;
    RunError(err.Message);
  end;
end;