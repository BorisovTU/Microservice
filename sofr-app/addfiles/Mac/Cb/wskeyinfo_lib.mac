import "globals.mac", rsexts;

macro GeyKeyInfoList(dirpath)
    var dir = TDirList(dirpath + "\\*.xml", "f");
    dir.Sort (0);
    var i = 0;
    var List = TArray;

    while (i < dir.Count)
        if (not dir.IsDir(i))
            List[List.size] = dirpath + "\\" + dir.Name(i);
        end;
        i = i + 1;
    end;

    return List;
end;