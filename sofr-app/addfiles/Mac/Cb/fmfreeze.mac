import FMinter, fm_retailfreeze, fm_bankingfreeze;

macro FMPartyFreeze( PartyID )
  var stat = 0;
  // RS-Retail. Процедура по замораживанию закрепленных за клиентом средств.
  stat = FMPartyFreezeRetal( PartyID );

  // RS-Banking
  if(not stat)
    stat = FMPartyFreezeBanking(PartyID);
  end;

  return stat;
end;

macro FMPartyUnFreeze( PartyID )
  var stat = 0;

  // RS-Banking
  if(not stat)
    stat = FMPartyUnFreezeBanking(PartyID);
  end;

  return stat;
end;
