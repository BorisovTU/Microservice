import PaymInter;

record memorder_paym( "pmpaym.dbt"   );   /* параметры платежа по клиентской платежке */
record memorder_prop( "pmprop.dbt"   );   /* буферы передаются в макрос из RS-Bank    */
record memorder_rmprop( "pmrmprop.dbt" ); /* при вызове ExecuteStep                   */

MACRO ExecuteStep( ):integer

  if( memorder_paym.PaymStatus != PM_FINISHED )
    MsgBox( "Обработка платежа в RS-Retail еще не завершена" );
    return 1;
  else
    return 0;
  end;

END;