/* Печать расходного кассового ордера из ББ */
import pralcash, prcsord;

record rpscshdoc ("pscshdoc.dbt", "bank.def");
record rpmpaym   ("pmpaym.dbt",   "bank.def");

macro PrintOutCashDoc( ord, pm, sb )
  setbuff( rpmpaym, pm  );
  setbuff( rpscshdoc,   ord );
  /* вызов печати стандартного расходного ордера */
  PrintOutgoCashOrder(rpmpaym, rpscshdoc, 0);
end;
