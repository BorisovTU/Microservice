class TWsGetClientProductListRequest
  var PartyID     : integer; //Идентификатор клиента, для которого формируется список продуктов
  var ObjectTypes /*: TArray<Integer>*/; //Типы отбираемых продуктов.
  var Rows        : integer; //Количество строк в ответе
  var OnDate      : date   ; //Дата, на которую проверяется актуальность
  var Oper        : integer; //Идентификатор пользователя в RS-Bank, от которого поступил запрос. Если не задано, то без ограничений.
  var Department  : integer; //Идентификатор подразделения в RS-Bank. Если не задано, то без ограничений.
  var Roles       /*: TArray<Integer>*/; //Список ролей, по которым надо отбирать продукты. 
end;

class TWsClientProductElem
  var ClientProductID       : integer; //Идентификатор продукта клиента в  RS-Bank (prdclient.ClientProductID)
  var SubsystemCode         : string ; //Условное обозначение подсистемы (БО), где хранится досье продукта.
  var Role                  : integer; //Роль клиента в продукте
  var PartyID               : integer; //ID клиента, которому предоставлен банковский продукт (prdclient.PartyID)
  var ProductKindID         : integer; //вид банковского продукта (prdkind.ProductKindID)
  var ProductKindName       : string ; //наименование вида банковского продукта (prdkind.Name)
  var ProductID             : integer; //Ид. банковского продукта (prdproduct.ProductID)
  var ProductName           : string ; //наименование банковского продукта (prdproduct.Name)
  var DocNumber             : string ; //номер документа продукта (prdclient.DocNumber)
  var DprtService           : integer; //Подразделение, где обслуживается (prdclient.Branch)
  var DprtOpen              : integer; //Подразделение, где открыт.
  var BeginDate             : date   ; //период действия - "с" (prdclient.BeginDate)
  var EndDate               : date   ; //период действия - "по" (prdclient.EndDate)
  var ParentClientProductID : integer; //Идентификатор родительского продукта клиента в RS-Bank (prdclient.ParentClientProductID)
  var SfContr               /*: TWsSfContrElem*/; //Описание договора обслуживания, в рамках которого предоставлен продукт клиенту
  var ObjectType            : integer; //вид объекта документа продукта (prdclient.ObjectType)
  var ObjectName            : string ; //Наименование вида объекта документа продукта (objects.Name)
  var ObjectID              : string ; //ID объекта документа продукта (prdclient.ObjectID)
  var Objects               /*: TArray<TWsClntProdObjElem>*/;
end;

class TWsSfContrElem
  var ID        : integer; //Идентификатор договора RS-Bank (sfcontr.ID)             
  var Number    : string ; //Номер договора (sfcontr.Number)         
  var DateBegin : date   ; //Дата начала договора (sfcontr.DateBegin)                
  var DateClose : date   ; //Дата окончания договора (sfcontr.DateClose)             
end;

class TWsClntProdObjElem
  var ClntProdObjID : integer; //ID объекта продукта клиента (prdclientobj.ClntProdObjID)                
  var BeginDate     : date   ; //Дата начала актуальности.Дата регистрации объекта (prdclientobj.BeginDate)               
  var EndDate       : date   ; //Дата окончания актуальности.Дата окончания использования объекта (prdclientobj.EndDate)             
  var ObjectType    : integer; //вид объекта для "объекта продукта" (prdclientobj.ObjectType)            
  var ObjectName    : string ; //наименование вида объекта (по objects.dbt)              
  var ObjectID      : string ; //внутренний ID объекта продукта (prdclientobj.ObjectID)
  var ObjectNumber  : string ; //прикладной номер объекта (prdclientobj.ObjectNumber)            
  var ParentID      : integer; //Идентификатор родительского объекта (prdclientobj.ParentID). Ссылка на объект верхнего уровня (идентификатор объекта). Если =0, то объект является дочерним по отношению к продукту             
end;

