/* Атрибуты структуры TWsGetBankListRequest - cтруктура входного параметра сервиса GetBankList. */
class TWsGetBankListRequest

  var Position  : integer; /* Национальные банки (РФ), Иностранные банки, Если не задано - все. */
  var CodeBIC   : string ; /* Если задан - выбираются данные только по заданному банку. */
  var CodeSWIFT : string ; /* Если задан - выбираются данные только по заданному банку. */
  var FromRow   : integer; /* С какой записи начинать. Если не задано или = 0 - с начальной. */
  var Rows      : integer; /* Сколько строк в ответе. Максимальное количество записей о банках в ответе. Если не задано - без ограничений. */

end;

/* Атрибуты структуры TWsGetBankListResponse - cтруктура элемента массива выходного параметра сервиса GetBankList. */
class TWsGetBankListResponse

  var PartyID     : integer; /* ИД банка. */
  var IsResident  : bool   ; /* Признак резидента. Резидент = true */
  var CodeBIC     : string ; /* БИК (BIC) банка. */
  var CodeSWIFT   : string ; /* SWIFT банка. */
  var FullName    : string ; /* Полное наименование. */
  var ShortName   : string ; /* Краткое наименование. */
  var LatName     : string ; /* Наименование латиницей. */
  var PostAddress : string ; /* Почтовый адрес. */
  var IntAddress  : string ; /* Международный адрес. */
  var CorrAccount : string ; /* Корсчет банка. */
  var BIC_RCC     : string ; /* БИК РКЦ. */
  var Country     : string ; /* Трехсимвольный код страны по справочнику стран (ISO). */

end;

class TWsGetCurrencyListRequest
  var FI_Kinds /*: TArray*/ ; //Указывается код вида ФИ в RS-Bank.
  var FromRow  : integer; //С какой записи начинать.
  var Rows     : integer; //Сколько строк в ответе.
end;

class TWsGetCurrencyListResponse
  var FIID        : integer; //Идентификатор ФИ в RS-Bank
  var FI_Code     : string ; //Внешний код
  var CCY         : string ; //Трехбуквенный код валюты ISO
  var ISO_Number  : string ; //Цифровой код валюты (три символа)
  var Name        : string ; //Краткое наименование
  var Definition  : string ; //Полное наименование
  var CountryCode : string ; //Двухбуквенный код страны эмитента валюты. Источник данных для этого поля пока не известен.
end;

class TWsGetDprtListRequest
  var Statuses /*: TArray*/ ; //Какие записи передавать.
  var DprtCode : integer; //Идентификатор подразделения.
  var DprtPartyNameMask : string; //Маска для поиска по наименованию или первые символы в наименовании подразделения.
end;

