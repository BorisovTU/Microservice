private macro PrintAddBilBookDistr( rs : RsdRecordset, TotalAmount18 : @money, TotalNDSAmount18 : @money, TotalAmount10 : @money, TotalNDSAmount10 : @money, TotalAmount0 : @money, TotalAmount20 : @money, TotalNDSAmount20 : @money, TotalFree : @money, AllTotalWithNDS : @money, fromdate : date)
