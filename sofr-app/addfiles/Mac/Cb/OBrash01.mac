IMPORT InsCarryDoc, "ob_lib.mac";

/************************************************************************/
/* Процедура шага                                                       */
/************************************************************************/
MACRO ExecuteParamStep (KindOper, KindDoc, FirstDoc)

        if (KindOper != ОперацияРасхВнебаланс)
                MsgBox ("Неправильно настроен шаг настроек операции");
                return 1;
        end;

        if (KindDoc != ПервДокВнебаланс)
                MsgBox ("Неправильно настроен шаг настроек операции");
                return 1;
        end;

        RETURN 0;
END;
