/**************************************************************************\
|  Формирование номера договора                                            |
\**************************************************************************/

import CommonInter, acq_val;

macro Create_NewNumber()
  var Number = "";
  var n = 0;

  Number = FormApplicationKey( SB_ACQUIRER );
  n = strlen(Number);

  while ( n < 30 );
    Number = Number + "0";
    n = n + 1;
  end;

  return Number;
end;