//3.5   Класс RSCPFRResponse ответа интерфейсов Отчет о зачислениях и Отчет о расхождении сумм
class RSCPFRResponse(_SenderID :String,_MessageSendID :String,_SenderPointID :Integer, _GUID: String)
    var SenderID        :String = _SenderID     ;   //ID абонента
    var MessageSendID   :String = _MessageSendID;   //ID сообщения-источника
    var SenderPointID   :Integer= _SenderPointID;       //Код подразделения из сообщения-источника
    var MessageRevID    :String = _GUID;   //Уникальный идентификатор ответа
    MessageRevID = SubStr(MessageRevID,2,StrLen(MessageRevID)-2);
    var ObjectID        :String = "";   //ID TweetMessage, созданного при приеме входящего сообщения
    var ObjectError     :String = "";   //Расшифровка сообщения об ошибке
end;

