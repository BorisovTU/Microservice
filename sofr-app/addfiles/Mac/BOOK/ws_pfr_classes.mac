// 3.13	Класс RSCPFRPayment
/*
+ При указании предыдущего платежного поручения обязательно указываются:
	1)	Summa
	2)	DocDate
	3)	DocNumber
+ При указании планируемых платежей обязательно указывается BankName.
+ Внимание! При получении платежа от АБС номер документа рассматривается как:
	1)	строка - необходима проверка корректности задания (только арабские цифры), используются последние 6;
	2)	целое число - необходима проверка на нахождение в диапазоне [0;999999].
	При обнаружении отклонения от формата - входящее сообщение отвергнуть с указанием причины <Некорректное значение в поле DocNumber>.
*/
class RSCPFRPayment
	var BankName       :String ; // ENR_Member->Name			Название банка                                       
	var BIK            :String ; // ENR_Member->BIK				БИК                                                      
	var Branch         :String ; // ENR_Member->DepartmentCode	Код подразделения                            
	var DocNumber      :String ; // ENR_Payment->DocNumber		Номер документа - используются последние 6 цифр  
	var DocDate        :Date   ; // ENR_Payment->DocDate		Дата документа                                       
	var Summa          :Money  ; // ENR_Payment->DocSum			Сумма платежа                                        
	var SurName        :String ; // ENR_Payment->DestFamily		Фамилия получателя                               
	var Name           :String ; // ENR_Payment->DestName		Имя получателя                                   
	var Patronymic     :String ; // ENR_Payment->DestPatronymic	Отчество получателя                          
	var AccountNumber  :String ; // ENR_Member->Account			Номер счета (получателя)                             
	var Amount         :Money  ;
end;


