private const LOG_FILENAME = "..\\TxtFile\\wfactivities.log";

private macro writeLog(type : String, msg : String)
  setOutput(LOG_FILENAME, true);
  [########## ######## ######### #] (date, time, type, msg);
  setOutput(null, true);  
  return msg;  
end;

macro logError(msg : String)
  writeLog("[ERROR]", msg);
  return msg;
end;

macro logMessage(msg : String)
  writeLog("[MESSAGE]", msg);
  return msg;
end;