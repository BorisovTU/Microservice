
file CVSettings ( "cvsettng.dbt" ) key 0;

macro FindCVSettings( TmpCashVal )

  CVSettings.ValType = TmpCashVal.TypeValue;
  CVSettings.Type = TmpCashVal.Type;
  CVSettings.ValCode = TmpCashVal.CodIntValue;
  if ( not GetEQ( CVSettings ) )
    CVSettings.ValCode = -1;
    if ( not GetEQ( CVSettings ) )
      CVSettings.Type = -1;
      if ( not GetEQ( CVSettings ) )
        return false;
      end;
    end;
  end;
  return true;
end;

