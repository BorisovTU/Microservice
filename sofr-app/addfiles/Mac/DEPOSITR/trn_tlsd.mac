macro Сверка_сумм_ПП()
  var cmd, rs;

  cmd = RsdCommand( "select sum(doc.t_sum) DocSum from dtrn_doc_dbt doc, dtrn_payf_dbt payf "+
                    "where doc.t_ListTransfer=payf.t_AutoKey and payf.t_PaymAppKind=:Trn_PaymAppKind and payf.t_PaymAppKey=:Trn_PaymAppKey" );
  
  cmd.addParam( "Trn_PaymAppKind", RSDBP_IN, Trn_PaymAppKind );
  cmd.addParam( "Trn_PaymAppKey", RSDBP_IN, Trn_PaymAppKey );
  rs = RsdRecordset(cmd);
  cmd.execute;
  rs.moveNext();

  if ( moneyl(rs.value("DocSum"))==GlobalSumBusines )

    ClearRecord(trnpaym);
    trnpaym.iApplicationKind=Trn_PaymAppKind;
    trnpaym.ApplicationKey=Trn_PaymAppKey;
    if ( GetEQ(trnpaym) )       

       cmd = RsdCommand( "update dtrn_payf_dbt set t_SignCheckFilial='X' where "+
                         "t_PaymAppKind=:Trn_PaymAppKind and t_PaymAppKey=:Trn_PaymAppKey" );
       cmd.addParam( "Trn_PaymAppKind", RSDBP_IN, Trn_PaymAppKind );
       cmd.addParam( "Trn_PaymAppKey", RSDBP_IN, Trn_PaymAppKey );
       cmd.execute;
       trnpaym.SignCheck = "X";
       if ( not update(trnpaym) )
         IsExistsError = true;
         Запись_в_отчет_расшифровки( "Не занесен признак сверки сумм ПП" );
         Insert (RepFile, "  Ошибка при изменении записи по ПП " );
         return 3;
       end;
    end;
  end;

  return 0;
  OnError(er) /* Обработка ошибок времени выполонения */
    IsExistsError = true;
    Запись_в_отчет_расшифровки( "Не занесен признак сверки сумм ПП" );
    ErrCode = Status( ErrText );
    Insert (RepFile, "  Ошибка в модуле " + string(er.Module) +  "Строка: " + string(er.Line) +
                     " при изменении записи по ПП: " + er.Message );
    return 3;
end;

macro IsAccordCont( ID, Name, CorrAccount, Account, BankID )
  var cmd, rs;

  /* !!! После реализации проекта №335 добавить условие на PartyID !!! */
  cmd = RsdCommand( "select 1 from dtrn_cntr_dbt cntr, dobjcode_dbt objcode where cntr.t_NumberContract=:NumberContract and "+
                    "((cntr.t_NameBusines=:Nme and cntr.t_MFO_Busines=objcode.t_Code and objcode.t_ObjectType=3 and objcode.t_CodeKind=3 and objcode.t_ObjectID=:BankID and "+
                    "cntr.t_CorAcc_Busines=:CorrAccount and cntr.t_Account_Busines=:Accnt) " +
                    /*"or cntr.t_PartyID=:ID" +*/ ")"   ); 
  
  cmd.addParam( "NumberContract", RSDBP_IN, NumberContract );
