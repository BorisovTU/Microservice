/************************************************************
                  Головная функция макроса
************************************************************/
macro LoadPaymentList(NameInputFile:string, p_makePayMsg:bool )
  var ext, dir, Name, cmd, rs, repl, IsRecExists = false, stat, FileDate, FileTime;
  var errmes="";

  if ( CheckESignFile( NameInputFile, errmes ) )
    fnsplit( NameInputFile, dir, Name_rep, ext );
    Name_rep = Name_rep + ".txt";
    if ( Open( InputFile, NameInputFile )   AND
         Open( RepFile, ReportFile + Name_rep ) )

      cmd = RsdCommand( "Select t_State from dtrnfile_dbt where t_Name=:Name and t_FileDate=:FileDate and t_FileTime=:FileTime" ); 

      fnsplit( NameInputFile, dir, Name, ext );
      Name = Name + "." + ext;
      cmd.addParam( "Name", RSDBP_IN, Name );
      GetFileInfo(NameInputFile, FileDate, FileTime);
      cmd.addParam( "FileDate", RSDBP_IN, FileDate );
      cmd.addParam( "FileTime", RSDBP_IN, FileTime );
      rs = RsdRecordset(cmd);

      cmd.execute;
      if ( rs.moveNext() )
         if ( (rs.value("t_State")==0) OR ({BatchMode}) )
           println(" Файл " + NameInputFile + " уже загружался");
           println(" Повторная загрузка отменена");
           return false;
         else
           repl = MsgBoxEx( string("Файл ", NameInputFile, " уже загружался с ошибками|Повторить загрузку?"), MB_YES+MB_NO, IND_NO );
           if ( repl == IND_NO    ) 
             println(" Файл " + NameInputFile + " уже загружался");
             println(" Повторная загрузка отменена");
             return false;
           end;
           IsRecExists = true;
         end;
      end;

      Чтение_записи_о_Подразделении();
      if ( not Обработка_исходного_файла() )
         Close( RepFile );
         Close( InputFile );
         ViewFile( RepFile );
         return false;
      end;
      Close( RepFile );
      Close( InputFile );

      ClearRecord(trnfile);
      trnfile.Name = Name;
      trnfile.ImportDate = {curdate};
      trnfile.FileDate=FileDate;
      trnfile.FileTime=FileTime;
      
      if ( IsExistsError==true )
         trnfile.State = 1;
      else
         trnfile.State = 0;
      end;
      if ( IsRecExists==false )
        stat = Insert(trnfile);
      else
        stat = Update(trnfile);
      end;
      if ( stat )
         ClearRecord(trnpmfil);
         trnpmfil.trnFileID = trnfile.ID;
         trnpmfil.PaymAppKind = Trn_PaymAppKind;
         trnpmfil.PaymAppKey = Trn_PaymAppKey;
         Insert(trnpmfil);
      end;

      if ( not {BatchMode} )
         ViewFile( RepFile );
      end;
      [ Работа закончена ];
      if ( IsExistsError==true )
         return false;
      else
         return true;
      end;
    else
      [ Ошибка при открытии файлов ];
      return false;
    end;
  else
    if ( errmes!="" )
       LogStr = "Список п/п по безнал.платежам не загружен.\n\n" +
                "Файл: \"" + NameInputFile + "\"\n" +
                "Защита загрузки с ЭЦП. " +
                "Файл подписан ЭЦП \"Бикрипт-КСБ\". " +
                "Процедура проверки ЭЦП вернула ошибку: " + errmes;                
    else
       LogStr = "Список п/п по безнал.платежам не загружен.\n\n" +
                "Файл: \"" + NameInputFile + "\"\n" +
                "Защита загрузки с ЭЦП. " +
                "Файл подписан ЭЦП \"Бикрипт-КСБ\". " +
                "ЭЦП не соответствует пакету загрузки.";
    end;

    TrnPayLog( LogStr );

    if ( errmes!="" )
      [
         Процедура проверки ЭЦП вернула ошибку:
         #
         Загрузка записей отменяется.](errmes);
    else
      [
         ЭЦП не соответствует содержанию файла.
         Загрузка записей отменяется.];
    end;
    return false;
  end;
end;
