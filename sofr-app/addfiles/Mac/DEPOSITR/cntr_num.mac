// Шаблон макроса формирования номера договора
//
// Функция makeNumber(contract:TDepContract):bool устанавливает значение 
// поля contract.Number
// и возвращает true в случае успеха,false в случает ошибки 

import DeprIntr;

macro makeNumber(contract:TDepContract): bool
  var num = 5;

  //msgbox(contract.Number);
  contract.Number = "101-" + string(num:o:2);
  //msgbox(contract.Number);

  return true;
end;