/*
$Name:               wf_import.mac
$Module:             DEPOSITR
$Description:        Импорт XPDL
*/

import wf_common;

private macro importProcess
  var path;
  if (selectFile(path, xpdlDir + "\\*.xpdl", "Выберите файл для импорта"))
    message("Выполняется импорт XPDL ...");
    wf.registerPackage(path);
    message;
    msgBox("Импорт файла завершен");
  end; 
onError(e)
  reportRsComError(e.err);
  message;
end;

importProcess;
//run("iisreset");

exit(1);