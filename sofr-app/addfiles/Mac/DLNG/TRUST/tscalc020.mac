/* Расчеты на ОРЦБ в ДУ
   Выполнять перевод средств на торговый счет ? */
import PTInter;

macro ExecuteCaseStep( Kind_Operation, Number_Step, FDoc, KindDoc )
  VAR FD, Oper = TRecHandler( "dl_comm" );

  Oper.SetRecordAddr( FDoc );
  FD = TS_CalcORCB_FD( Oper );

  /*Шаг может быть выполнен только в операции расчетов с ММВБ*/
  if( FD.IsMMVB() )
     return "30"; /* Перевод средств на торговый счет */
  end;

  return "70"; /*Списание/зачисление средств по итогам торгов*/
end;
