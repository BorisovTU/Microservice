/* СНП - корректировка СЧА */

IMPORT "tscorrcost.mac";

MACRO ExecuteStep( DlDoc, FDoc, DocKind, ID_Operation, ID_Step )

  var ServOp = TRecHandler( "tssrvop.dbt" );
  if( TS_RunFromServiceOper( ServOp ) != true )
     MsgBox( "Шаг \"СНП - корректировка СЧА\" должен выполняться только из соответствующей сервисной операции." );
     return 1;
  end;

  return ВыполнитьКорректировкуСЧА( DlDoc, FDoc, DocKind, ID_Operation, ID_Step, ServOp );
END;
