/* Операция сопровождения общих условий ОФБУ 
   Пролонгация договора 1й шаг
   Выполняется при вставке цепочки из сервисной операции подведения итогов*/
IMPORT InsCarryDoc, "tsofbuprolong.mac";

MACRO ExecuteStep( DlDoc, FDoc, DocKind, ID_Operation, ID_Step )

  VAR Order = TRecHandler( "tsorder.dbt" );

  Order.SetRecordAddr( FDoc );

  return ОформитьПролонгацию( Order, DlDoc, ID_Operation, ID_Step );
END;

