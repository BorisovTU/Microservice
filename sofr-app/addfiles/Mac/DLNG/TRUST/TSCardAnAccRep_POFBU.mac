                             "N10_SH8",  СЧА_,
                             "N10_SJ8",  F( N1_() + N1_B7(0) ),
                             "N10_SJ9",  F( N10_J8(0) + "-" + N10_H8(0) ),
                             "N10_SJ10", F( N10_J9(0) + "/" + N10_H8(0) + "*100" ) );

          CopyAllSheetInTotalBook( "Данные по ОФБУ для публикации", false, "N10_All", 0, "Данные по ОФБУ для публикации" );


          /* печатаем вкладку 'Журнал расчета СЧА и курса пая'*/

          if( m_SheetN11 == false )
             UseNewSheetInTotalBook();
             m_SheetN11 = true;
          end;
          SetActiveSheet( "Журнал расчета СЧА и курса пая" );

          PrintFormatString( "",
                             "N11_JH1", F( N10_() + N10_H1(0) ),
                             "N11_JC2", F( N10_() + N10_C2(0) ),
                             "N11_JD2", F( N10_() + N10_D2(0) ),
                             "N11_JD3", m_BegDate,
                             "N11_JF3", GetDateAfterWorkDays(m_FormData.ReportDate, -1));

          /* найдём наименьшую дату значения итога СНП */
          var MinBegDate_СНП:date = date(0,0,0);
          var cmd, RSD, select;
          select = " SELECT min(t_BeginDate) as BeginDate " +
                   "   FROM dtstotal_dbt                  " +
                   "  WHERE t_OrderID    = ?              " + 
                   "    AND t_TotalType  = ?              ";
          cmd = RSDCommand( select );
          cmd.NullConversion = true;
          cmd.addParam( "", RSDBP_IN, m_OrderFD.Order().rec.ID );
          cmd.addParam( "", RSDBP_IN, ДУ_СНП );
          cmd.execute();
          RSD = TRsbDataSet( cmd );
          if( RSD.MoveNext() and (RSD.BeginDate != NULL) )
             MinBegDate_СНП = SQL_ConvTypeDate(RSD.BeginDate);
          end;

          RegisterTable( "N11_MainTable_1", "",
                         "A7", "N11_JB7", "N11_JC7", "N11_JD7", "N11_JE7", "N11_JF7", "N11_JG7", "N11_JH7", "N11_JI7", "N11_JJ7", "N11_JK7", "N11_JL7" );

          var cmd_date = RSDCommand( " SELECT distinct t_BeginDate " +
                                     "   FROM dtstotal_dbt         " +
                                     "  WHERE t_OrderID    = ?     " + 
                                     "    AND t_TotalType  = ?     " +
                                     "    AND t_EventID    = ?     " +
                                     "    AND t_BeginDate  <= ?    " +
                                     " ORDER BY t_BeginDate        " );
          cmd_date.NullConversion = true;
          cmd_date.addParam( "", RSDBP_IN, m_OrderFD.Order().rec.ID );
          cmd_date.addParam( "", RSDBP_IN, ДУ_СНП );
          cmd_date.addParam( "", RSDBP_IN, TS_DemandEvent("79") );
          cmd_date.addParam( "", RSDBP_IN, m_FormData.ReportDate );
          cmd_date.execute();
          var RSD_date = TRsbDataSet( cmd_date );
          var i:integer = 0;
          while( RSD_date.MoveNext() and (RSD_date.BeginDate != NULL) )

             var CalcDate:date = GetDateAfterWorkDays(SQL_ConvTypeDate(RSD_date.BeginDate), -1);
             var yyyy_:integer = 0, mm_:integer = 0;
             DateSplit( CalcDate, null, mm_, yyyy_ );
             var BDate = date(1, mm_, yyyy_);

             if( BDate < m_BegDate )
                BDate = MinBegDate_СНП;
             end;

             var СЧА75:money = $0.0;
             if( ПолучитьИтогДУ(m_OrderFD.Order().rec.ID, ДУ_СЧА75, NULL, 0, NULL, NULL, NULL, CalcDate, NULL, NULL, @СЧА75, 0, 0) )
                СЧА75 = $0.0;
             end;

             var Н_Купр_ув:money = $0.0, Н_Купр_ув_end:money = $0.0, Н_Купр_ув_beg:money = $0.0;
             if(ПолучитьИтогДУ(m_OrderFD.Order().rec.ID, ДУ_ИТОГ_Н_Купр, null, 0, NULL, NULL, NULL, CalcDate-1, @Н_Купр_ув_beg, NULL, NULL))
                Н_Купр_ув_beg = $0.0;
             end;
             if(ПолучитьИтогДУ(m_OrderFD.Order().rec.ID, ДУ_ИТОГ_Н_Купр, null, 0, NULL, NULL, NULL, CalcDate, @Н_Купр_ув_end, NULL, NULL))
                Н_Купр_ув_end = $0.0;
             end;
             Н_Купр_ув = Н_Купр_ув_end - Н_Купр_ув_beg;

             var СЧА79:money = $0.0;
             if( ПолучитьИтогДУ(m_OrderFD.Order().rec.ID, ДУ_ИТОГ_СЧА, NULL, 0, NULL, NULL, NULL, CalcDate, NULL, NULL, @СЧА79, 0, 0) )
                СЧА79 = $0.0;
             end;

             var УвеличениеПаев_beg:money = $0.0, УменьшениеПаев_beg:money = $0.0,
                 УвеличениеПаев_end:money = $0.0, УменьшениеПаев_end:money = $0.0;
             if( ПолучитьИтогДУ(m_OrderFD.Order().rec.ID, ДУ_КПУ, TS_DemandEvent("85"), 0, NULL, NULL, NULL, CalcDate, @УвеличениеПаев_end, NULL, NULL, 0, 0) )
                УвеличениеПаев_end = $0.0;
             end;
             if( ПолучитьИтогДУ(m_OrderFD.Order().rec.ID, ДУ_КПУ, TS_DemandEvent("86"), 0, NULL, NULL, NULL, CalcDate, NULL, @УменьшениеПаев_end, NULL, 0, 0) )
                УменьшениеПаев_end = $0.0;
             end;
             if( ПолучитьИтогДУ(m_OrderFD.Order().rec.ID, ДУ_КПУ, TS_DemandEvent("85"), 0, NULL, NULL, NULL, (CalcDate - 1), @УвеличениеПаев_beg, NULL, NULL, 0, 0) )
                УвеличениеПаев_beg = $0.0;
             end;
             if( ПолучитьИтогДУ(m_OrderFD.Order().rec.ID, ДУ_КПУ, TS_DemandEvent("86"), 0, NULL, NULL, NULL, (CalcDate - 1), NULL, @УменьшениеПаев_beg, NULL, 0, 0) )
                УменьшениеПаев_beg = $0.0;
             end;

             PrintTableLine( "N11_JB7", CalcDate,                                                                  NULL,
                             "N11_JC7", СЧА75,                                                                     NULL,
                             "N11_JD7", Н_Купр_ув,                                                                 NULL,
                             "N11_JE7", СЧА79,                                                                     NULL,
                             "N11_JF7", m_OrderFD.КоличествоПаев(GetDateAfterWorkDays(CalcDate, -1)),              m_OrderFD.Order().rec.DP_AmountPrecision,
                             "N11_JG7", УвеличениеПаев_end - УвеличениеПаев_beg,                                   m_OrderFD.Order().rec.DP_AmountPrecision,
                             "N11_JH7", УменьшениеПаев_end - УменьшениеПаев_beg,                                   m_OrderFD.Order().rec.DP_AmountPrecision,
                             "N11_JI7", F( N11_F7(i) + "+" + N11_G7(i) + "-" + N11_H7(i) ),                        m_OrderFD.Order().rec.DP_AmountPrecision,
                             "N11_JJ7", m_OrderFD.СНП(SQL_ConvTypeDate(RSD_date.BeginDate)),                       NULL,
                             "N11_JK7", TS_IIF(i == 0, "", F( "(" + N11_J7(i) + "/" + N11_J7(i-1) + "-1)*100" ) ), NULL,
                             "N11_JL7", F( "(" + N11_J7(i) + "/" + m_OrderFD.СНП(BDate) + "-1)*100" ),             NULL );

             i = i + 1;
          end;

          if( i == 0 )
             PrintTableLine( "N11_JB7", "", NULL,
                             "N11_JC7", "", NULL,
                             "N11_JD7", "", NULL,
                             "N11_JE7", "", NULL,
                             "N11_JF7", "", NULL,
                             "N11_JG7", "", NULL,
                             "N11_JH7", "", NULL,
                             "N11_JI7", "", NULL,
                             "N11_JJ7", "", NULL,
                             "N11_JK7", "", NULL,
                             "N11_JL7", "", NULL );
          end;

          EndTable();
          CopyAllSheetInTotalBook( "Журнал расчета СЧА и курса пая", false, "N11_All", 0, "Журнал расчета СЧА и курса пая" );

          /* печатаем вкладку 'ИТОГ'*/

          if( m_SheetN1 == false )
             UseNewSheetInTotalBook();
             m_SheetN1 = true;
          end;
          SetActiveSheet( "ИТОГ" );

          /* Заголовок отчета */

          var OpenDate:date = date(0,0,0);
          var MngPlan = m_OrderFD.MngPlan(m_FormData.ReportDate);
          if( MngPlan != NULL )
             OpenDate = MngPlan.rec.OpenDate;
          end;
          PrintFormatString( "",
                             "N1_IF1", m_EndDate,
                             "N1_B2",  F( N10_() + N10_C2(0) ),
                             "N1_C2",  F( N10_() + N10_D2(0) ),
                             "N1_IF2", OpenDate );

          /* Расчет отдельных показателей СЧА и СНП на отчетную дату, а также изменение СНП и доходности */

          var ТСЧА:money = $0.0; 
          if( ПолучитьИтогДУ(m_OrderFD.Order().rec.ID, ДУ_ИТОГ_СЧА, NULL, 0, NULL, NULL, NULL, GetDateAfterWorkDays(m_FormData.ReportDate, -1), NULL, NULL, @ТСЧА) )
             ТСЧА = $0.0;
          end;

          var ТСНП = m_OrderFD.СНП(m_FormData.ReportDate);

          /* определяем границы "текущего месяца" */
          var BegCurDate:date, EndCurDate:date;
          var CurMon:integer, CurYear:integer;
          var m_BegDateMon:integer, m_BegDateYear:integer;
          DateSplit( m_FormData.ReportDate, null, CurMon, CurYear );
          DateSplit( m_BegDate, null, m_BegDateMon, m_BegDateYear );
         
          if( (m_BegDateYear == CurYear) and (m_BegDateMon == CurMon) )
             BegCurDate = m_BegDate;
          else
             BegCurDate = date(1, CurMon, CurYear);
          end;
          EndCurDate = date(LastDay(CurMon, CurYear), CurMon, CurYear);

          var ССНП = m_OrderFD.СНП(BegCurDate);
          var B = ДУ_ДнейВГоду(CurYear);
          var Days1 = m_FormData.ReportDate - BegCurDate; /*не включая отчётную дату*/

          var ПСНП = m_OrderFD.СНП(MinBegDate_СНП);
          var Days2 = m_FormData.ReportDate - MinBegDate_СНП;
          var ГСНП = m_OrderFD.СНП(date(1,1,CurYear));
          var Days3 = m_FormData.ReportDate - date(1,1,CurYear);

          /* дата значения последнего созданного итога ДУ_СНП78 */
          var MaxBegDate_СНП78:date = date(0,0,0);
          select = " SELECT max(t_BeginDate) as BeginDate " +
                   "   FROM dtstotal_dbt                  " +
                   "  WHERE t_OrderID    = ?              " + 
                   "    AND t_TotalType  = ?              " + 
                   "    and t_BeginDate <= ?              ";
          cmd = RSDCommand( select );
          cmd.NullConversion = true;
          cmd.addParam( "", RSDBP_IN, m_OrderFD.Order().rec.ID );
          cmd.addParam( "", RSDBP_IN, ДУ_СНП78 );
          cmd.addParam( "", RSDBP_IN, m_FormData.ReportDate );
          cmd.execute();
          RSD = TRsbDataSet( cmd );
          if( RSD.MoveNext() and (RSD.BeginDate != NULL) )
             MaxBegDate_СНП78 = GetDateAfterWorkDays(SQL_ConvTypeDate(RSD.BeginDate), 1);
          end;

          var II7:money = $0.0, IL7:money = $0.0, IP7:money = $0.0, IS7:money = $0.0, IV7:money = $0.0, IY7:money = $0.0;
          if( ССНП != 0.0 )
             II7 = (ТСНП / ССНП - 1) * 100;
          end;
          if( not ((ССНП == 0.0) or (Days1 == 0.0)) )
             IL7 = (ТСНП - ССНП) / ССНП * 100 * B / Days1;
          end;
          if( ПСНП != 0.0 )
             IP7 = (ТСНП / ПСНП - 1) * 100;
          end;
          if( not ((ПСНП == 0.0) or (Days2 == 0.0)) )
             IS7 = (ТСНП - ПСНП) / ПСНП * 100 * B / Days2;
          end;
          if( ГСНП != 0.0 )
             IV7 = (ТСНП / ГСНП - 1) * 100;
          end;
          if( not ((ГСНП == 0.0) or (Days3 == 0.0)) )
             IY7 = (ТСНП - ГСНП) / ГСНП * 100 * B / Days3;
          end;

          var ПоследняяОтчётнаяДата:date = date(0,0,0);
          if( CurMon <= 3 )
             ПоследняяОтчётнаяДата = date(31, 12, CurYear-1);
          elif( (CurMon > 3) and (CurMon <= 6) )
             ПоследняяОтчётнаяДата = date(31, 3, CurYear);
          elif( (CurMon > 6) and (CurMon <= 9) )
             ПоследняяОтчётнаяДата = date(30, 6, CurYear);
          elif( (CurMon > 9) and (CurMon <= 12) )
             ПоследняяОтчётнаяДата = date(30, 9, CurYear);
          end;
          if( IsWorkday(ПоследняяОтчётнаяДата) == 0 )
             ПоследняяОтчётнаяДата = GetDateAfterWorkDays(ПоследняяОтчётнаяДата, -1);
          end;

          var Amount_ДП:integer = 0;
          cmd = RSDCommand( " select count(1) NRec from ( " + SqlQuery + " ) " );
          cmd.addParam("", RSDBP_IN, TS_DOC_DPOFBU            );
          cmd.addParam("", RSDBP_IN, m_OrderFD.Order().rec.ID );
          cmd.addParam("", RSDBP_IN, m_FormData.ReportDate    );
          cmd.addParam("", RSDBP_IN, PM_WRITEOFF_SUM_BUY      );
          cmd.addParam("", RSDBP_IN, PM_WRTSUM_FORM           );
          cmd.execute();
          RSD = TRsbDataSet(cmd);
          if( RSD.MoveNext() )
             Amount_ДП = SQL_ConvTypeInteger(RSD.NRec);
          end;

          var Delta_ДП = -1;

          PrintFormatString( "",
                             "N1_IB7", ТСЧА,
                             "N1_IF7", ТСНП,
                             "N1_II7", II7,
                             "N1_IL7", IL7,
                             "N1_IP7", IP7,
                             "N1_IS7", IS7,
                             "N1_IV7", IV7,
                             "N1_IY7", IY7,
                             "N1_ID11", TS_IIF(MaxBegDate_СНП78 == Date(0,0,0), "", MaxBegDate_СНП78 ),
                             "N1_IE11", m_OrderFD.СНП(MaxBegDate_СНП78),
                             "N1_IE12", F( FormulaIF() + "(" + N1_F7(0) + ">" + N1_F11(0) + ";(" + N1_F7(0) + "-" + N1_F11(0) + ")*" + TS_IIF(Amount_ДП > 0, N1_I16(Delta_ДП+Amount_ДП), 0.0) + "*" + N8_() + N8_A19(0) + ";0)" ) );

          CopyAllSheetInTotalBook( "ИТОГ", false, "N1_Head", 0, "ИТОГ" );

          /* Договора присоединения */

          i = 0;
          if( Amount_ДП > 0 )

             var NumStr:integer = 0;
             var PrevOrderID:integer = -1;

             RegisterTable( "N1_MainTable_1", "",
                            "N1_IA15", "N1_IB15", "N1_IC15", "N1_ID15", "N1_IE15", "N1_IF15", "N1_IG15", "N1_IH15", "N1_II15", "N1_IJ15",
                            "N1_IK15", "N1_IL15", "N1_IM15", "N1_IN15", "N1_IO15", "N1_IP15", "N1_IQ15", "N1_IR15", "N1_IS15", "N1_IT15",
                            "N1_IU15", "N1_IV15", "N1_IW15", "N1_IX15", "N1_IY15", "N1_IZ15", "N1_IAA15", "N1_IAB15", "N1_IAC15", "N1_IAD15" );

             cmd = RSDCommand( SqlQuery );
             cmd.addParam("", RSDBP_IN, TS_DOC_DPOFBU            );
             cmd.addParam("", RSDBP_IN, m_OrderFD.Order().rec.ID );
             cmd.addParam("", RSDBP_IN, m_FormData.ReportDate    );
             cmd.addParam("", RSDBP_IN, PM_WRITEOFF_SUM_BUY      );
             cmd.addParam("", RSDBP_IN, PM_WRTSUM_FORM           );
             cmd.execute();
             RSD = TRsbDataSet(cmd);
             var CurOrderFD:TS_OrderFD;
             var BegValidDate, EndValidDate;
             while( RSD.MoveNext() )

                if( PrevOrderID != RSD.ID )
                   NumStr = NumStr + 1;
                   CurOrderFD = TS_OrderFD(0, RSD.ID);
                   var Valid = CurOrderFD.Valid( m_FormData.ReportDate );
                   if( Valid != NULL )
                      BegValidDate = Valid.rec.BeginDate;
                      EndValidDate = Valid.rec.EndDate;
                   end;
                end;

                var RequestFD = TS_RequestFD(0, RSD.DocumentID);

                var СКу:money = $0.0;
                if(ПолучитьИтогДУ(CurOrderFD.Order().rec.ID, ДУ_ИТОГ_СК, TS_DemandEvent("85"), 0, NULL, NULL, NULL, RequestFD.Request().rec.CapitalDate, NULL, NULL, @СКу))
                   СКу = $0.0;
                end;

                var w0, Amount0:money = $0.0;
                if( GetLotFirstInstance(RSD.SumID, @w0) )
                   Amount0 = w0.Amount;
                end;

                var N1_IG15;
                if( IsWorkday(m_EndDate) == 1 )
                   N1_IG15 = F( N1_F1(0) + "-" + N1_E16(Delta_ДП+i) );
                else
                   N1_IG15 = F( FormulaDATE(m_FormData.ReportDate) + "-" + N1_E16(Delta_ДП+i) );
                end;

                PrintLineTable1( TS_IIF(PrevOrderID != RSD.ID, NumStr, ""),
                                 CurOrderFD.Trustor().rec.ShortName,
                                 CurOrderFD.Number(),
                                 RequestFD.Ground().rec.RegistrDate,
                                 RequestFD.Request().rec.CapitalDate,
                                 ДатаВыводаКапитала(RSD.SumID, m_FormData.ReportDate),
                                 N1_IG15,
                                 RSD.BalanceCost,
                                 F( FormulaIF() + "(" + N1_E16(Delta_ДП+i) + "<=" + FormulaDATE(date(28,12,2007)) + ";" + N1_H16(Delta_ДП+i) + "*0" + DecimalSeparator + "2/100;0)" ),
                                 F( N1_H16(Delta_ДП+i) + "-" + N1_I16(Delta_ДП+i) ),
                                 m_OrderFD.СНП(RequestFD.Request().rec.CapitalDate),
                                 F( ДУ_ЧислоCЗаданнойТочностью(RSD.Amount, m_OrderFD.Order().rec.DP_AmountPrecision) ),
                                 F( N1_L16(Delta_ДП+i) + "*" + N1_F7(0) ),
                                 F( N1_M16(Delta_ДП+i) + "-" + N1_J16(Delta_ДП+i) ),
                                 F( N1_N16(Delta_ДП+i) + "/" + N1_J16(Delta_ДП+i) + "*100/" + N1_G16(Delta_ДП+i) + "*" + N8_() + N8_A9(0) ),
                                 F( "(" + N1_M16(Delta_ДП+i) + "/" + N1_J16(Delta_ДП+i) + "-1)*100" ),
                                 TS_IIF(PrevOrderID != RSD.ID, EndValidDate, ""),
                                 СКу,
                                 F( N1_R16(Delta_ДП+i) + "*0" + DecimalSeparator + "2/100" ),
                                 "",
                                 F( ДУ_ЧислоCЗаданнойТочностью(Amount0, m_OrderFD.Order().rec.DP_AmountPrecision) ),
                                 F( ДУ_ЧислоCЗаданнойТочностью(Amount0 - RSD.Amount, m_OrderFD.Order().rec.DP_AmountPrecision) ),
                                 F( FormulaIF() + "(" + N1_G16(Delta_ДП+i) + ">365;0;" + FormulaIF() + "(" + N1_G16(Delta_ДП+i) + ">180;" + N1_M16(Delta_ДП+i) + "*1/100;" + N1_M16(Delta_ДП+i) + "*2/100))" ),
                                 F( N1_M16(Delta_ДП+i) + "-" + N1_W16(Delta_ДП+i) ),
                                 F( FormulaIF() + "(" + N1_N16(Delta_ДП+i) + ">0;(" + N1_N16(Delta_ДП+i) + "-" + N1_W16(Delta_ДП+i) + ")*0" + DecimalSeparator + "13;0)" ),
                                 F( N1_X16(Delta_ДП+i) + "-" + N1_Y16(Delta_ДП+i) ),
                                 F( N1_Z16(Delta_ДП+i) + "-" + N1_H16(Delta_ДП+i) ),
                                 F( N1_AA16(Delta_ДП+i) + "/" + N1_H16(Delta_ДП+i) + "/" + N1_G16(Delta_ДП+i) + "*" + N8_() + N8_A9(0) + "*100" ),
                                 F( FormulaIF() + "(" + FormulaDATE(CurOrderFD.BeginDate()) + "<" + FormulaDATE(ПоследняяОтчётнаяДата) + ";" + m_OrderFD.СНП(ПоследняяОтчётнаяДата) + "*" + N1_L16(Delta_ДП+i) + ";" + N1_J16(Delta_ДП+i) + ")" ),
                                 F( "(" + N1_M16(Delta_ДП+i) + "-" + N1_AC16(Delta_ДП+i) + ")/" + N1_AC16(Delta_ДП+i) )
                               );

                PrevOrderID = RSD.ID;
                i = i + 1;
             end;

             /* сторока с итогами */
             SetCurrentLineFont( 10, true, false, null, null, COLOR_GRAY );
             PrintLineTable1( "",
                              "Общий итог",
                              "", "", "", "", "",
                              F( FormulaSUM() + "(" + N1_H16(Delta_ДП) + ":" + N1_H16(Delta_ДП+i-1) + ")" ),
                              F( FormulaSUM() + "(" + N1_I16(Delta_ДП) + ":" + N1_I16(Delta_ДП+i-1) + ")" ),
                              F( FormulaSUM() + "(" + N1_J16(Delta_ДП) + ":" + N1_J16(Delta_ДП+i-1) + ")" ),
                              "",
                              F( FormulaSUM() + "(" + N1_L16(Delta_ДП) + ":" + N1_L16(Delta_ДП+i-1) + ")" ),
                              F( FormulaSUM() + "(" + N1_M16(Delta_ДП) + ":" + N1_M16(Delta_ДП+i-1) + ")" ),
                              F( FormulaSUM() + "(" + N1_N16(Delta_ДП) + ":" + N1_N16(Delta_ДП+i-1) + ")" ),
                              "", "", "",
                              F( FormulaSUM() + "(" + N1_R16(Delta_ДП) + ":" + N1_R16(Delta_ДП+i-1) + ")" ),
                              F( FormulaSUM() + "(" + N1_S16(Delta_ДП) + ":" + N1_S16(Delta_ДП+i-1) + ")" ),
                              F( FormulaSUM() + "(" + N1_T16(Delta_ДП) + ":" + N1_T16(Delta_ДП+i-1) + ")" ),
                              F( FormulaSUM() + "(" + N1_U16(Delta_ДП) + ":" + N1_U16(Delta_ДП+i-1) + ")" ),
