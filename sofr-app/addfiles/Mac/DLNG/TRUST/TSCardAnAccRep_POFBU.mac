                    N4_OB10 = N4_OB10 + dlsum.rec.Sum;
                 end;
                 if( FindDLSUM( RSD3.DocKind, RSD3.DealIDBuy, DLSUM_KIND_AGENT_ONCE_IMPORT, RSD3.DealDate, dlsum ) == 0)
