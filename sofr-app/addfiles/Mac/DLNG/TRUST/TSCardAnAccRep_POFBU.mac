        EndTable();
        CopyAllSheetInTotalBook( "Акции", false, "N2_Table2", 0, "Акции" );

        /* печатем таблицу "Комиссии" */
        
        if( ExistsCom > 0 )

           var DeltaCom = DeltaDeal + i + k - 1;
