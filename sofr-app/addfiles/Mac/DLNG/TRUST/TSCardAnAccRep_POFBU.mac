        EndTable();
        CopyAllSheetInTotalBook( "Облигации", false, "N4_Table2", 0, "Облигации" );

        /* печатем таблицу "Начисленный доход" */

        Query6 = RSDCommand( HeadSelect2 + SqlQuery + FooterQuery );
