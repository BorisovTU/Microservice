       var RSD = TRsbDataSet( cmd );
       if( RSD.MoveNext() and (RSD.DocumentID != NULL) )
          var RequestFD = TS_RequestFD(0, RSD.DocumentID);
          RetVal = RequestFD.Request().rec.CapitalDate;
       else
          RetVal = "";
       end;

       return RetVal;
    END;

