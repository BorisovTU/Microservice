                            TS_GetCouponSumByPeriod( SQL_ConvTypeInteger(RSD3.FIID), SQL_ConvTypeSum(RSD3.Amount), SQL_ConvTypeDate(RSD3.Date), m_EndDate, "", null, null, null, null ),
