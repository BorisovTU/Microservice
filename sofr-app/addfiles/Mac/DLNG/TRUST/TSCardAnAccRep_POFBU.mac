          SqlData_Н_Кдв_ум.addParam("", RSDBP_IN, ДУ_ИТОГ_Н_Кдв);
          SqlData_Н_Кдв_ум.addParam("", RSDBP_IN, m_BegDate);
          SqlData_Н_Кдв_ум.addParam("", RSDBP_IN, m_EndDate);
          SqlData_Н_Кдв_ум.execute();
          var DataSet_Н_Кдв_ум = TRsbDataSet(SqlData_Н_Кдв_ум);

          var B56, C56, D56, E56, G56, H56, I56, J56, L56, M56, N56, O56;
          i = 0;
          while(i < Amount_m )

             if( DataSet_Н_Купр_ув.MoveNext() )
                B56 = SQL_ConvTypeDate(DataSet_Н_Купр_ув.rec.BeginDate);
                C56 = SQL_ConvTypeSum(DataSet_Н_Купр_ув.rec.RestAmount);
             else
                B56 = C56 = "";
             end;

             if( DataSet_Н_Купр_ум.MoveNext() )
                D56 = SQL_ConvTypeDate(DataSet_Н_Купр_ум.rec.BeginDate);
                E56 = SQL_ConvTypeSum(DataSet_Н_Купр_ум.rec.RestAmount);
             else
                D56 = E56 = "";
             end;

             if( DataSet_Н_Кусп_ув.MoveNext() )
                G56 = SQL_ConvTypeDate(DataSet_Н_Кусп_ув.rec.BeginDate);
                H56 = SQL_ConvTypeSum(DataSet_Н_Кусп_ув.rec.RestAmount);
             else
                G56 = H56 = "";
             end;

             if( DataSet_Н_Кусп_ум.MoveNext() )
                I56 = SQL_ConvTypeDate(DataSet_Н_Кусп_ум.rec.BeginDate);
                J56 = SQL_ConvTypeSum(DataSet_Н_Кусп_ум.rec.RestAmount);
             else
                I56 = J56 = "";
             end;

             if( DataSet_Н_Кдв_ув.MoveNext() )
                L56 = SQL_ConvTypeDate(DataSet_Н_Кдв_ув.rec.BeginDate);
                M56 = SQL_ConvTypeSum(DataSet_Н_Кдв_ув.rec.RestAmount);
             else
                L56 = M56 = "";
             end;

             if( DataSet_Н_Кдв_ум.MoveNext() )
                N56 = SQL_ConvTypeDate(DataSet_Н_Кдв_ум.rec.BeginDate);
                O56 = SQL_ConvTypeSum(DataSet_Н_Кдв_ум.rec.RestAmount);
             else
                N56 = O56 = "";
             end;

             PrintTableLine( "N1_IB55", B56, NULL,
                             "N1_IC55", C56, NULL,
                             "N1_ID55", D56, NULL,
                             "N1_IE55", E56, NULL,
                             "N1_IG55", G56, NULL,
                             "N1_IH55", H56, NULL,
                             "N1_II55", I56, NULL,
                             "N1_IJ55", J56, NULL,
                             "N1_IL55", L56, NULL,
                             "N1_IM55", M56, NULL,
                             "N1_IN55", N56, NULL,
                             "N1_IO55", O56, NULL );
             i = i + 1;
          end;

          B56 = D56 = G56 = I56 = L56 = N56 = "Итого:";
          if( i > 0 )
             C56 = F( FormulaSUM() + "(" + N1_C56(DeltaCom) + ":" + N1_C56(DeltaCom + i - 1) + ")" );
             E56 = F( FormulaSUM() + "(" + N1_E56(DeltaCom) + ":" + N1_E56(DeltaCom + i - 1) + ")" );
             H56 = F( FormulaSUM() + "(" + N1_H56(DeltaCom) + ":" + N1_H56(DeltaCom + i - 1) + ")" );
             J56 = F( FormulaSUM() + "(" + N1_J56(DeltaCom) + ":" + N1_J56(DeltaCom + i - 1) + ")" );
             M56 = F( FormulaSUM() + "(" + N1_M56(DeltaCom) + ":" + N1_M56(DeltaCom + i - 1) + ")" );
             O56 = F( FormulaSUM() + "(" + N1_O56(DeltaCom) + ":" + N1_O56(DeltaCom + i - 1) + ")" );
          else
             C56 = E56 = H56 = J56 = M56 = O56 = 0.0;
          end;

          PrintTableLine( "N1_IB55", B56, NULL,
                          "N1_IC55", C56, NULL,
                          "N1_ID55", D56, NULL,
                          "N1_IE55", E56, NULL,
                          "N1_IG55", G56, NULL,
                          "N1_IH55", H56, NULL,
                          "N1_II55", I56, NULL,
                          "N1_IJ55", J56, NULL,
                          "N1_IL55", L56, NULL,
                          "N1_IM55", M56, NULL,
                          "N1_IN55", N56, NULL,
                          "N1_IO55", O56, NULL );
          EndTable();

          CopyAllSheetInTotalBook( "ИТОГ", false, "N1_Table4", 0, "ИТОГ" );

          /* "Дивиденды" */

          RegisterTable( "N1_MainTable_5", "",
                         "A61", "N1_IB61", "N1_IC61" );

          var SqlData_Д_Двд = RSDCommand( SqlQuery_Итог(1) );
          SqlData_Д_Двд.addParam("", RSDBP_IN, m_OrderFD.Order().rec.ID);
          SqlData_Д_Двд.addParam("", RSDBP_IN, ДУ_ИТОГ_Д_Двд);
          SqlData_Д_Двд.addParam("", RSDBP_IN, m_BegDate);
          SqlData_Д_Двд.addParam("", RSDBP_IN, m_EndDate);
          SqlData_Д_Двд.execute();
          var DataSet_Д_Двд = TRsbDataSet(SqlData_Д_Двд);

          while( DataSet_Д_Двд.MoveNext() )
             PrintTableLine( "N1_IB61", SQL_ConvTypeDate(DataSet_Д_Двд.rec.BeginDate), NULL,
