           AB6  = N2_F12(DeltaDeal+i+k);
           AB7  = N2_G12(DeltaDeal+i+k);
           AB9  = N2_I12(DeltaDeal+i+k);
           AB11 = N2_K12(DeltaDeal+i+k);
           AB13 = N2_M12(DeltaDeal+i+k);
          
           i = i + 1;
        end;

        SetCurrentLineFont( 10, true, false, null, null, COLOR_GRAY );
