       if( (sign == null) or (sign < 0) )
          RetVal = RetVal + " AND tstotal.t_Sign < 0 ";
       else
          RetVal = RetVal + " AND tstotal.t_Sign > 0 ";
       end;

       RetVal = RetVal + " GROUP BY tstotal.t_BeginDate "
                       + " ORDER BY tstotal.t_BeginDate ";

       return RetVal;
    END;

    MACRO ДатаВыводаКапитала( BuyID:integer, CreateDate:date )

       var RetVal;
       var cmd = RSDCommand( " select paym.t_DocumentID                                      " +
                             "   from dpmwrtlnk_dbt lnk, dpmwrtsum_dbt lot, dpmpaym_dbt paym " +
                             "  where lnk.t_BuyID       = ?                                  " +
                             "    and lnk.t_CreateDate <= ?                                  " +
                             "    and lot.t_SumID = lnk.t_SaleID                             " +
                             "    and paym.t_paymentid = lot.t_DocID                         " +
                             "    and RowNum = 1                                             " +
                             " order by lnk.t_CreateDate DESC, lnk.t_LnkID DESC              " );
       cmd.NullConversion = true;
       cmd.addParam( "", RSDBP_IN, BuyID );
       cmd.addParam( "", RSDBP_IN, CreateDate );
