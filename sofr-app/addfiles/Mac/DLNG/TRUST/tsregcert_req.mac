/* Шаг "Оформление сертификата долевого участия"*/
IMPORT TSInter;

PRIVATE VAR Request = TRecHandler( "tsiorqst.dbt" ); /*Заполняется программой при выполнении шага*/

MACRO ExecuteStep( _DlDoc, FDoc, DocKind, _ID_Operation, ID_Step )

  if( TS_RegistryCertification( null, Request.rec.ID ) != 0 )
     MsgBox( "СРДУ не оформлен. Необходимо выполнить это действие позже." );
     return 1;
  end;

  return 0;
END;


