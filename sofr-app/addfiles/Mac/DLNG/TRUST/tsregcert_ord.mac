/* Шаг "Оформление сертификата долевого участия"*/
IMPORT TSInter;

MACRO ExecuteStep( _DlDoc, FDoc, DocKind, _ID_Operation, ID_Step )

  VAR Order = TRecHandler( "tsorder.dbt" );

  Order.SetRecordAddr( FDoc );

  if( TS_RegistryCertification( Order.rec.ID, 0 ) != 0 )
     MsgBox( "СРДУ не оформлен." );
     return 1;
  end;

  return 0;
END;


