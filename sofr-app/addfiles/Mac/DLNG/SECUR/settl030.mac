/* Операция Расчеты на ОРЦБ         
   Шаг - Перевод средств на торговый счет*/
import "settl.mac";

macro ExecuteStep( Doc, adr, DocKind, ID_Operation, ID_Step )
  VAR comm = TRecHandler( "dl_comm.dbt" );

  comm.SetRecordAddr( adr );

  if( not ПереводСредствНаТорговыйСчет( Doc, comm ))
     return 1;
  end;

  return 0;
end;
