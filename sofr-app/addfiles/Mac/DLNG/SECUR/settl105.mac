/*
 $Name: settl105.mac
 $Module: Ценые бумаги
 $Description: Вчтавка цепочки возврата с клирингового на корреспондентский счет при импорте денежной выписки
 */

/* Операция Расчеты на ОРЦБ         
   Шаг - Перевод средств на клиринго-вый счет с корсчета*/
import "settl.mac";

macro ExecuteStep( Doc, adr )
  VAR comm = TRecHandler( "dl_comm.dbt" );

  comm.SetRecordAddr( adr );

  if( not ВозвратСКлирСчетаНаКорсчет( Doc, comm ) )
     return 1;
  end;

  return 0;
end;

