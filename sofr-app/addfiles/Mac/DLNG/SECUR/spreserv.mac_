  elif( FI_IsShare( Data.FD.Fininstr ) or FI_IsInvestmentShare(Data.FD.Fininstr) ) /* ПКУ со статусом Поставлен и ц/б - Акция и паи тоже*/
     /*Группа ПКУ*/
