PRIVATE VAR rsvprm    = TRecHandler( "rsvprm" );
PRIVATE VAR InAcc     = TRecHandler( "account" );      /*счет вложений*/
PRIVATE VAR ReservAcc = TRecHandler( "account" );  /*счет резерва*/
