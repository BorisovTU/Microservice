    QueryData = TRsbDataSet(cmd);
//    var est_res_prc = double( readNoteForObject( Data.ObjKind, UniID( Data.ReservObj, Data.ObjKind), 24, Data.DateReserv ) );    //24-коэф. оценочного резерва

    while( QueryData.MoveNext() )
