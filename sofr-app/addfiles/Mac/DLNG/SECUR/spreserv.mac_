       end;
       ArrFIIDNumAccountBPP.AddAccount(ArrFIID(j),KINDPORT_TRADE,InAcc.rec.Account,-Rest);
       FindAccFlag = true;
     end;
    
     if( DefineAcc(CategName[BPP],GetFIRoleByPortfolio( KINDPORT_SALE )) )
