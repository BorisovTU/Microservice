    cmd_InsTmp = DL_RSDCommand( " begin \n RSB_PMWRTOFF.RSI_WRTEstReserveLotsTMP(?, ?, ?); \n end;" );
    cmd_InsTmp.AddParam(Data.DateReserv);
    cmd_InsTmp.AddParam(Data.ReservObj.rec.FIID);
    cmd_InsTmp.AddParam(Data.ScOprServDoc.Division);
    DataSet_InsTmp = cmd_InsTmp.Execute();
*/
