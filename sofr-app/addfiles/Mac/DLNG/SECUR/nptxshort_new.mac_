            +                                                                 string(TXOBJ_BASEG9) + ") "
            +           "                              AND obj.t_Date BETWEEN " + GetSQLDate(m_BeginDate) + " AND " + GetSQLDate(m_EndDate)
            +           "                ) "
/*          +           "             OR EXISTS(SELECT 1 val"
            +           "                     FROM dnptxobj_dbt nptxobj,                    "
            +           "                          dnptxobdc_dbt lnk,                       "
            +           "                          doproper_dbt oper,                       "
            +           "                          dparty_dbt party                         "
