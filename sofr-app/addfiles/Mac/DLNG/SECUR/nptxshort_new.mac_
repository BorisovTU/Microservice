  MACRO Index_618()
    var Plusi;
    var sum1 = $0;
    var sum2 = $0;
    var i = TXOBJ_PLUS3;

    while(i <= TXOBJ_PLUS6)
        DL_GetNPTXOBJSumByClient(m_CurrInvestor,
                                 string(i),
                                 m_BeginDate,
                                 m_EndDate,
                                 TXOBJ_DIR_ALL,
                                 @Plusi, null, true
                                );

        sum1 = sum1 + Plusi * (i - TXOBJ_PLUS3 + 3);
        sum2 = sum2 + Plusi;

        i = i + 1;
    end;

    if(sum2 != 0)
        return sum1 / sum2;
    end;
    return 0;
  END;

