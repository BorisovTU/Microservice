      m_BaseMaterial, m_BaseSpecial, m_BaseSpecialDiv, m_DivPaySec, m_DivPaySec_1, m_BG, m_TaxRateGeneral, m_TaxDueNow, m_PS, m_PD,
      m_PG,m_SumOutNow, m_SheetName, m_query, m_count, m_Qnty, m_FRrub, m_TaxLink, m_BaseDiv, m_BaseBill, m_PaidBill, m_Base0, m_DivPay_Sec, m_MinusG_618, m_Limit_618, m_Plusi, m_FR_Sec_3Y,
      m_DivPay_Sec_0, m_PaidGeneral_0, m_PaidSpecial_0, m_Paid35_0,
      m_TaxDueNow_15, m_CoupRub_DEPO, m_TaxFR_OTC_DEPO, m_TaxFR_market_DEPO, m_BaseG5_DEPO, m_ComDepo_DEPO, m_CoupRubS_DEPO,
      m_BG_DEPO, m_BP, m_PP, m_BP_DEPO, m_BaseG5, m_TaxBase_618, m_TaxBase_618_DEPO, m_Minus_618_DEPO, m_Minus_618, m_Limit_618_DEPO,
      m_TaxBase_13, m_TaxBase_13_DEPO, m_TaxBase_15, m_TaxBase_15_DEPO, m_BaseDiv_DEPO, m_PG_DEPO, m_PGd, m_PGd_DEPO,
      m_PP_DEPO, m_PPd, m_PPd_DEPO, m_BaseG1_13, m_BaseSpecial_Div, m_BaseG1_15, m_BaseSpecial_Div_15, m_TaxDueNow_sum, m_TaxDueNow_15_sum;

  VAR m_CurrInvestorSheetName, m_CurrInvestor, m_CurrInvestorName, m_CurrIIS;
  
