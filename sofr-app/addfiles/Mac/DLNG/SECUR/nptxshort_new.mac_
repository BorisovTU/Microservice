  /*Сумма налога по общей ставке, подлежащая перечислению в бюджет налоговым агентом*/
  MACRO TaxDueNow(forSum:bool)
    var tmp_sum = iif(forSum, m_TaxDueNow_sum, m_TaxDueNow);

    if( tmp_sum == NULL )
      tmp_sum = $0;

      if(this.ReportType() != DL_TXHOLD_OPTYPE_LUCRE)
        
        if((this.ReportType() == DL_TXHOLD_OPTYPE_CLOSE) OR (this.ReportType() == DL_TXHOLD_OPTYPE_ENDYEAR))
          tmp_sum = iif(forSum, this.TaxDueNow1_sum, this.TaxDueNow1(false));
        else
          var cmd = DL_RSDCommand(  " select 1 "
                                  + "   from dnptxop_dbt "
                                  + "  where t_DocKind IN (" + DL_HOLDNDFL + ", " + DL_WRTMONEY + ")"
                                  + "    and t_Client = ? "
                                  + "    and t_OperDate = ? "
                                  + IIF(m_CurrIIS == 1, " and t_IIS = 'X' ", " and t_IIS <> 'X' ")
                                  + "    and t_SubKind_Operation = " + DL_TXHOLD_OPTYPE_OUTMONEY
                                 );

          cmd.AddParam(m_CurrInvestor);
          cmd.AddParam(this.Dend());
          if(cmd.GetCount() > 0)
            if (forSum)
              if (this.TaxDueNow1_all_sum > this.SumOutNow)
                tmp_sum = this.TaxDueNow1_sum;
              else
                tmp_sum = this.TaxDueNow1_all_sum;
              end;
            else
              if (this.TaxDueNow1_all(false) > this.SumOutNow)
                tmp_sum = this.TaxDueNow1(false);
              else
                tmp_sum = this.TaxDueNow1_all(false);
              end;
            end;
          end;
        end;
      end;
    end;

    tmp_sum = round(tmp_sum,0);

    return iifset(forSum, @m_TaxDueNow_sum, @m_TaxDueNow, tmp_sum);
  END;

