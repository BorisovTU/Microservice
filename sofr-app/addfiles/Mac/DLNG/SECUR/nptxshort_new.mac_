  /*Количество, проданное в этой сделке продажи, из этой покупки*/
  MACRO Qnty(IsMaterial)
    if(m_Qnty == null)
      if(IsMaterial == NULL)
         IsMaterial = false;
      end;

      if(IsMaterial)
         m_Qnty = this.Qnty();
      else
         m_Qnty = m_RS.Qnty;
      end;
    end;
    return m_Qnty;
  END;

  MACRO ЭтоВиртПокупка()
     return (m_RS.BuyIsVirtual == 1);
