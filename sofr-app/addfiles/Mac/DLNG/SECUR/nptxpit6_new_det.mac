   /**
     @brief Получить количество записей для раздела 1
     @return Количество записей
   */
   macro Count():Integer
      var queryCount = "select count(1) as t_Count " +
                       "from dnptx6calc2_tmp ";

      var cmdCount = DL_RSDCommand(queryCount);
      var dsCount = cmdCount.Execute();
