import "sp_categ.mac";

macro ExecuteStep( Doc, FDoc )
  record deal(dl_tick);       
  var    FD, error; 
  var    dat;

  SetBuff( deal, FDoc ); 
  FD = SPFirstDoc( deal, false );

  if( FD.ЕстьНеиспТО() )
     MsgBox("Обработаны не все ТО по операции");
     return 1;
  end;

  return 0;
end;
