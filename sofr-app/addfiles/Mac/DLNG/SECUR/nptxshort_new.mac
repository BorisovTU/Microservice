                  + "    AND obj.t_Kind = " + TXOBJ_NKDB_TS
                  + "    AND obj.t_AnaliticKind2 = " + TXOBJ_KIND2030
                  + "    AND obj.t_Analitic2 = rc.t_CouponNumber "
