  MACRO TaxDueNow(forSum:bool)
    var tmp_sum = iif(forSum, m_TaxDueNow_sum, m_TaxDueNow);

    if( tmp_sum == NULL )
      tmp_sum = $0;

      if(this.ReportType() != DL_TXHOLD_OPTYPE_LUCRE)
        
        if((this.ReportType() == DL_TXHOLD_OPTYPE_CLOSE) OR (this.ReportType() == DL_TXHOLD_OPTYPE_ENDYEAR))
          tmp_sum = iif(forSum, this.TaxDueNow1_sum, this.TaxDueNow1(false));
        else
          var cmd = DL_RSDCommand(  " select 1 "
                                  + "   from dnptxop_dbt "
                                  + "  where t_DocKind IN (" + DL_HOLDNDFL + ", " + DL_WRTMONEY + ")"
                                  + "    and t_Client = ? "
                                  + "    and t_OperDate = ? "
                                  + IIF(m_CurrIIS == 1, " and t_IIS = 'X' ", " and t_IIS <> 'X' ")
                                  + "    and t_SubKind_Operation = " + DL_TXHOLD_OPTYPE_OUTMONEY
                                 );

          cmd.AddParam(m_CurrInvestor);
          cmd.AddParam(this.RepDend());
          if(cmd.GetCount() > 0)
            if (forSum)
              if (this.TaxDueNow1_all_sum > this.SumOutNow)
                tmp_sum = this.TaxDueNow1_sum;
              else
                tmp_sum = this.TaxDueNow1_all_sum;
              end;
            else
              if (this.TaxDueNow1_all(false) > this.SumOutNow)
                tmp_sum = this.TaxDueNow1(false);
              else
                tmp_sum = this.TaxDueNow1_all(false);
              end;
            end;
  END;
        end;
      end;
    end;

    tmp_sum = round(tmp_sum,0);

    return iifset(forSum, @m_TaxDueNow_sum, @m_TaxDueNow, tmp_sum);
  END;

  MACRO TaxDueNow_15(forSum:bool)
    var tmp_sum = iif(forSum, m_TaxDueNow_15_sum, m_TaxDueNow_15);

    if( tmp_sum == NULL )
      tmp_sum = $0;

      if((this.ReportType() == DL_TXHOLD_OPTYPE_CLOSE) OR (this.ReportType() == DL_TXHOLD_OPTYPE_ENDYEAR))
          tmp_sum = iif(forSum, this.TaxDueNow1_15_sum, this.TaxDueNow1_15(false));
      elif((this.ReportType() == DL_TXHOLD_OPTYPE_OUTMONEY) OR (this.ReportType() == DL_TXHOLD_OPTYPE_OUTAVOIR))
        var cmd = DL_RSDCommand(  " select 1 "
                                + "   from dnptxop_dbt "
                                + "  where t_DocKind IN (" + DL_HOLDNDFL + ", " + DL_WRTMONEY + ")"
                                + "    and t_Client = ? "
                                + "    and t_OperDate = ? "
                                + IIF(m_CurrIIS == 1, " and t_IIS = 'X' ", " and t_IIS <> 'X' ")
                                + "    and t_SubKind_Operation = " + DL_TXHOLD_OPTYPE_OUTMONEY
                               );

        cmd.AddParam(m_CurrInvestor);
        cmd.AddParam(this.RepDend());
        if(cmd.GetCount() > 0)
          if(cmd.GetCount() > 0)
            if (forSum)
              if (this.TaxDueNow1_all_sum > this.SumOutNow)
                tmp_sum = this.TaxDueNow1_15_sum;
              else
                tmp_sum = $0;
              end;
            else
              if (this.TaxDueNow1_all(false) > this.SumOutNow)
                tmp_sum = this.TaxDueNow1_15(false);
              else
                tmp_sum = $0;
              end;
        end;
      end;
    end;
      end;
    end;

    tmp_sum = round(tmp_sum,0);

    return iifset(forSum, @m_TaxDueNow_15_sum, @m_TaxDueNow_15, tmp_sum);
  END;

  /*Сумма денежных средств, уплаченная клиенту */
  MACRO SumOutAfterDeduction()

   if((this.ReportType() != DL_TXHOLD_OPTYPE_LUCRE) and (this.SumOutNow() > 0))
     return this.SumOutNow() - this.TaxDueNow_all();
   end;
   return 0;
  END;

  MACRO SumOutAfterDeduction_sum()

   if((this.ReportType() != DL_TXHOLD_OPTYPE_LUCRE) and (this.SumOutNow() > 0))
     return this.SumOutNow() - this.TaxDueNow_all_sum();
   end;
   return 0;
  END;

  //Старт
  PRIVATE MACRO Create()
    var query, DataSet, cmd;
    var iter = 1;
    var stat = 0;
    var InvestorSheetName, Year = 0;
    var cnt = 0, i = 0;
    var Sym = "";
    var Sym_ = "";
    var OldSym = "";
    var PoiNum = 0;
    var isFirst = true;

    m_BeginDate = this.RepDbeg();
    m_EndDate   = this.RepDend();
    var Investor = m_Form.GetFieldValue( PNFLD_NPTXSHORT_INVESTORCODE );

    if(m_EndDate == date(31,12,2022))
      datesplit(m_EndDate, null, null, Year);
    end;

    cmd = DL_RSDCommand();

    query =   " SELECT q.InvestorName, q.Investor, q.KindIIS, q.InvestorSheetName "
            + "   FROM ("
            +           " SELECT pt.t_PartyID as Investor, 0 as KindIIS, "
            +           "        trim( pt.t_Name ) as InvestorName, "
            +           "        ((CASE WHEN ( (SELECT LENGTH (trim(persn.t_Name1)) FROM DUAL) > 20) "
            +           "                    THEN  SUBSTR (trim(persn.t_Name1), 1, 20) " 
            +           "               ELSE trim(persn.t_Name1) END) " 
            +           "         || case when(persn.t_Name2 = CHR(1))then '' else SUBSTR (persn.t_Name2, 1, 1) end"
            +           "         || case when(persn.t_Name3 = CHR(1))then '' else SUBSTR (persn.t_Name3, 1, 1) end) as InvestorSheetName "
            +           "   FROM dpersn_dbt persn, dparty_dbt pt "
            +           "  WHERE     pt.t_LegalForm   = " + string(legal_form_phys)
            +           "        AND persn.t_PersonID = pt.t_PartyID "
            +           "        AND (   EXISTS(SELECT 1"
            +           "                         FROM dclient_dbt cl, dsfcontr_dbt sf"
            +           "                        WHERE     cl.t_PartyID   =  pt.t_PartyID"
            +           "                              AND cl.t_ServiceKind IN (" + string(PTSK_STOCKDL) + ", "
            +                                                                       string(PTSK_DV) + ", "
            +                                                                       string(PTSK_DEPOS) + ", "
            +                                                                       string(PTSK_SVO) + ") "
            +           "                              AND sf.t_PartyID = cl.t_PartyID "
            +           "                              AND sf.t_ServKind = cl.t_ServiceKind "
            +           "                              AND rsi_npto.CheckContrIIS(sf.t_ID) = 0 "
            +           "                           ) "
            +           "             OR EXISTS(SELECT 1"
            +           "                         FROM dnptxobj_dbt obj"
            +           "                        WHERE     obj.t_Client = pt.t_PartyID "
            +           "                              AND obj.t_Kind IN (" + string(TXOBJ_BASESPECIAL) + ", "
            +                                                                 string(TXOBJ_BASEBILL) + ", "
            +                                                                 string(TXOBJ_BASEG1) + ", "
            +                                                                 string(TXOBJ_BASEG2) + ", "
            +                                                                 string(TXOBJ_BASEG3) + ", "
            +                                                                 string(TXOBJ_BASEG9) + ") "
            +           "                              AND obj.t_Date BETWEEN ? AND ? "
            +           "                ) "
            +           "             OR EXISTS(SELECT 1"
            +           "                         FROM dnptxdias_dbt ds"
            +           "                        WHERE     ds.t_ClientID = pt.t_PartyID "
            +           "                              AND ds.t_IIS <> 'X' "
            +           "                              AND ds.t_Year = ? " 
            +           "                ) "
            +           "        ) ";

    cmd.AddParam(m_BeginDate);
    cmd.AddParam(m_EndDate);
    cmd.AddParam(Year);

    if(((Investor != null) AND (Investor > 0)))
      query = query + " AND pt.t_PartyID = ? ";
      cmd.AddParam(IIF(Investor!=null,Investor,-1));
    end;

    query = query        
            + "  UNION ALL "
            +           " SELECT pt.t_PartyID as Investor, 1 as KindIIS, "
            +           "        trim( pt.t_Name ) as InvestorName, "
            +           "        'ИИС_'|| ((CASE WHEN ( (SELECT LENGTH (trim(persn.t_Name1)) FROM DUAL) > 20) "
            +           "                    THEN  SUBSTR (trim(persn.t_Name1), 1, 20) " 
            +           "               ELSE trim(persn.t_Name1) END) " 
            +           "         || case when(persn.t_Name2 = CHR(1))then '' else SUBSTR (persn.t_Name2, 1, 1) end"
            +           "         || case when(persn.t_Name3 = CHR(1))then '' else SUBSTR (persn.t_Name3, 1, 1) end) as InvestorSheetName "
            +           "   FROM dpersn_dbt persn, dparty_dbt pt"
            +           "  WHERE     pt.t_LegalForm   = " + string(legal_form_phys)
            +           "        AND persn.t_PersonID = pt.t_PartyID"
            +           "        AND (   EXISTS(SELECT 1"
            +           "                         FROM dclient_dbt cl, dsfcontr_dbt sf "
            +           "                        WHERE     cl.t_PartyID   = pt.t_PartyID"
            +           "                              AND cl.t_ServiceKind IN (" + string(PTSK_STOCKDL) + ", "
            +                                                                       string(PTSK_DV) + ", "
            +                                                                       string(PTSK_DEPOS) + ", "
            +                                                                       string(PTSK_SVO) + ") "
            +           "                              AND sf.t_PartyID = cl.t_PartyID "
            +           "                              AND sf.t_ServKind = cl.t_ServiceKind "
            +           "                              AND rsi_npto.CheckContrIIS(sf.t_ID) <> 0 "
            +           "                      )"
            +           "             OR EXISTS(SELECT 1"
            +           "                         FROM dnptxobj_dbt obj"
            +           "                        WHERE     obj.t_Client = pt.t_PartyID"
            +           "                              AND obj.t_Kind IN (" + string(TXOBJ_BASESPECIAL_IIS) + ", "
            +                                                                 string(TXOBJ_BASEGENERAL_IIS) + ", "
            +                                                                 string(TXOBJ_BASEG5) + ") "
            +           "                              AND obj.t_Date BETWEEN NVL( RSI_NPTO.GetFirstDateIIS (pt.t_PartyId), ? ) AND ? " 
            +           "              ) "
            +           "            )";

    cmd.AddParam(m_BeginDate);
    cmd.AddParam(m_EndDate);

    if(((Investor != null) AND (Investor > 0)))
      query = query + " AND pt.t_PartyID = ? ";
      cmd.AddParam(IIF(Investor!=null,Investor,-1));
    end;

    query = query
            + "        ) q "
            + " ORDER BY q.InvestorName, q.Investor ASC, q.KindIIS";
    
    cnt = cmd.GetCount(query);
    if(cnt > 0)
      InitProgress( cnt, "Краткая справка по расчету НДФЛ", "Краткая справка по расчету НДФЛ" );

      DataSet = cmd.Execute(query);
      stat = DataSet.moveNext();
      while(stat)

        m_CurrIIS = int(DataSet.KindIIS);
        m_CurrInvestor = int(DataSet.Investor);
        m_CurrInvestorName = string(DataSet.InvestorName);
        InvestorSheetName = string(DataSet.InvestorSheetName);

        TXOBJ_BASEG_2_3_9.size = 0;
        if(this.ClientStatus() == CL_STATE_RES)
          TXOBJ_BASEG_2_3_9(0) = TXOBJ_BASEG2;
          TXOBJ_BASEG_2_3_9(1) = TXOBJ_BASEG3;
          TXOBJ_BASEG_2_3_9(2) = TXOBJ_BASEG9;
        else
          TXOBJ_BASEG_2_3_9(0) = TXOBJ_BASEGENERAL;
          TXOBJ_BASEG_2_3_9(1) = TXOBJ_BASEBILL;
        end;

        DateSplit( this.RepDend(), null, null, Year );
        if ((DataSet.KindIIS == 1) and (m_Form.GetFieldValue(PNFLD_NPTXSHORT_TYPE) == 50))
          m_BeginDate = GetFirstDateIIS(int(DataSet.Investor));
        else
          m_BeginDate = Date(1,1,Year);
        end;

        m_CurrInvestorSheetName = string(InvestorSheetName);
        
        stat = DataSet.moveNext();
        if(stat and (InvestorSheetName == string(DataSet.InvestorSheetName))) //инициалы инвесторов совпадают
          m_CurrInvestorSheetName = trim(InvestorSheetName) + string (iter);
          iter = iter + 1;
        else
          if(iter > 1)
            m_CurrInvestorSheetName = trim(InvestorSheetName) + string (iter);
          end;
          iter = 1;
        end;

        //Общий расчет
        CreateTotalCalc();
        PoiNum = PoiNum + 1;

        if(SET_CHAR == m_Form.GetFieldValue(PNFLD_NPTXSHORT_REALIZ))
          if(CreateRealizRep() == true)
            PoiNum = PoiNum + 1;
          end;
        end;

        if(SET_CHAR == m_Form.GetFieldValue(PNFLD_NPTXSHORT_SHORTPOS))
          if(CreateShortPosRep() == true)
            PoiNum = PoiNum + 1;
          end;
        end;
        
        if(SET_CHAR == m_Form.GetFieldValue(PNFLD_NPTXSHORT_REPO))
          if(CreateRepoRep() == true)
            PoiNum = PoiNum + 1;
          end;
        end;

        if((m_Form.GetFieldValue(PNFLD_NPTXSHORT_TYPE) == 10) or (m_Form.GetFieldValue(PNFLD_NPTXSHORT_TYPE) == 50) or (m_Form.GetFieldValue(PNFLD_NPTXSHORT_TYPE) == 20))
          if(CerateInvestDedRep() == true)
            PoiNum = PoiNum + 1;
          end;
        end;

        if(SET_CHAR == m_Form.GetFieldValue(PNFLD_NPTXSHORT_LUCRE))
          if(CreateReportNPTXLucre() == true)
            PoiNum = PoiNum + 1;
          end;
        end;

        if(SET_CHAR == m_Form.GetFieldValue(PNFLD_NPTXSHORT_PFI))
            if(CreateReportPFIExchange() == true)
              PoiNum = PoiNUm + 1;
            end;
            if(CreateReportPFIOutExchange() == true)
              PoiNum = PoiNum + 2;
            end;
        end;

        if(POI_MODE)
            if(isFirst)
                Sym = substr(m_CurrInvestorName, 1, 3);
            end;
            Sym_ = substr(m_CurrInvestorName, 1, 3);
            ItogSym = Sym + "-" + Sym_;
            ItogSym = GetUniqAdd( ItogSym );
            if(PoiNum >= POI_SIZE_PACK)
                SaveSubTotalBook(ItogSym, false);
                CreateTotalBook();
                PoiNum = 1;
                isFirst = true;
            else
                isFirst = false;
            end;
        end;

        UseProgress(i = i + 1);
      end;

      RemProgress();
    else
      SetActiveSheet( "Справка по расчету НДФЛ" );  
      PrintFormatString( NULL,"N1_NoRepData","Нет данных, удовлетворяющих параметрам отчета");
      CopyAllSheetInTotalBook( "Справка по расчету НДФЛ", false, "N1_NoRepData", 0 );
    end;

  END;

 
