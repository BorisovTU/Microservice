                  + "    0 AS ValS, "
                  + "    0 AS SalePrice, "
                  + "    0 AS LegKindSale, "
                  + "    0 AS SPoint, "
                  + "    0 AS SPrincipal,    "
                  + "    0 AS VpS, "
                  + "    0 AS DealSaleID, "
