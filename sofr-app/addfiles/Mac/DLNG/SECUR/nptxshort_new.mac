                 /*22 */   "N2_ComExcB",     
                 /*23 */   "N2_ComB",        
                 /*24 */   "N2_TaxFRlink",   
                 /*25 */   "N2_SecType",     
                 /*26 */   "N2_TaxTags",     
                 /*27 */   "N2_AA"      
                 );
