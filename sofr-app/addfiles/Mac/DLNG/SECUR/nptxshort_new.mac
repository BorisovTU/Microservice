  PRIVATE MACRO GetTaxPaidBySNOB(Rate:integer, forDEPO:bool)
    var query, cmd, DataSet;
    var TaxPaid = $0;
    var TaxBaseKind = "";
    var Year;

    datesplit(m_EndDate, NULL, NULL, Year);

    if(forDEPO == true)
      if(SET_CHAR == m_Form.GetFieldValue(PNFLD_NPTXSHORT_BYDEPO))
        if(m_CurrIIS != 0)
          TaxBaseKind = string(NPTXTOTALBASE_TAXBASEKIND_IIS);
        else
          if(РАБОТА_В_РЕЖИМЕ_СНОБ() == true)
            TaxBaseKind = NPTXTOTALBASE_TAXBASEKIND_ONB+","+NPTXTOTALBASE_TAXBASEKIND_BROK+", 0";
          else
            TaxBaseKind = string(NPTXTOTALBASE_TAXBASEKIND_BROK);
          end;
        end;

        cmd = DL_RSDCommand();

        query =   " select NVL(SUM(t_NdflKeepAmount), 0) as SumNdflKeepAmount "
                + "   from dnptxtbext_dbt "
                + "  where t_ClientID = ? "
                + "    and t_TaxBaseKind IN ("+TaxBaseKind+")"
                + "    and t_NdflKeepRate = ? ";

        cmd.AddParam(m_CurrInvestor);
        cmd.AddParam(Rate);

        if(this.ReportType() == DL_TXHOLD_OPTYPE_ENDYEAR)
          query = query + " and t_TaxPeriod = ? ";
          cmd.AddParam(Year);
        else
          query = query + " and t_IncDate BETWEEN ? and ? "
                        + " and t_TaxPeriod = ? ";
        
          cmd.AddParam(m_BeginDate);
          cmd.AddParam(m_EndDate);
          cmd.AddParam(Year);
        end;

        DataSet = cmd.Execute(query);
        if(DataSet.moveNext())
          TaxPaid = DataSet.SumNdflKeepAmount;
        end;

        if(this.ReportType() == DL_TXHOLD_OPTYPE_ENDYEAR)
          cmd = DL_RSDCommand();

          query =   " select NVL(SUM(t_HoldPITax), 0) as SumHoldPITax "
                  + "   from dnptxtotalbase_dbt "
                  + "  where t_ClientID = ? "
                  + "    and t_StorState = " + NPTXTOTALBASE_STORSTATE_ACTIVE
                  + "    and t_TaxBaseKind IN ("+TaxBaseKind+")"
                  + "    and t_RateHoldPITax = ? "
                  + "    and instr(UPPER(t_Description), UPPER('Диасофт')) <> 0"
                  + "    and t_TaxPeriod = ? ";

          cmd.AddParam(m_CurrInvestor);
          cmd.AddParam(Rate);
          cmd.AddParam(Year);

          DataSet = cmd.Execute(query);
          if(DataSet.moveNext())
            TaxPaid = TaxPaid + DataSet.SumHoldPITax;
          end;
        end;
      end;
    else
      if(m_CurrIIS != 0)
        TaxBaseKind = string(NPTXTOTALBASE_TAXBASEKIND_IIS);
      else
        if(РАБОТА_В_РЕЖИМЕ_СНОБ() == true)
          TaxBaseKind = NPTXTOTALBASE_TAXBASEKIND_ONB+","+NPTXTOTALBASE_TAXBASEKIND_BROK+", 0";
        else
          TaxBaseKind = string(NPTXTOTALBASE_TAXBASEKIND_BROK);
        end;
      end;

      cmd = DL_RSDCommand();

      query =   " select NVL(SUM(t_HoldPITax), 0) as SumHoldPITax "
              + "   from dnptxtotalbase_dbt "
              + "  where t_ClientID = ? "
              + "    and t_StorState = " + NPTXTOTALBASE_STORSTATE_ACTIVE
              + "    and t_TaxBaseKind IN ("+TaxBaseKind+")"
              + "    and t_RateHoldPITax = ? "
              + "    and instr(UPPER(t_Description), UPPER('Диасофт')) = 0";

      cmd.AddParam(m_CurrInvestor);
      cmd.AddParam(Rate);

      if(this.ReportType() == DL_TXHOLD_OPTYPE_ENDYEAR)
        query = query + " and t_TaxPeriod = ? ";
        cmd.AddParam(Year);
      else
        query = query + " and t_IncDate BETWEEN ? and ? "
                      + " and t_TaxPeriod = ? ";
      
        cmd.AddParam(m_BeginDate);
        cmd.AddParam(m_EndDate);
        cmd.AddParam(Year);
      end;

      DataSet = cmd.Execute(query);
      if(DataSet.moveNext())
        TaxPaid = DataSet.SumHoldPITax;
      end;
    end;

    return TaxPaid;
  END;

