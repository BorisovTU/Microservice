  PRIVATE MACRO GetAmountSNOBfromStor()
     var llv = TRecHandler("llvalues.dbt");
     var Request = c_GetAmountSNOBRequest();
     var Year;
     
     datesplit(m_EndDate, null, null, Year);

     Request.GetAmountSNOBReq = c_GetAmountSNOBReq();
     Request.GetAmountSNOBReq.IsActivRecord      = true;
     Request.GetAmountSNOBReq.IsSNOB             = false;
     Request.GetAmountSNOBReq.ClientId           = c_IntegrationSymbolicIdentifierXType();
     Request.GetAmountSNOBReq.ClientId.ObjectID  = m_CurrInvestor;
     Request.GetAmountSNOBReq.TaxYear            = Year;

     if((m_CurrIIS != 0) or (РАБОТА_В_РЕЖИМЕ_СНОБ() == false))
       var TaxBaseKind = 0;
       
       if(m_CurrIIS != 0)
         TaxBaseKind = NPTXTOTALBASE_TAXBASEKIND_IIS;
       elif(РАБОТА_В_РЕЖИМЕ_СНОБ() == false)
         TaxBaseKind = NPTXTOTALBASE_TAXBASEKIND_BROK;
       end;
       
       if((TaxBaseKind > 0) and (LL_FindLLVALUES(OBJTYPE_STB_TAXBASEKIND, TaxBaseKind, llv) == true) )
         Request.GetAmountSNOBReq.TaxBaseType = c_IntegrationDictionaryRecordXType();    // Вид налоговой базы по НДФЛ, к которой относится событие        Необязательное
         Request.GetAmountSNOBReq.TaxBaseType.RecordCode = llv.rec.Flag;
       end;
     end;

     var Resp = GetAmountSNOBReq(Request);
     
     var isOk = true;
     if((Resp == NULL) or (ValType(Resp) != V_GENOBJ) or (ValType(Resp.GetAmount) == V_UNDEF) or (ValType(Resp.GetAmount.RecordInfoList) == V_UNDEF) or
        ((ValType(Resp.ErrorList) != V_UNDEF) and (Resp.ErrorList.size > 0) and (string(trim(Resp.ErrorList[0].ErrorCode))) != "0")
       )
       isOk = false;
     end;

     if(isOk)
       if(Resp.GetAmount.RecordInfoList.Size > 0)
         var i = 0;
         while(i < Resp.GetAmount.RecordInfoList.Size)
           var IIS = 0;
           if(Resp.GetAmount.RecordInfoList[i].TaxBaseType.RecordCode == "6")
             IIS = 1;
           end;

           m_StorCollect.Add(IIF(StrLwr(Resp.GetAmount.RecordInfoList[i].SystemCode) == "diasoft", true, false), 
                             IIS, 
                             Resp.GetAmount.RecordInfoList[i].NDFLTaxRate, 
                             Resp.GetAmount.RecordInfoList[i].NOBAmount, 
                             Resp.GetAmount.RecordInfoList[i].NDFLKeepAmount);

           i = i + 1;
         end;
       end;
     else
       m_StorCollect.Init();
     end;
  END;

  PRIVATE MACRO GetTaxBaseBySNOB(Rate:integer, forDEPO:bool)
    var query, cmd, DataSet;
    var TaxBase = $0;
    var TaxBaseKind = "";
    
    if(forDEPO == true)
      if(SET_CHAR == m_Form.GetFieldValue(PNFLD_NPTXSHORT_BYDEPO))
        if(m_CurrIIS != 0)
          TaxBaseKind = string(NPTXTOTALBASE_TAXBASEKIND_IIS);
        else
          if(РАБОТА_В_РЕЖИМЕ_СНОБ() == true)
            TaxBaseKind = NPTXTOTALBASE_TAXBASEKIND_ONB+","+NPTXTOTALBASE_TAXBASEKIND_BROK+", 0";
          else
            TaxBaseKind = string(NPTXTOTALBASE_TAXBASEKIND_BROK);
          end;
        end;

        query =   " select NVL(SUM(t_NobAmount), 0) as SumNobAmount "
                + "   from dnptxtbext_dbt "
                + "  where t_ClientID = ? "
                + "    and t_TaxBaseKind IN ("+TaxBaseKind+")"
                + "    and t_NdflCalcRate = ? "
                + "    and t_IncDate BETWEEN ? and ? ";

        cmd = DL_RSDCommand(query);

        cmd.AddParam(m_CurrInvestor);
        cmd.AddParam(Rate);
        cmd.AddParam(m_BeginDate);
        cmd.AddParam(m_EndDate);

        DataSet = cmd.Execute();
        if(DataSet.moveNext())
          TaxBase = DataSet.SumNobAmount;
        end;
      end;
    else
      if(m_CurrIIS != 0)
        TaxBaseKind = string(NPTXTOTALBASE_TAXBASEKIND_IIS);
      else
        if(РАБОТА_В_РЕЖИМЕ_СНОБ() == true)
          TaxBaseKind = NPTXTOTALBASE_TAXBASEKIND_ONB+","+NPTXTOTALBASE_TAXBASEKIND_BROK+", 0";
        else
          TaxBaseKind = string(NPTXTOTALBASE_TAXBASEKIND_BROK);
        end;
      end;

      query =   " select NVL(SUM(t_TaxBaseCurrPay), 0) as SumTaxBaseCurrPay "
              + "   from dnptxtotalbase_dbt "
              + "  where t_ClientID = ? "
              + "    and t_StorState = " + NPTXTOTALBASE_STORSTATE_ACTIVE
              + "    and t_Type = 1 " //Тип события = Выплата дохода
              + "    and t_TaxBaseKind IN ("+TaxBaseKind+")"
              + "    and t_RateCalcPITax = ? "
              + "    and t_IncDate BETWEEN ? and ? ";

      cmd = DL_RSDCommand(query);

      cmd.AddParam(m_CurrInvestor);
      cmd.AddParam(Rate);
      cmd.AddParam(m_BeginDate);
      cmd.AddParam(m_EndDate);

      DataSet = cmd.Execute();
      if(DataSet.moveNext())
        TaxBase = DataSet.SumTaxBaseCurrPay;
      end;
    end;

    return TaxBase;
  END;

  MACRO TaxBase_13(forDEPO:bool)
    var TaxBase_13 = $0;

    if((m_EndDate == date(31,12,2022)) and (m_CurrIIS == 0))
      ЗагрузитьЗначенияДЕПО_СОФР_2022();
      if(forDEPO)
        TaxBase_13 = НОБ_ДЕПО;
      else
        TaxBase_13 = НОБ_СОФР;
      end;
    elif(m_EndDate >= GetStartSTBDate())
      TaxBase_13 = GetTaxBaseBySNOB(100*IIF(this.ClientStatus() == CL_STATE_RES, NPTXGENTAXRATE_RESIDENT, NPTXGENTAXRATE_NOTRESIDENT), forDEPO);
    else
      if (this.ClientStatus() == CL_STATE_RES)
        if (forDEPO)
          TaxBase_13 = Min(Max15, this.BG(true) + this.BP(true));
        else
          if(m_CurrIIS == 0)
            TaxBase_13 = Min(Max15, this.BG(true) + this.B2(false)) - this.BG(true) + this.B3(false) + this.B9(false);
          else
            TaxBase_13 = Min(Max15, this.BG(true) + this.BaseG5(false)) - this.BG(true);
          end;
        end;
      else
        TaxBase_13 = this.BG(forDEPO);
      end;
    end;

    return TaxBase_13;
  END;

  MACRO TaxBase_15(forDEPO:bool)
    var TaxBase_15 = $0;

    if((m_EndDate == date(31,12,2022)) and (m_CurrIIS == 0))
      ЗагрузитьЗначенияДЕПО_СОФР_2022();
      if(forDEPO)
        TaxBase_15 = НОБ_ДЕПО_КБК3;
      else
        TaxBase_15 = НОБ15_СОФР;
      end;
    elif(m_EndDate >= GetStartSTBDate())
      TaxBase_15 = GetTaxBaseBySNOB(100*NPTXGENTAXRATE_RESIDENT_15, forDEPO);
    else
      if (this.ClientStatus() == CL_STATE_RES)
        if (forDEPO)
          TaxBase_15 = Max(this.BG(true) + this.BP(true) - Max15, $0);
        else
          TaxBase_15 = this.BG(false) + this.BP(false) - this.TaxBase_13(false);
        end;
      end;
    end;

    return TaxBase_15;
  END;

