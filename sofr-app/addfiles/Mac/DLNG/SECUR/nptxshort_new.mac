private macro GetSystDateTime(SystDate:@date, SystTime:@time)
  var cmd = DL_RSDCommand("select sysdate dt from dual");
  var DataSet = cmd.execute();

  DataSet.moveNext();

  DtTmSplit(DataSet.dt, SystDate, SystTime);
end;

PRIVATE MACRO iifset(cond:bool, var1:@variant, var2:@variant, val)
  if (cond)
    return var1 = val;
  else
    return var2 = val;
  end;
END;

PRIVATE CLASS StorSNOBData(_forDepo, _IIS, _Rate, _SumTaxBaseCurrPay, _SumHoldPITax)
  var forDepo           = _forDepo;
  var IIS               = _IIS;
  var Rate              = _Rate;
  var SumTaxBaseCurrPay = _SumTaxBaseCurrPay;
  var SumHoldPITax      = _SumHoldPITax;
END;

PRIVATE CLASS StorSNOBCollect()
  var m_arr = NULL;

  macro IsData()
    if(m_arr == NULL)
      return false;
    end;

    return true;
  end;

  macro Clear()
    m_arr = NULL;
  end;

  macro Init()
    m_arr = TArray();
  end;

  macro find(_forDepo, _IIS, _Rate)
    var i = -1;

    if(this.IsData() == true)
      var j = 0;
      while(j < m_arr.size)
        if((m_arr[j].forDepo == _forDepo) and (m_arr[j].IIS == _IIS) and (m_arr[j].Rate == _Rate))
          i = j;
          break;
        end;

        j = j + 1;
      end;
    end;

    return i;
  end;

  macro Add(_forDepo, _IIS, _Rate, _SumTaxBaseCurrPay, _SumHoldPITax)
    if(this.IsData() == false)
      this.Init();
    end;

    var i = this.find(_forDepo, _IIS, _Rate);
    if(i == -1)
      m_arr[m_arr.size] = StorSNOBData(_forDepo, _IIS, _Rate, _SumTaxBaseCurrPay, _SumHoldPITax);
    else
      m_arr[i].SumTaxBaseCurrPay = m_arr[i].SumTaxBaseCurrPay + _SumTaxBaseCurrPay;
      m_arr[i].SumHoldPITax = m_arr[i].SumHoldPITax + _SumHoldPITax;
    end;
  end;

  macro GetSumTaxBaseCurrPay(_forDepo, _IIS, _Rate)
     var SumTaxBaseCurrPay = 0;

     var i = this.find(_forDepo, _IIS, _Rate);
     if(i >= 0)
       SumTaxBaseCurrPay = m_arr[i].SumTaxBaseCurrPay;
     end;

     return SumTaxBaseCurrPay;
  end;

  macro GetSumHoldPITax(_forDepo, _IIS, _Rate)
     var SumHoldPITax = 0;

     var i = this.find(_forDepo, _IIS, _Rate);
     if(i >= 0)
       SumHoldPITax = m_arr[i].SumHoldPITax;
     end;

     return SumHoldPITax;
  end;

  this.Clear();

END;

