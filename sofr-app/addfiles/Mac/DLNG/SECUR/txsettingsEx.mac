// Макрос перенастройки параметров лотов НУ после изменения настроек АРНУ
import "cb_sql.mac";

macro CorrectLots(IsWebservice:Bool)
   VAR cmd;

   cmd = RsdCommand(RslDefCon, "begin\n RSB_SCTX.TXRetuningLots(1,1,1,1,1);\n end; ");
   cmd.execute();

   if(not IsWebservice)
      msgbox ("Перенастройка параметров лотов НУ выполнена успешно.");
   end;
end;
