/* Операция конвертации ц/б
   Шаг закрытия выпуска и завершения операции*/
import InsCarryDoc, "sp_categ.mac";

macro ExecuteStep( Doc, FDoc, DocKind, OperationID )

  record comm( dl_comm );
  SetBuff( comm, FDoc );

  /*Закрытие выпуска - 2-я очередь реализации.*/

  if( not InsertOprStatus( SP_OPERSTATUS_GLOBALOPER_KIND_DO, SP_OPERSTATUS_GLOBALOPER_DO_CLOSE) ) 
     msgbox( "Ошибка при установке статуса операции" );
     return 1;
  end;

  return 0;
end; 

