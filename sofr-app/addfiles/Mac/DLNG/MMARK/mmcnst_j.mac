MACRO get_expenses(DealType, OperDate)
    var
        retVal = "";

    if (IsCLAIMSACQ(GetOperationGroup(DealType)))
        retVal = get_exp_cm(OperDate);
    else
        retVal = tdr_expenses;
    end;

    return retVal;
END;

