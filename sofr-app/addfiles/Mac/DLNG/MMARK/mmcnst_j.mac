MACRO tdr_rest(FlagTypeDeal)
    if (FlagTypeDeal == 4)
        return tdr_odmainrest;
	elif (FlagTypeDeal == 6)  /*SVE DEF-22682*/
        return tdr_mainrest_tf;	
    end;
    
    return tdr_mainrest;
END;

