/*
  Номера комиссий модуля фиссико
*/
import oralib;

class DvCommissNumbers()

  macro GetNumberByCode(code:string):integer
    var query = "select t_number from dsfcomiss_dbt where t_code = :1";
    var sql = ExecSQLselectPrmDyn(query, code);
    if (sql.moveNext())
      return sql.value("t_number");
    end;
    return 0;
  end;
end;