Import globals, BankInter, CurrInter;

MACRO RestAMulty ( Счет, КодВалюты, ДатаК, ДатаН, Глава )
    if (КодВалюты==null) КодВалюты = 0; end; 
    if (ДатаК==null) ДатаК = {curdate}; end;
    if (Глава==null) Глава = 1; end;

    if (КодВалюты)
        return RestAC(Счет,КодВалюты,ДатаК,ДатаН,Глава);
    end;

    return RestA(Счет,ДатаК,ДатаН,Глава);
END;