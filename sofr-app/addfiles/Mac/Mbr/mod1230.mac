/*  Функция создания запроса выборки подтверждений для квитовки              */
/*  Используется в модуле 1230                                               */

macro СоздатьЗапрос()
 var sql:string;

 /* Основной запрос */
 sql = " SELECT DISTINCT wlconf.t_ConfID " + 
       " FROM   dwlconf_dbt wlconf " + 
       " WHERE  wlconf.t_Direct = 'X'  " + /* входящее*/
       " AND    wlconf.t_State  = 50 " ;   /*WLD_STATUS_CONF_RECEIV - принято*/
 
 return sql;
end;