import "fnsRegDecAl_AP.mac";

macro GenDoc( addrMes )
  RECORD wlmes(wlmes);
  SetBuff( wlmes, addrMes );

  PrintLog(2, "Генерация учетного объекта по сообщению APO");

  return GenRegDecisionAP(wlmes, WLD_TYPE_REGDEC_APO);

  OnError(er)
    ExeptionMessage(er);
    return FALSE;
end;
