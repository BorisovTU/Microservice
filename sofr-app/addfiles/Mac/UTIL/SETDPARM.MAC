file listfdep("listfdep.dbt") key 9999;
file depparm("DEPPARM.DBT") write;
rewind(listfdep);
while(Next(listfdep))
/*msgbox("!!!");*/
  clearrecord(depparm);
  depparm.FNcash = listfdep.FNcash;
  insert(depparm);
end; /* while */

