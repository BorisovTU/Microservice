import rsd;

private var sql, cmd;
private var p_acctrnid = 7453;

GetInt(p_acctrnid, "Идентификатор счета");

sql =       "UPDATE dpmpaym_dbt ";
sql = sql + "   SET T_PAYMSTATUS = 30 ";
sql = sql + " WHERE T_PAYMENTID = :p_acctrnid ";
cmd = RSDCommand(sql);
cmd.addParam("p_acctrnid", RSDBP_IN, p_acctrnid);
cmd.execute();

cmd.close();
cmd = Null;
sql =       "UPDATE dpmpaym_dbt ";
sql = sql + "   SET T_PAYMSTATUS = 3000 ";
sql = sql + " WHERE T_PAYMENTID = :p_acctrnid ";
cmd = RSDCommand(sql);
cmd.addParam("p_acctrnid", RSDBP_IN, p_acctrnid);
cmd.execute();


cmd.close(); cmd = NULL; sql = NULL;

msgbox("Успех!");