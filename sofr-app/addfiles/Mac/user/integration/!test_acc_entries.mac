import rsd;

private var sql, cmd;
private var p_acctrnid = 41652;

GetInt(p_acctrnid, "Идентификатор счета");

sql =       "UPDATE dacctrn_dbt ";
sql = sql + "   SET t_ground = SUBSTR(t_ground, 1, (LENGTH(t_ground) - 1)) || (TO_NUMBER(SUBSTR(t_ground, -1, 1)) + 1) ";
sql = sql + " WHERE t_acctrnid = :p_acctrnid ";
cmd = RSDCommand(sql);
cmd.addParam("p_acctrnid", RSDBP_IN, p_acctrnid);
cmd.execute();

cmd.close(); cmd = NULL; sql = NULL;

msgbox("Успех!");