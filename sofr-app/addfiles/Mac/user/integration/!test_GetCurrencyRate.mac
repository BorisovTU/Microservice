import "ws_synch_SOFR.mac";

private var result;
private var repDate;
private var day, mon, year, date_str;

repDate = date();

  if (GetDate(repDate,"Введите дату, за которую нужно получить курсы")) 
     DateSplit ( repDate, day, mon, year );
     date_str = String(day:o:2)+"."+String(mon:o:2)+"."+String(year:o:4);

     result = ws_SOFR_GetCurrencyRate(null,null,string("845321;5017;1;3;", date_str,";2"));
     msgbox("Done!");

  end;