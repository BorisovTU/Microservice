   // структура хранения счетов
   private class CAccs(_acc, _mpcode, _mpregdate,_servkind, _servkindsub, _isocur, _sect_name)
      var acc, mpcode, servkind, servkindsub, mpregdate, isocur, sect_name;
      acc         = _acc;         
