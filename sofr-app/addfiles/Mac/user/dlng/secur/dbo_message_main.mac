   /**
   @brief Проверка соответствия договора форме 4.4

   @return 0 - использовать форму 4.4
   @return -1 - галочка "форма 4.4", но при этом недостаточно данных
   @return 1 - не использовать форму 4.4

   BOSS-1050 новый шаблон уведомления для ЮЛ (только ООО) по форме Приложения 4.4
   Форма 4.4. используется при выполнении следующих условий:
      1. Клиент - юр.лицо
      2. Установлена галочка "форма 4.4" (категория CONTRACT_CATEGORY_NOTE44 = CONTRACT_CATEGORY_NOTE44_YES)
      3. Заполнены номер договора (n102 = примечание 102) и номер счета депо (n101 = примечание 101). 
   n101 и n102 заполняются в constructor
   Проверка на ЮЛ производится за пределами функции
   BOSS-1790 Дли "доверительный управляющий" всегда форма 4.1
   */

   private macro CheckForm4_4()
      private const CONTRACT_CATEGORY_NOTE44 = 126;
      private const CONTRACT_CATEGORY_NOTE44_YES = 1;
      private const CONTRACT_CATEGORY_NOTE44_NO = 2;

      var objectid = String(ddl_id:o:34);
      var ObjCat = RsbObjCategories(OBJTYPE_BROKERCONTR_DL, objectid);
      var Attrcode = 0;
      var res = 1;

      //BOSS-1790
      var error, stat;
      var IsTrustee = ВидСубъекта(PartyId, PTK_CONFID_MANAGER, error); // является ли субъект доверительным управляющим 

      if (ObjCat.GetMainAttr( CONTRACT_CATEGORY_NOTE44, date(), attr, attrcor ) == 0)
        Attrcode = attrcor.attrid;
      end;

      if (IsTrustee)
        res = 1;
        if (Attrcode == CONTRACT_CATEGORY_NOTE44_YES)
            stat = ObjCat.ConnectAttr(CONTRACT_CATEGORY_NOTE44, CONTRACT_CATEGORY_NOTE44_NO, "",  "", {curdate} );
            if(stat == 0)
               stat = ObjCat.Save(objectid);
            end;
        end;
      else 
        if (Attrcode == CONTRACT_CATEGORY_NOTE44_YES)
          if ( (n101 != "") and (n104 != "") ) // в соответствии с требованиями проверяем только номер договора и номер счета, хотя на печать выводится еще дата договора и торговый счет
             res = 0;
          else 
             res = -1;
          end;
        else
          res = 1;
        end;
      end;

      ObjCat = NULL;
      return res;
   end;

   // наименование секции обслуживания
   private macro GetNameSectionGetNameSection(kind, kindsub, isocur)
      var retval = "";

      if (kind == 15)
         retval = "Срочный рынок";
      elif(kind == 1)
         if (KindSub == 8)
             retval = "Фондовый рынок";
         elif (KindSub == 9)
             retval = "Внебиржевой рынок";
         end;
      elif(kind == 21)
         if (KindSub == 8)
            retval = "Валютный рынок";
         end;
      end;

      return RetVal + " (" + isocur + ")";
   end;
   // пустая SQL строка
   macro emp_str(p_par)
      if ((p_par == null) or (p_par == nullval) or (p_par == strfor(1)) or (p_par == strfor(0)) or (p_par == "01.01.0001"))
         return "";
      end;         
      return p_Par;
   end;
   
   // клиент - ГО
   macro isGOClient(p_par)
      return ((p_Par == "") or (p_par == "00"));
   end;
   
   // статус договора - обработка завершена
   macro processingCompleted()      
      return Cnt_Stat > 0;  
      // реализация с обращением к базе
      var sqls, rsdc;
      sqls = String( 
                  "DECLARE   \n"
                 ,"   v_Ret NUMBER(5);   \n"
                 ,"BEGIN   \n"
                 ,"   SELECT COUNT(1)   \n"
                 ,"     INTO v_Ret   \n"
                 ,"     FROM Dobjatcor_Dbt Dob   \n"
                  ,"    WHERE Dob.t_Object = Lpad(To_Char(:p_Cnt_Id), 34, '0')   \n"
                 ,"      AND Dob.t_Attrid = 3   \n"
                 ,"      AND Dob.t_Objecttype = 207   \n"
                 ,"      AND Dob.t_Groupid = 101;   \n"
                 ,"   :p_Ret := v_Ret;   \n"
                 ,"END;   \n"
                );
      rsdc = rsdcommand(sqls);
      rsdc.AddParam("p_Cnt_Id", RSDBP_IN, ddl_id);
      rsdc.AddParam("p_Ret", RSDBP_OUT, v_integer  );
      rsdc.execute;
      sqls = int(rsdc.Value("p_Ret"));
      rsdc = close; 
      return sqls > 0;
      onError
         return false;
   end;
   
