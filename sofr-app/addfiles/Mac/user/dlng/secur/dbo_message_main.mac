   /*BIQ - 7031*/
   //создать оповещение
   MACRO CftCode(p_kindCode,p_Partyid)
    var cftCodeOrINN = "";
    var query, cmd, DataSet;
     query  ="SELECT t_Code "+
                       " FROM DOBJCODE_DBT "+
                       " WHERE t_ObjectType = 3 "+
                       " AND t_CodeKind = ? "+
                       " AND t_ObjectID = ? "+
                       " AND t_state <>1";
        cmd = DL_RSDCommand(query);
        cmd.AddParam(p_kindCode);
        cmd.AddParam(p_Partyid);
        DataSet = cmd.execute();
        if(DataSet.MoveNext())
           cftCodeOrINN = DataSet.Code;
        end;
     return cftCodeOrINN;
   END;

   MACRO CreateNotificationTxt()
      var notificationText;
      var query,cmd,DataSet;
      var bornDate;
      var codeCft;

      var NotoficationDateTime = "";

      query = " SELECT msg.t_senddate,                                                                              " +
              "        msg.t_sendtime,                                                                              " +
              "        TO_CHAR (msg.t_senddate, 'DD.MM.YYYY')||' '||TO_CHAR (msg.t_sendtime, 'HH24:MI:SS') senddttm " +
              "   FROM ddlcontrmsg_dbt msg                                                                          " +
              "  WHERE msg.t_id =                                                                                   " +
              "        (SELECT MAX (t_id)                                                                           " +
              "           FROM ddlcontrmsg_dbt                                                                      " +
              "          WHERE     t_kind = 500                                                                     " +
              "                AND t_dlcontrid = :1                                                                 " +
              "                AND t_SENDDATE > TO_DATE ('01.01.0001', 'dd.mm.yyyy'))                               ";
      cmd = DL_RSDCommand(query);
      cmd.AddParam(ddl_id);
      DataSet = cmd.execute();
      
      if(DataSet.moveNext())
         NotoficationDateTime = DataSet.senddttm;
      end;
    
      if(Prt_Lfrm ==2 )
         query = " SELECT t_born         "+
                 "   FROM dpersn_dbt     "+
                 "  WHERE t_personid = ? ";
         cmd = DL_RSDCommand(query);
         cmd.AddParam(PartyId);
         DataSet = cmd.execute();
         
         if(DataSet.moveNext())
            bornDate = string(date(DataSet.born):f);
         end;
      end;

      codeCft = CftCode(101, PartyId);

      notificationText = NumberDBO+";"+codeCft+";"+1+";"+N101+";"+string(toAnsi(Prt_Fnam))+";"+bornDate+";"+NotoficationDateTime;
      return notificationText;
   END;

