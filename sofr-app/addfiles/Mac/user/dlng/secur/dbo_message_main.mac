                    ,"               ORDER BY Msg.t_Createdate DESC, Msg.t_Createtime DESC)   \n"
                    ,"       WHERE Rownum = 1;   \n"
                    ,"   EXCEPTION   \n"
                    ,"      WHEN OTHERS THEN   \n"
                    ,"         v_Ret := NULL;   \n"
                    ,"   END;   \n"
                    ,"   :p_Ret := v_Ret;   \n"
                    ,"END;   \n"
                   );
      rsdc = rsdcommand(sqls);
      rsdc.AddParam("p_Cnt_Id", RSDBP_IN, ddl_id);
      rsdc.AddParam("p_Ret", RSDBP_OUT, v_string, 200);
      rsdc.execute;
      sqls = rsdc.Value("p_Ret");
      rsdc = null;                                   
      if ((sqls != null) and (sqls != nullval))
         FormFilename = sqls;
      end;
   end;
   

   /**
    @brief Получить код елемента справочника по его имени
    @param[in] Name - наименование значения справочника
    @return 0 - Если результат запроса null, замена на нуль
    @return >0  Код элемента справочника найден, возврат в переменной retval
    DEF-53800
   */
   macro GetNumTaxGroup(Name : string) : integer
   var retval = "";
   var exec = RSDCommand( "select  nvl(max(t_Element),0) as t_Element  from dllvalues_dbt where t_List = :p_List and T_NAME = :p_Name ");
         exec.addParam( "p_List", RSDBP_IN, 5009);     
         exec.addParam( "p_Name", RSDBP_IN, Name);
         exec.execute();

   var rs = RSDRecordSet(exec);
   if(rs.MoveNext())
      retval = rs.value("t_Element");
   end;
   return retval;
   END;
   
   
