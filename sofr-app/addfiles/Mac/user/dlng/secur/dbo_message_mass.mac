   macro chk_tmp_strc()
      var sqls, rsdc;
      sqls = String( 
            "DECLARE   \n"
           ,"   i INTEGER;   \n"
           ,"   s VARCHAR2(1000);   \n"
           ,"   e VARCHAR2(2000);   \n"
           ,"   \n"
           ,"   PROCEDURE Run_Sql IS   \n"
           ,"   BEGIN   \n"
           ,"      IF s IS NULL THEN   \n"
           ,"         RETURN;   \n"
           ,"      END IF;   \n"
           ,"      EXECUTE IMMEDIATE s;   \n"
           ,"   END;   \n"
           ,"   \n"
           ,"BEGIN   \n"
           ,"   BEGIN   \n"
           ,"      SELECT COUNT(1) INTO i FROM User_Tables t WHERE t.Table_Name = 'DBO_MESSAGE_TMP';   \n"
           ,"      IF i = 0 THEN   \n"
           ,"         s := 'create table DBO_MESSAGE_TMP ' +   \n"
           ,"              '(t_dlid NUMBER(10), t_avrexch CHAR(1), t_outexch CHAR(1), t_dvexch CHAR(1), t_moneyexch CHAR(1), t_name VARCHAR2(100), t_partyid NUMBER(10),' +   \n"
           ,"              ' t_number VARCHAR2(20), t_datebegin DATE, t_state VARCHAR2(50), t_message VARCHAR2(2000), t_issend CHAR(1), t_countsend NUMBER(10), t_datecreate DATE,' +   \n"
           ,"              ' t_datesend DATE, t_email VARCHAR2(50), t_resend CHAR(1), t_dateclose DATE) ' +   \n"
           ,"              ' tablespace USERS pctfree 10 initrans 1 maxtrans 255 storage (initial 64K next 1M minextents 1 maxextents unlimited)';   \n"
           ,"         Run_Sql;   \n"
           ,"      END IF;   \n"
           ,"      IF s IS NULL THEN   \n"
           ,"         SELECT COUNT(1)   \n"
           ,"           INTO i   \n"
           ,"           FROM User_Tab_Cols t   \n"
           ,"          WHERE t.Table_Name = 'DBO_MESSAGE_TMP'   \n"
           ,"            AND t.Column_Name = 'T_DATECLOSE';   \n"
           ,"         IF i = 0 THEN   \n"
           ,"            s := 'alter table DBO_MESSAGE_TMP add t_dateclose date';   \n"
           ,"         END IF;   \n"
           ,"         Run_Sql;   \n"
           ,"         SELECT Data_Precision   \n"
           ,"           INTO i   \n"
           ,"           FROM User_Tab_Cols t   \n"
           ,"          WHERE t.Table_Name = 'DBO_MESSAGE_TMP'   \n"
           ,"            AND t.Column_Name = 'T_COUNTSEND';   \n"
           ,"         IF i = 5 THEN   \n"
           ,"            s := 'alter table DBO_MESSAGE_TMP modify t_countsend NUMBER(10)';   \n"
           ,"            Run_Sql;   \n"
           ,"         END IF;   \n"
           ,"         SELECT Data_Length   \n"
           ,"           INTO i   \n"
           ,"           FROM User_Tab_Cols t   \n"
           ,"          WHERE t.Table_Name = 'DBO_MESSAGE_TMP'   \n"
           ,"            AND t.Column_Name = 'T_MESSAGE';   \n"
           ,"         IF i < 2000 THEN   \n"
           ,"            s := 'alter table DBO_MESSAGE_TMP modify T_MESSAGE VARCHAR2(2000)';   \n"
           ,"            Run_Sql;   \n"
           ,"         END IF;   \n"
           ,"      END IF;   \n"
           ,"   EXCEPTION   \n"
           ,"      WHEN OTHERS THEN   \n"
           ,"         e := Nvl(e, Dbms_Utility.Format_Error_Stack || ' ' || Dbms_Utility.Format_Error_Backtrace);   \n"
           ,"   END;   \n"
           ,"   :e := e;   \n"
           ,"END;   \n"
          );
      rsdc = rsdcommand(sqls);
      rsdc.AddParam("e", RSDBP_OUT, v_string, 2000);
      rsdc.execute;
      rsdc = null;
      onerror(err);
   end;

   macro get_sess_id
