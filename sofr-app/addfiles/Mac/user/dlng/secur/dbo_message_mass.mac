                 ,"                FROM Ddlcontrmsg_Dbt m   \n"
                 ,"               WHERE m.t_Dlcontrid = Dl.t_Dlcontrid   \n"
                 ,"                 AND m.t_Kind = 500) t_Datecreate   \n"
                 ,"            ,(SELECT MAX(m.t_Senddate)   \n"
                 ,"                FROM Ddlcontrmsg_Dbt m   \n"
                 ,"               WHERE m.t_Dlcontrid = Dl.t_Dlcontrid   \n"
                 ,"                 AND m.t_Kind = 500) t_Datesend   \n"
                 ,"            ,Dl.t_Email   \n"
                 ,"            ,0 t_Countsend   \n"
                 ,"            ,Chr(0) t_Resend   \n"
                 ,"           ,(SELECT md.t_respdate  \n"
                 ,"                 FROM Ddlcontrmsg_Dbt md  \n"
                 ,"                WHERE md.t_kind = 1  \n"
                 ,"                  AND md.t_sendmesstate = 210  \n"
                 ,"                  AND md.t_respdate <> to_date('01010001','ddmmyyyy')  \n"
                 ,"                  AND md.t_dlcontrid = Dl.t_Dlcontrid) as T_DateMMVBAnswer  \n"
                 ,"            ,(SELECT mt.t_respTime  \n"
                 ,"                 FROM Ddlcontrmsg_Dbt mt  \n"
                 ,"                WHERE mt.t_kind = 1  \n"
                 ,"                  AND mt.t_sendmesstate = 210  \n"
                 ,"                  AND mt.t_respdate <> to_date('01010001','ddmmyyyy')  \n"
                 ,"                  AND mt.t_dlcontrid = Dl.t_Dlcontrid) as T_TimeMMVBAnswer  \n"
                 ,"           ,(SELECT td.t_date  \n"
                 ,"               FROM dnotetext_dbt td   \n"
                 ,"               WHERE td.t_objecttype = 207  \n"
                 ,"                 AND td.t_notekind = 102  \n"
                 ,"                 AND td.t_date <> to_date('01010001','ddmmyyyy')  \n"
                 ,"                 AND td.t_documentid = LPAD(Dl.t_Dlcontrid, 34, '0')) as T_DateDepoAnswer  \n"
                 ,"           ,(SELECT tt.t_time   \n"
                 ,"               FROM dnotetext_dbt tt  \n"
                 ,"               WHERE tt.t_objecttype = 207  \n"
                 ,"                 AND tt.t_notekind = 102  \n"
                 ,"                 AND tt.t_date <> to_date('01010001','ddmmyyyy')  \n"
                 ,"                 AND tt.t_documentid = LPAD(Dl.t_Dlcontrid, 34, '0')) as T_TimeDepoAnswer \n"
                 ,"        FROM Ddlcontr_Dbt Dl   \n"
                 ,"       INNER JOIN Dsfcontr_Dbt Sfn   \n"
                 ,"          ON (Sfn.t_Id = Dl.t_Sfcontrid)  \n"
                 ,"         where sfn.t_partyid <> 1   \n"
                 ,"           AND EXISTS (SELECT 1  \n"
                 ,"                       FROM Ddlcontrmsg_Dbt m  \n"
                 ,"                      WHERE m.t_kind = 1 \n"
                 ,"                        AND m.t_sendmesstate = 210 \n"
                 ,"                        AND m.t_respdate <> to_date('01010001','ddmmyyyy') \n"
                 ,"                        AND m.t_dlcontrid = Dl.t_Dlcontrid)  \n"
                 ,"   AND NOT EXISTS (SELECT 1 \n"
                 ,"                      FROM Ddlcontrmsg_Dbt m \n"
                 ,"                     WHERE m.t_dlcontrid = Dl.t_Dlcontrid \n"
                 ,"                       AND m.t_kind = 500 \n"
                 ,"                      AND m.t_senddate <> to_date('01010001','ddmmyyyy')) \n"
                 ,"           AND EXISTS (SELECT 1 \n"
                 ,"                       FROM dnotetext_dbt t  \n"
                 ,"                       WHERE t.t_objecttype = 207  \n"
                 ,"                         AND t.t_notekind = 102 \n"
                 ,"                         AND t.t_documentid = LPAD(Dl.t_Dlcontrid, 34, '0') \n"
                 ,"                         AND t.t_date <> to_date('01010001','ddmmyyyy'))) t \n"
                 ,"     where t.t_Dateclose = to_date('01010001','ddmmyyyy')  \n"
                 ,"       and t.t_Datebegin > trunc(add_months(sysdate,-3));   \n"
                 ,"   COMMIT;   \n"
                 ,"EXCEPTION   \n"
                 ,"   WHEN OTHERS THEN   \n"
                 ,"      ROLLBACK;   \n"
                 ,"      :p_Err := v_Err;   \n"
                 ,"END;   \n"
                );
   rsdc = rsdcommand(sqls);
   rsdc.AddParam("p_Err", RSDBP_OUT, v_string, 2000);
   rsdc.execute;
   if (not rsdc)        
      runerror("Ошибка получения списка договоров");
   end;
   sqls = rsdc.Value("p_Err");
   if ((sqls != null) and (sqls != nullval))
      runerror("Ошибка получения данных по договору", sqls);
   end;    
   rsdc = null;
   return true;   
   onerror(e)                                            
      rsdc = null;
      if ((e.Err != null) or (e.Err != nullval))
         msgbox(e.Err);
      else 
         msgbox(e.Message + " " + e.Module + " " + e.Line);
      end;                
      return false;
   end;

