   macro upd_line(p_Rwd, p_Msg)
   var sqls, rsdc;
   if ((p_Msg == null) or (p_Msg == nullval))
      p_Msg = "";
   end;
   sqls = String( 
         "DECLARE   \n"
        ,"   v_Rwd VARCHAR2(50) := :p_Rwd;   \n"
        ,"   v_Msg DBO_MESSAGE_TMP.t_Message%TYPE := Substr(:p_Msg, 1, 2000);   \n"
        ,"BEGIN   \n"
        ,"   UPDATE DBO_MESSAGE_TMP t   \n"
        ,"      SET (t_Avrexch   \n"
        ,"         ,t_Outexch   \n"
        ,"         ,t_Dvexch   \n"
        ,"         ,t_Moneyexch   \n"
        ,"         ,t_Name   \n"
        ,"         ,t_Partyid   \n"
        ,"         ,t_Number   \n"
        ,"         ,t_Datebegin   \n"
        ,"         ,t_Dateclose   \n"
        ,"         ,t_State   \n"
        ,"         ,t_Issend   \n"
        ,"         ,t_Datecreate   \n"
        ,"         ,t_Datesend   \n"
        ,"         ,t_Email   \n"
        ,"         ,t_Resend   \n"
        ,"         ,t_Countsend   \n"
        ,"         ,t_Message) =   \n"
        ,"          (SELECT Decode((SELECT COUNT(1)   \n"
        ,"                           FROM Ddlcontrmp_Dbt Dlp   \n"
        ,"                          INNER JOIN Dsfcontr_Dbt Sf ON (Sf.t_Id = Dlp.t_Sfcontrid)   \n"
        ,"                          WHERE Dlp.t_Dlcontrid = Dl.t_Dlcontrid   \n"
        ,"                            AND Sf.t_Servkind = 1   \n"
        ,"                            AND Sf.t_Servkindsub = 8)   \n"
        ,"                        ,0   \n"
        ,"                        ,Chr(0)   \n"
        ,"                        ,Chr(88)) Avrexch   \n"
        ,"                 ,Decode((SELECT COUNT(1)   \n"
        ,"                           FROM Ddlcontrmp_Dbt Dlp   \n"
        ,"                          INNER JOIN Dsfcontr_Dbt Sf ON (Sf.t_Id = Dlp.t_Sfcontrid)   \n"
        ,"                          WHERE Dlp.t_Dlcontrid = Dl.t_Dlcontrid   \n"
        ,"                            AND Sf.t_Servkind = 1   \n"
        ,"                            AND Sf.t_Servkindsub = 9)   \n"
        ,"                        ,0   \n"
        ,"                        ,Chr(0)   \n"
        ,"                        ,Chr(88)) Outexch   \n"
        ,"                 ,Decode((SELECT COUNT(1)   \n"
        ,"                           FROM Ddlcontrmp_Dbt Dlp   \n"
        ,"                          INNER JOIN Dsfcontr_Dbt Sf ON (Sf.t_Id = Dlp.t_Sfcontrid)   \n"
        ,"                          WHERE Dlp.t_Dlcontrid = Dl.t_Dlcontrid   \n"
        ,"                            AND Sf.t_Servkind = 15)   \n"
        ,"                        ,0   \n"
        ,"                        ,Chr(0)   \n"
        ,"                        ,Chr(88)) Dvexch   \n"
        ,"                 ,Decode((SELECT COUNT(1)   \n"
        ,"                           FROM Ddlcontrmp_Dbt Dlp   \n"
        ,"                          INNER JOIN Dsfcontr_Dbt Sf ON (Sf.t_Id = Dlp.t_Sfcontrid)   \n"
        ,"                          WHERE Dlp.t_Dlcontrid = Dl.t_Dlcontrid   \n"
        ,"                            AND Sf.t_Servkind = 21   \n"
        ,"                            AND Sf.t_Servkindsub = 8)   \n"
        ,"                        ,0   \n"
        ,"                        ,Chr(0)   \n"
        ,"                        ,Chr(88)) Moneyexch   \n"
        ,"                 ,Sfn.t_Name   \n"
        ,"                 ,Sfn.t_Partyid   \n"
        ,"                 ,Sfn.t_Number   \n"
        ,"                 ,Sfn.t_Datebegin   \n"
        ,"                 ,Sfn.t_Dateclose   \n"
        ,"                 ,(SELECT Atr.t_Name   \n"
        ,"                     FROM Dobjatcor_Dbt Atc   \n"
        ,"                    INNER JOIN Dobjattr_Dbt Atr ON (Atr.t_Objecttype = Atc.t_Objecttype AND   \n"
        ,"                                                   Atc.t_Groupid = Atr.t_Groupid AND Atc.t_Attrid = Atr.t_Attrid)   \n"
        ,"                    WHERE Atc.t_Objecttype = 207   \n"
        ,"                      AND Atc.t_Groupid = 101   \n"
        ,"                      AND Trunc(SYSDATE) BETWEEN Atc.t_Validfromdate AND Atc.t_Validtodate   \n"
        ,"                      AND Atc.t_Object = Lpad(Dl.t_Dlcontrid, 34, '0')) t_State   \n"
        ,"                 ,Decode((SELECT COUNT(1)   \n"
        ,"                           FROM Ddlcontrmsg_Dbt m   \n"
        ,"                          WHERE m.t_Dlcontrid = Dl.t_Dlcontrid   \n"
        ,"                            AND m.t_Kind = 500)   \n"
        ,"                        ,0   \n"
        ,"                        ,Chr(0)   \n"
        ,"                        ,Chr(88)) t_Issend   \n"
        ,"                 ,(SELECT MAX(m.t_Createdate)   \n"
        ,"                     FROM Ddlcontrmsg_Dbt m   \n"
        ,"                    WHERE m.t_Dlcontrid = Dl.t_Dlcontrid   \n"
        ,"                      AND m.t_Kind = 500) t_Datecreate   \n"
        ,"                 ,(SELECT MAX(m.t_Senddate)   \n"
        ,"                     FROM Ddlcontrmsg_Dbt m   \n"
        ,"                    WHERE m.t_Dlcontrid = Dl.t_Dlcontrid   \n"
        ,"                      AND m.t_Kind = 500) t_Datesend   \n"
        ,"                 ,Dl.t_Email   \n"
        ,"                 ,Chr(0) t_Resend   \n"
        ,"                 ,CASE -- если успех - изменить признак на обработанный этой сессией   \n"
        ,"                     WHEN v_Msg = ' ' THEN   \n"
        ,"                      -1 * Abs(t.t_Countsend)   \n"
        ,"                     ELSE -- иначе освободить договор для других сессий   \n"
        ,"                      0   \n"
        ,"                  END t_Countsend   \n"
        ,"                 ,CASE   \n"
        ,"                     WHEN v_Msg IS NULL THEN   \n"
        ,"                      t.t_Message   \n"
        ,"                     WHEN v_Msg = ' ' THEN   \n"
        ,"                      NULL   \n"
        ,"                     ELSE   \n"
        ,"                      Substr(v_Msg || ' ' || t.t_Message, 1, 2000)   \n"
        ,"                  END t_Message   \n"
        ,"             FROM Ddlcontr_Dbt Dl   \n"
        ,"            INNER JOIN Dsfcontr_Dbt Sfn ON (Sfn.t_Id = Dl.t_Sfcontrid)   \n"
        ,"            INNER JOIN Dparty_Dbt Prt ON (Prt.t_Partyid = Sfn.t_Partyid)   \n"
        ,"            WHERE Dl.t_Dlcontrid = t.t_Dlid)   \n"
        ,"    WHERE t.Rowid = v_Rwd;   \n"
        ,"   COMMIT;   \n"
        ,"EXCEPTION   \n"
        ,"   WHEN OTHERS THEN   \n"
        ,"      ROLLBACK;   \n"
        ,"END;   \n"
       );
   rsdc = rsdcommand(sqls);
   rsdc.AddParam("p_Rwd", RSDBP_IN, p_Rwd);
   rsdc.AddParam("p_Msg", RSDBP_IN, p_Msg);
   rsdc.execute;                         
   rsdc = null;
   onerror(e);
   end;

   macro pushUpload(p_Prt)
      var sqls, rsdc;
      var v_Typ_Conn = 0, v_Prt_Expb = 0, v_Prt_Quik = 0;
      var ans;
      sqls = String( 
            "DECLARE   \n"
           ,"   v_Prt Dparty_Dbt.t_Partyid%TYPE := :p_Prt;   \n"
           ,"BEGIN   \n"
           ,"   -- тип соединения = СКП ЭП   \n"
           ,"   SELECT COUNT(1)   \n"
           ,"     INTO :p_Typ_Conn   \n"
           ,"     FROM Dsfcontr_Dbt Cntr   \n"
           ,"    INNER JOIN Ddlcontr_Dbt Dl ON (Dl.t_Sfcontrid = Cntr.t_Id)   \n"
           ,"    WHERE Cntr.t_Partyid = v_Prt   \n"
           ,"      AND Cntr.t_Dateclose = Rsb_Secur.Unknowndate   \n"
           ,"      AND (EXISTS (SELECT 1   \n"
           ,"             FROM Dobjatcor_Dbt o   \n"
           ,"            WHERE o.t_Objecttype = 207   \n"
           ,"              AND o.t_Groupid = 170   \n"
           ,"              AND o.t_Attrid = 1   \n"
           ,"              AND (o.t_Validtodate >= Rsbsessiondata.Curdate OR o.t_Validtodate = Rsb_Secur.Unknowndate)   \n"
           ,"              AND o.t_Validfromdate <= Rsbsessiondata.Curdate   \n"
           ,"              AND Lpad(Dl.t_Dlcontrid, 34, '0') = o.t_Object)   \n"
           ,"              or cntr.t_datebegin >= to_date('28.12.2019', 'DD.MM.YYYY'));    \n"
           ,"   -- выгружен в РСХБ-Брокер   \n"
           ,"   SELECT COUNT(1)   \n"
           ,"     INTO :p_Prt_Expb   \n"
           ,"     FROM Dobjatcor_Dbt o   \n"
           ,"    WHERE o.t_Objecttype = 3   \n"
           ,"      AND o.t_Groupid = 170   \n"
           ,"      AND o.t_Object = Lpad(v_Prt, 10, '0')   \n"
           ,"      AND o.t_Attrid = 1   \n"
           ,"      AND (o.t_Validtodate >= Rsbsessiondata.Curdate OR o.t_Validtodate = Rsb_Secur.Unknowndate);   \n"
           ,"   -- есть код QUIK   \n"
           ,"   SELECT COUNT(1)   \n"
           ,"     INTO :p_Prt_Quik   \n"
           ,"     FROM Dobjcode_Dbt o   \n"
           ,"    WHERE o.t_Objecttype = 3   \n"
           ,"      AND o.t_Codekind = 105   \n"
           ,"      AND o.t_Objectid = v_Prt   \n"
           ,"      AND o.t_Bankclosedate = Rsb_Secur.Unknowndate;   \n"
           ,"END;   \n");
      rsdc = rsdcommand(sqls);
      rsdc.AddParam("p_Prt", RSDBP_IN, p_Prt);
      rsdc.AddParam("p_Typ_Conn", RSDBP_OUT, v_integer);
      rsdc.AddParam("p_Prt_Expb", RSDBP_OUT, v_integer);
      rsdc.AddParam("p_Prt_Quik", RSDBP_OUT, v_integer);
      rsdc.execute;                         
      v_Typ_Conn = int(rsdc.Value("p_Typ_Conn"));
      v_Prt_Expb = int(rsdc.Value("p_Prt_Expb"));
      v_Prt_Quik = int(rsdc.Value("p_Prt_Quik"));
      rsdc = null;
      if (v_Typ_Conn > 0)
         return "Метод аутентификации СКП ЭП или договор заключен после 28.12.19 обновление не требуется";
      end;
      if (v_Prt_Expb > 0)
         //обновление РСХБ-Брокер
         execMacrofile("global_utils_intgr.mac","m_make_event_to_process",5026, p_Prt);
      else
         ans = "Не требуется обновление РСХБ Брокер";
      end;
      if (v_Prt_Quik > 0)
         // обновление аккаунта Quik 
         execMacrofile("global_utils_intgr.mac","m_make_event_to_process",5025, p_Prt);
      else
         ans =ans  +  "   Не требуется обновление QUIK";
      end;  
      return "";   
   onerror(e);
      return e.Message;     
   end;
   
   macro sel_work(add_del)       
      var i = 0, v_ob, sqls, rsdc, rsds, t_st, t_1st, v_dt_msg, v_rslt_msg = ""  ;
      sqls = String( 
               "SELECT t.t_Dlid, t.t_Partyid, t.Rowid Rwd, COUNT(1) Over() r_All, Row_Number() Over(ORDER BY t.t_Dlid) n_All   \n"
              ,"  FROM DBO_MESSAGE_TMP t   \n"
              ," WHERE t.t_Countsend = Sys_Context('Userenv', 'SESSIONID')   \n"
              ,"   AND Nvl(t.t_Dateclose, To_Date('01010001', 'DDMMYYYY')) = To_Date('01010001', 'DDMMYYYY')   \n"
              ," ORDER BY t.t_Dlid   \n"
            );
      rsdc = rsdcommand(sqls);
      rsds = rsdrecordset(rsdc, RSDVAL_CLIENT_IF_NEEDED, RSDVAL_FORWARD_ONLY);
      rsds.Open;
      rsds.MoveNext();
      t_st = time();
      // если ничего не выбрано - никакой обработки
      if ((rsds.Value("r_All") == null) or (rsds.Value("r_All") == nullval) or (rsds.Value("r_All") == 0))
         rsds.Close;
         rsdc = rsds = null;
         return;
      end;
      Initprogress(int(rsds.Value("r_All")), "", "Обработка");
      SetDialogFlag(0);
      while (true)
         t_1st = time();
         if (add_del < 0)
            if (del_subcontr(rsds.Value("t_Dlid"), 21, 8))
               upd_line(rsds.Value("rwd"), " ");
            end;
         else
            v_dt_msg = date(0,0,0);

           //не открываем v_rslt_msg = crt_subcontr(rsds.Value("t_Dlid"), 21, 8, @v_Dt_Msg);
            if (strlen(trim(v_rslt_msg)) == 0)
               // не формировать уведомление, если сформировано сегодня
              // if (v_Dt_Msg < {curdate})
                  if (v_Ob == null)
                     v_Ob = dboMessage(rsds.Value("t_Dlid"));
                  else
                     v_Ob.ddl_id = rsds.Value("t_Dlid");
                     v_Ob.Constructor;
                  end;
                  if (v_Ob and (v_Ob.statsend != -1))    
                     if (rsds.Value("n_All") < rsds.Value("r_All"))
                        v_Ob.isLast = false; 
                     end;
                     v_Ob.CreateMessage(true, true, true, false, "");
                     if (v_Ob.statsend == -1)
                        v_rslt_msg = v_Ob.statMes;
                     else   
                        v_rslt_msg = " ";
                     end;
                  else
                     if (v_Ob)
                        v_rslt_msg = v_Ob.statMes;
                     else
                        v_rslt_msg = "Объект уведомления не создан";
                        v_Ob = null;
                     end;
                  end;  
              // else   
               //   v_rslt_msg = " Сегодня уже формировалось ";
               //end;
            end;
            if (strlen(trim(v_rslt_msg)) == 0)  
           //    v_rslt_msg = pushUpload(int(rsds.Value("t_Partyid")));
            end;
            if (strlen(trim(v_rslt_msg)) == 0)  
               upd_line(rsds.Value("rwd"), " Ок ");
            else
               upd_line(rsds.Value("rwd"), v_rslt_msg);
            end;
         end;
         if (not rsds.MoveNext())
            break;
         end;
         SetDialogFlag(1);
         Useprogress(int(rsds.Value("n_All")));
         SetDialogFlag(0);
      end;            
      if (v_Ob)
         v_Ob.isLast = true;
         v_Ob.cls_word();
      end;
      rsds.Close;
      rsdc = rsds = null;
      SetDialogFlag(1);
      remprogress();
      //msgbox(time - t_st);
   onerror(e)
      SetDialogFlag(1);
      remprogress();
   end;

   macro sel_save(sql)
      var sqls, rsdc;
      sqls = String( 
         "DECLARE   \n"
        ,"   v_Rwd VARCHAR2(50) := :p_Rwd;   \n"
        ,"   v_Ses NUMBER(10) := Sys_Context('Userenv', 'SESSIONID');   \n"
        ,"   v_Err VARCHAR2(2000);   \n"
        ,"BEGIN   \n"
        ,"   BEGIN   \n"
        ,"      UPDATE DBO_MESSAGE_TMP l   \n"
        ,"         SET l.t_Countsend = v_Ses   \n"
        ,"       WHERE l.Rowid = v_Rwd   \n"
        ,"         AND Nvl(l.t_Dateclose, To_Date('01010001', 'DDMMYYYY')) = To_Date('01010001', 'DDMMYYYY')   \n"
        ,"         AND (Nvl(l.t_Countsend, 0) IN (0, v_Ses, -1 * v_Ses) OR NOT EXISTS   \n"
        ,"              (SELECT 1   \n"
        ,"                 FROM V$session v   \n"
        ,"                INNER JOIN Dregistry_Dbt r ON (r.t_Audsid = v.Audsid)   \n"
        ,"                WHERE v.Audsid = l.t_Countsend));   \n"
        ,"      IF SQL%ROWCOUNT = 0 THEN   \n"
        ,"         v_Err := 'Не найден или находится в обработке у другого пользователя';   \n"
        ,"         RAISE No_Data_Found;   \n"
        ,"      END IF;   \n"
        ,"      COMMIT;   \n"
        ,"   EXCEPTION   \n"
        ,"      WHEN OTHERS THEN   \n"
        ,"         ROLLBACK;   \n"
        ,"         v_Err := Nvl(v_Err, Dbms_Utility.Format_Error_Stack || ' ' || Dbms_Utility.Format_Error_Backtrace);   \n"
        ,"   END;   \n"
        ,"   :p_Err := v_Err;   \n"
        ,"END;   \n"
       );
      rsdc = rsdcommand(sqls);
      rsdc.AddParam("p_Rwd", RSDBP_IN, sql.Value("Rwd"));
      rsdc.AddParam("p_Err", RSDBP_OUT, v_string, 2000);
      rsdc.execute;     
      sqls = rsdc.Value("p_Err");   
      if ((sqls == null) or (sqls == nullval))
         sqls = "";
      end;
      rsdc = null;
      return sqls;
   onerror(e);
      sqls = e.Message;
      return sqls;      
   end;
   
   macro on_scr_init(sql, field, key_kod)
   AddMultiAction(sql, 316);
   AddMultiAction(sql, 322);
   while (sql.MoveNext())
      if (int(sql.Value("t_Dlid")) == v_lin)
         gotoscroll(sql);
         break;
      end;
   end;                                     
   if ((sql.Value("t_Dlid") != null) and (sql.Value("t_Dlid") != nullval) and (int(sql.Value("t_Dlid")) != v_lin))
      sql.MoveFirst();
      gotoscroll(sql);
   end;                     
   v_lin = -1;
   end;

   macro on_scr_mlt_beg(sql, field, key_kod)
   fl_end = false;
   if (key_kod == 316)
      v_lin = -1;
      if (msgboxex("Отправить уведомления по выделенным договорам? ", MB_YES + MB_NO, IND_YES) == IND_NO)
         return CM_MSEL_STOP_KEEP;
      end;
   end;
   if (key_kod == 322)
      v_lin = -1;
      if (msgboxex("Удалить субдоговоры по выделенным записям?", MB_YES + MB_NO, IND_YES) == IND_NO)
         return CM_MSEL_STOP_KEEP;
      end;
   end;
   end;

   macro on_scr_mlt_end(sql, field, key_kod)  
   fl_end = true;
   // обработка отобранных
   if (key_kod == 316)   
      sel_work(1);
   end;   
   if (key_kod == 322)
      sel_work(-1);
   end;   
   end;

   macro on_scr_mlt_sel(sql, field, key_kod)
   var sel_rslt = "";
   if (key_kod == 316)
      if (v_lin < 0) 
         v_lin = int(sql.Value("t_Dlid"));
      end;
   end;
   // расставить отметки для выбранных к обработке договоров
   sel_rslt = sel_save(sql);
   return CM_MSEL_CONT_CLEAR;
   end;

   macro on_scr_key_empty(sql, field, key_kod)
   if (fl_end)        
      return cm_select;
   end;
   end;

