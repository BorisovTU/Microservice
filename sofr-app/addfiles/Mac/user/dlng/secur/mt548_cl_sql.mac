                                              (SELECT SUBSTR (str, 7, 10)
                                                 FROM (    SELECT TO_CHAR (LTRIM (SUBSTR (txt, INSTR (txt, CHR (10), 1, LEVEL) + 1,   INSTR (txt, CHR (10), 1, LEVEL + 1) - INSTR (txt, CHR (10), 1, LEVEL) - 1)))
                                                                     str, TO_CHAR ( REPLACE ( SUBSTR ( SUBSTR ( txt, INSTR (txt, ':70D:'), INSTR (SUBSTR (txt, INSTR (txt, ':70D:')), CHR (10) || ':')), 13), CHR (10))) f70D, typ
                                                             FROM (SELECT CHR (10) || str || CHR (10) txt, typ
                                                                     FROM (SELECT str, TYPE typ
                                                                             FROM mtrec mt2
                                                                            WHERE mt2.id_paym = ttbl.ID_PAYM))
                                                       CONNECT BY LEVEL <= LENGTH (txt) - LENGTH (REPLACE (txt, CHR (10))) - 1)
                                                WHERE str LIKE (':25D::%') AND ROWNUM = 1)
