                      SELECT TO_CHAR (LTRIM (SUBSTR (txt, INSTR (txt,CHR (10),1,LEVEL)+ 1,   INSTR (txt,
                                                      CHR (10),1,LEVEL + 1)- INSTR (txt,CHR (10),1,LEVEL)- 1)))str_548
                        FROM (SELECT CHR (10) || str || CHR (10) txt, typ
                                FROM (SELECT str, TYPE typ
                                        FROM mtrec
                                       WHERE id_paym = ?))
                        CONNECT BY LEVEL <= LENGTH (txt) - LENGTH (REPLACE (txt, CHR (10))) - 1];
                        cmd_548 = RsdCommand(EndCaptureOutput());
                        cmd_548.AddParam("id_paym", RSDBP_IN, rs.value("idpaym"));

               end;


              rs_548 = RSDRecordset(cmd_548, rsdval_client, rsdval_static);
              Scroll_548.run(rs_548, null, "Содержимое сообщения", "ESC - Выход");
              return CM_IGNORE;
           else
            msgbox("Нет сквитованных МТ548");
            return CM_IGNORE;
           end;
           return CM_IGNORE;

        elif (key == 18)//  CTRL+R
             return cm_select;
                
        elif (key == 291)//  ALT+H
           if (rs.value("t_confirm_num")!="")
            if (rs.value("t_clientid")==-1)
               StartCaptureOutput;
               [
                 SELECT ass.*,
                        PCK_MAIN.ID_IPSPCK,
                        Pay_MAIN.ID_PAYM paym_sec,
                        REC.TYPE, p.numreferdoc,
                        (SELECT TO_CHAR (
