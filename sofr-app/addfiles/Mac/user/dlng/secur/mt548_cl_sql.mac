  AddCol ( 4, "t_confirm_num",    "Номер вх. сообщения",     17, true);
  AddCol ( 5, "t_confirm_date",     "Дата Время" ,             16, true);
  AddCol ( 6, "t_confirm_status", "Статус",                  10, true);
  AddCol ( 7, "t_confirm_reason", "Причина",                 10, true);
  AddCol ( 8, "t_confirm_comment","Комментарий",             50, true);

end;

    var rsd;

 //   Fill_MT548_TMP;

    StartCaptureOutput;
[
   WITH ttbl AS
         (SELECT tick.t_dealid,
                 tick.t_dealcode,
                 tick.t_dealcodets,
                 tick.t_dealdate,
                 LEG.T_MATURITY,
                 LEG.T_PFI,
                 leg.T_PRINCIPAL,
                 avr.t_isin,
                 leg.T_COST,
                 leg.T_TOTALCOST,
                 leg.t_cfi,
                 TICK.T_CLIENTID,
                 tick.t_partyid,
                 case
                   when tick.t_clientid = -1 then
                    (SELECT NVL(MAX(pp.id_paym), MAX(a.id_paym))
                       FROM pckpay pp,
                            ipspck mp,
                            payass a,
                            mtrec  m,
                            payass a1,
                            mtn9x  mt
                      WHERE pp.id_ipspck(+) = mp.id_ipspck
                        AND mp.id_paym(+) = a.id_paym
                        AND a1.id_paym = m.id_paym(+)
                        AND a1.id_paym = mt.id_paym(+)
                        AND a.associate = TO_CHAR(tick.t_dealid)
                        AND a.bankid =
                            TO_CHAR(NVL(8, DECODE(m.TYPE, 518, 3, 599, 4, 8)))
                        AND a1.id_paym = pp.id_paym
                        AND a1.id_sass =
                            RSP_COMMON.GET_ID_BY_CODE_DICT(P_TABLE => 'SASS',
                                                           P_CODE  => 1)
                        AND m.TYPE || mt.TYPE = '548'
                      GROUP BY m.TYPE || mt.TYPE)
                   else
            (SELECT max(mt.id_paym)
              FROM mtrec mt
            WHERE   INSTR (mt.str, ':35B:ISIN '||avr.t_isin) > 0
                   AND INSTR (mt.str, ':98A::TRAD//'||to_char(t_dealdate,'yyyymmdd')) > 0
                   AND INSTR (mt.str, ':98A::SETT//'||to_char(t_maturity,'yyyymmdd')) > 0
                   AND INSTR (mt.str,    replace( ':19A::SETT//'|| (SELECT f1.t_ccy FROM dfininstr_dbt f1 WHERE f1.t_fiid = leg.t_cfi) || to_char(t_totalcost),'.',',') ) > 0
                   AND INSTR (mt.str, ':36B::SETT//FAMT/'|| TO_CHAR (leg.T_PRINCIPAL * (SELECT F2.T_FACEVALUE FROM dfininstr_dbt f2 WHERE f2.t_fiid = LEG.T_PFI))) > 0 
                   AND 1=1)
                   end IDPaym
             FROM ddl_tick_dbt tick, ddl_leg_dbt leg, davoiriss_dbt avr
                       WHERE tick.t_dealid = leg.t_dealid
                         AND avr.t_fiid = leg.t_pfi
                         AND tick.t_bofficekind = 101
                         AND tick.t_marketofficeid = 0
--                         AND tick.t_clientid != -1
                         AND tick.t_dealstatus != 20 )
                SELECT ttbl.*,
                           case when ttbl.t_clientid = -1 then   
                           (select pa1.id_paym
                              from payass pa1, mtrec m1
                             where m1.id_paym = pa1.id_paym
                               and pa1.id_sass = 21
                               and pa1.associate = ttbl.t_dealid) else null end t_out_paym_id,
                           case when ttbl.t_clientid = -1 then     
                           (select case
                                     when m2.type is null then
                                      chr(1)
                                     else
                                      'MT' || to_char(m2.type)
                                   end
                              from payass pa2, mtrec m2
                             where m2.id_paym = pa2.id_paym
                               and pa2.id_sass = 21
                               and pa2.associate = ttbl.t_dealid) else 'Н/Д' end t_out_paym_type,
                           (SELECT p1.numreferdoc FROM paym p1 WHERE p1.id_paym = ttbl.idpaym) T_CONFIRM_NUM,
                           (SELECT TO_CHAR(TO_DATE(SUBSTR(str, 13, 14),'yyyy.mm.dd hh24:mi:ss'),'dd.mm.yyyy hh24:mi:ss')
                              FROM (SELECT TO_CHAR(LTRIM(SUBSTR(txt,INSTR(txt,CHR(10),1,LEVEL) + 1,
                                                                INSTR(txt,CHR(10),1,LEVEL + 1) -INSTR(txt,CHR(10),1,LEVEL) - 1))) str,
                                           TO_CHAR(REPLACE(SUBSTR(SUBSTR(txt,INSTR(txt,':70D:'),INSTR(SUBSTR(txt,INSTR(txt,':70D:')),CHR(10) || ':')),13),CHR(10))) f70D,typ
                                      FROM (SELECT CHR(10) || str || CHR(10) txt, typ
                                              FROM (SELECT str, TYPE typ
                                                      FROM mtrec
                                                     WHERE id_paym = ttbl.idpaym))
                                    CONNECT BY LEVEL <= LENGTH(txt) -
                                               LENGTH(REPLACE(txt, CHR(10))) - 1)
                             WHERE str LIKE (':98_::PREP//%')) t_confirm_date,
                           (SELECT SUBSTR(str, 7, 10)
                              FROM (SELECT TO_CHAR(LTRIM(SUBSTR(txt,
                                                                INSTR(txt,CHR(10),1,LEVEL) + 1,
                                                                INSTR(txt,CHR(10),1,LEVEL + 1) -
                                                                INSTR(txt,CHR(10),1,LEVEL) - 1))) str,
                                           TO_CHAR(REPLACE(SUBSTR(SUBSTR(txt,INSTR(txt,':70D:'),
                                                                         INSTR(SUBSTR(txt,INSTR(txt,':70D:')),CHR(10) || ':')),13),CHR(10))) f70D,typ
                                      FROM (SELECT CHR(10) || str || CHR(10) txt, typ
                                              FROM (SELECT str, TYPE typ
                                                      FROM mtrec
                                                     WHERE id_paym = ttbl.idpaym))
                                    CONNECT BY LEVEL <= LENGTH(txt) -
                                               LENGTH(REPLACE(txt, CHR(10))) - 1)
                             WHERE str LIKE (':25D::%')
                               AND ROWNUM = 1) t_confirm_status,
                           (SELECT SUBSTR(str, 7, 10)
                              FROM (SELECT TO_CHAR(LTRIM(SUBSTR(txt,INSTR(txt,CHR(10),1,LEVEL) + 1,
                                                                INSTR(txt,CHR(10),1,LEVEL + 1) -
                                                                INSTR(txt,CHR(10),1,LEVEL) - 1))) str,
                                           TO_CHAR(REPLACE(SUBSTR(SUBSTR(txt,INSTR(txt,':70D:'),
                                                                         INSTR(SUBSTR(txt,INSTR(txt,':70D:')),CHR(10) || ':')),13),CHR(10))) f70D,typ
                                      FROM (SELECT CHR(10) || str || CHR(10) txt, typ
                                              FROM (SELECT str, TYPE typ
                                                      FROM mtrec
                                                     WHERE id_paym = ttbl.idpaym))
                                    CONNECT BY LEVEL <= LENGTH(txt) -
                                               LENGTH(REPLACE(txt, CHR(10))) - 1)
                             WHERE str LIKE (':24B::%')
                               AND ROWNUM = 1) t_confirm_reason,
                           (SELECT f70D
                              FROM (SELECT TO_CHAR(LTRIM(SUBSTR(txt,
                                                                INSTR(txt,CHR(10),1,LEVEL) + 1,
                                                                INSTR(txt,CHR(10),1,
                                                                      LEVEL + 1) -INSTR(txt,
                                                                      CHR(10),1,LEVEL) - 1))) str,
                                           TO_CHAR(REPLACE(SUBSTR(SUBSTR(txt,INSTR(txt,':70D:'),
                                                                         INSTR(SUBSTR(txt,INSTR(txt,':70D:')),
                                                                               CHR(10) || ':')),13),CHR(10))) f70D,typ
                                      FROM (SELECT CHR(10) || str || CHR(10) txt, typ
                                              FROM (SELECT str, TYPE typ
                                                      FROM mtrec
                                                     WHERE id_paym = ttbl.idpaym))
                                    CONNECT BY LEVEL <= LENGTH(txt) -
                                               LENGTH(REPLACE(txt, CHR(10))) - 1)
                             WHERE str LIKE (':70D::%')) t_confirm_comment
                      FROM ttbl;

        ];

var m_sql = EndCaptureOutput();
rsd = RSDRecordset(m_sql, rsdval_client, rsdval_static);   
while(MainScroll.Run (rsd, null,"Статусы внебиржевых сделок (по МТ548)","Enter Просмотр  ALT+H История  CTRL+R Обновить  ESC Выход"))
 // Fill_MT548_TMP ();
  rsd = RSDRecordset(m_sql, rsdval_client, rsdval_static);   
end;
exit(1);

