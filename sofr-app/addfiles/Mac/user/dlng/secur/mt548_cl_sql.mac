               ]; 

               cmd_548_all = RsdCommand(EndCaptureOutput());
               cmd_548_all.AddParam("dealid", RSDBP_IN, rs.value("t_dealid"));
               rs_548_all = RSDRecordset(cmd_548_all,rsdval_client, rsdval_static);
            else
               StartCaptureOutput;
               [  with ttbl as 
                     (SELECT mt.id_paym
                       FROM mtrec mt
                      WHERE     INSTR (mt.str, ':35B:ISIN '||:ISIN) > 0
                            AND INSTR (mt.str, ':98A::TRAD//'||to_char(:TRADDATE,'yyyymmdd')) > 0
                            AND INSTR (mt.str, ':98A::SETT//'||to_char(:SETTDATE,'yyyymmdd')) > 0
