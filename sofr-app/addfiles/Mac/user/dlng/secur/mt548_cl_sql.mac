           end; 
        end;
    end;
    return CM_DEFAULT;

  end;

  AddCol ( 0, "t_dealid",         "ID сделки",               8, true);
  AddCol ( 1, "t_dealcode",       "Код сделки (внутренний)", 11, true);
  AddCol ( 2, "t_dealcodeTS",     "Код сделки (внешний)",    11, true);
  AddCol ( 3, "t_dealdate",           "Дата  сделки",            10, true);
