MACRO ExecuteStep(Buffer, Document)
	array mas;
	var messok,messerr,i;
	

	mas(0) = "Автоматическая квитовка";
	mas(1) = "Принудительная квитовка";

	messok = "Квитовка успешно выполнена!";
	messerr = "Квитовка не выполнена.";

	i=menu(mas," ESC - отмена, <Enter> - Квитовка ", "Выберите способ квитовки. ");
	if (i == -1)
		msgbox("Квитовка отложена!");
		return 1;
	elif (i == 0)
		msgbox(messerr);
		return 1;
	elif (i == 1)
		msgbox(messok);
		return 0;
	else
		return 1;
	end;		


END;