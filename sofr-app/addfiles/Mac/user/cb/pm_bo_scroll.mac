    /*Обработчик смены фокуса*/
    macro eventHandlerRF(ev)
      if((this.focusedControl.name == "dt1") or (this.focusedControl.name == "dt2"))
        if((getControl("dt1").value != date(0,0,0)) and (getControl("dt2").value != date(0,0,0)) and (getControl("dt2").value < getControl("dt1").value))
          msgboxex("Дата окончания периода не может быть меньше даты начала");
          if(this.focusedControl.name == "dt1")
            getControl("dt1").value = getControl("dt2").value;
          else
            getControl("dt2").value = getControl("dt1").value;
          end;
        end;
      end;
      return true;
    end;
    /*Обработчик нажатия кнопок*/
    macro eventHandlerKP(ev)
      if(ev.keycode == 27/*Esc*/)
        fill = false;
        close(ev.keycode);
        return true;
      elif(ev.keycode == 316/*F2*/) 
        eventHandlerRF(ev);
        dt1 = getControl("dt1").value;
        dt2 = getControl("dt2").value;
        chk_errors = getControl("chk_errors").checked;
        chk_reject = getControl("chk_reject").checked;
        chk_control = getControl("chk_control").checked;
        chk_others = getControl("chk_others").checked;
        chk_secur = getControl("chk_secur").checked;
        chk_dv = getControl("chk_dv").checked;
        chk_v = getControl("chk_v").checked;
        chk_mbk = getControl("chk_mbk").checked;
        chk_manual = getControl("chk_manual").checked;
        fill = true;
        close(ev.keycode);
        return true;
      end;
    end;
    /*Конструктор*/
    InitTRsbPanel();
    InitFillPanel();
    /*Обработчики*/
    addEventHandler(RSB_EV_KEY_PRESSED, r2m(this, "eventHandlerKP"));
    addEventHandler(RSB_EV_REMOVE_FOCUS, r2m(this, "eventHandlerRF"));  
  end;

  /* Получение номера сделки по ид платежа */
  //dockind Таблица Номер связка
  //16  pmrmprop.dbt  number  paymentid
  //101 dl_tick.dbt dealcodets  dealid
  //102 dl_tick.dbt dealcodets  dealid
  //105 dl_comm.dbt commcode  documentid
  //109 dl_order.dbt  ordernumber contractid
  //110 dl_order.dbt  ordernumber contractid
  //140 dl_nett.dbt dealnumber  nettingid
  //141 dl_tick.dbt dealcodets  dealid
  //142 dl_tick.dbt dealcodets  dealid
  //143 dl_tick.dbt dealcodets  dealid
  //159 dl_acc.dbt code id //Simanov
  //194 dvoper.dbt  code  id
  //199 dvndeal.dbt extcode id
  //322 pmrmprop.dbt  number  paymentid
  //4607  nptxop.dbt  code  id
  //4608  nptxop.dbt  code  id
  //4627  dvcsapm.dbt codets  pmid
  //4813  dvndeal.dbt extcode id
  //4815  dvndeal.dbt extcode id
  //4816  dvdeal.dbt  extcode id
  //4832  dl_tick.dbt dealcodets  dealid
  macro getDealNumber(paymentid, dockind)
    var sqlstr, cmd, rs;

    if ((dockind == 16) or (dockind == 322))
      sqlstr="select t_number from dpmrmprop_dbt t, dpmpaym_dbt p where p.t_paymentid=t.t_paymentid and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if ((dockind == 101) or (dockind == 102) or (dockind == 141) or (dockind == 142) or (dockind == 143) or (dockind == 4832))
      sqlstr="select t_dealcode t_number from ddl_tick_dbt t, dpmpaym_dbt p where p.t_documentid=t.t_dealid and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if (dockind == 105)
      sqlstr="select t_commcode t_number from ddl_comm_dbt t, dpmpaym_dbt p where p.t_documentid=t.t_documentid and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if ((dockind == 109) or (dockind == 110))
      sqlstr="select t_ordernumber t_number from ddl_order_dbt t, dpmpaym_dbt p where p.t_documentid=t.t_contractid and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if (dockind == 140)
      sqlstr="select t_dealnumber t_number from ddl_nett_dbt t, dpmpaym_dbt p where p.t_documentid=t.t_nettingid and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if (dockind == 159) 
      sqlstr="select t_code t_number from ddl_acc_dbt t, dpmpaym_dbt p where p.t_documentid=t.id and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if (dockind == 194)
      sqlstr="select t_code t_number from ddvoper_dbt t, dpmpaym_dbt p where p.t_documentid=t.t_id and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if ((dockind == 4607) or (dockind == 4645))
      sqlstr="select t_code t_number from dnptxop_dbt t, dpmpaym_dbt p where p.t_documentid=t.t_id and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if (dockind == 4608)
      sqlstr="select t_code t_number from dnptxop_dbt t, dpmpaym_dbt p where p.t_documentid=t.t_id and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if (dockind == 4627)
      sqlstr="select t_codets t_number from ddvcsapm_dbt t, dpmpaym_dbt p where p.t_documentid=t.t_pmid and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if ((dockind == 199) or (dockind == 4813) or (dockind == 4815))
      sqlstr="select t_extcode t_number from ddvndeal_dbt t, dpmpaym_dbt p where p.t_documentid=t.t_id and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    if (dockind == 4816)
      sqlstr="select t_extcode t_number from ddvdeal_dbt t, dpmpaym_dbt p where p.t_documentid=t.t_id and p.t_dockind=:dockind and p.t_paymentid = :paymentid";
    end;
    cmd = RsdCommand(sqlstr);
    cmd.AddParam("dockind",   RSDBP_IN, dockind);
    cmd.AddParam("paymentid", RSDBP_IN, paymentid);
    cmd.Execute();
    rs = TRsbDataSet(cmd);
    if(rs.MoveNext())
       return rs.value("t_number");
    end;
    return "";
  OnError(er)
    return "";
  end;

  macro getDocKindName(dockind)
    var cmd = RsdCommand("select nvl(oprkdoc.t_name, chr(1)) t_name from doprkdoc_dbt oprkdoc where oprkdoc.t_dockind = ?");
    cmd.AddParam("dockind",  RSDBP_IN, dockind);
    cmd.Execute();
    var rs = TRsbDataSet(cmd);
    if(rs.MoveNext())
      return rs.value("t_name");
    end;
    return "";
  end;

  /*Контроль платежа - проверить, что нет неисполненных платежей от контрагента (требований к контрагенту), где плановая дата меньше, чем текущий операционный день*/
  macro CheckDate(paymentid, dockind, errmsg:@string) 
    return true;
    var dockindname = "";
    var partyname = "";
    var amount = "";
    var dealNumber = "";
    if ((dockind == 16) or (dockind == 101) or (dockind == 105) or (dockind == 140) or (dockind == 194) or (dockind == 322) or (dockind == 4607) or (dockind == 4832)) // не обрабатывать платежи с видом первички (t_dockind) 140 и 194
      return true;
    end;

    var cmd = RsdCommand("select t_payer, t_dockind, t_amount, p.t_name, f.t_name t_fiidname, t_paymentid" +
                         "  from dpmpaym_dbt t, dparty_dbt p, dfininstr_dbt f" +
                         " where t_payer in (select t_payer from dpmpaym_dbt where t_paymentid = ?)" +
                         "   and t_paymstatus != 32000" +
                         "   and t_paymentid != ?" +
                         "   and t_valuedate < ?" +
                         "   and t_payer != ?" +
                         //"   and t_receiver = ?" +
                         "   and t_payer = p.t_partyid(+)" +
                         "   and f.t_fiid = t.t_payfiid" +
                         "   and rownum = 1" +
