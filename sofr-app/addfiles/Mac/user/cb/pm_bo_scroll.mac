  /*Контроль лимитов - Отправить запрос в Лимитный модуль*/
  macro CheckLimit(paymentid, dockind, errmsg:@string) 
    return true;
    var cmd;
    var dockindname = "";
    var dealNumber = "";
    var errcode;

    if ((dockind != 102) and (dockind != 199) and (dockind != 4813) and (dockind != 4815)) // Функция отрабатывается только для документов 102, 199, 4813, 4815, для других видов не запускается.
      return true;
    end;

    if(not ExecDealControlLimit(paymentid, dockind, @errcode))
      dealNumber = getDealNumber(paymentid, dockind);
      if ((valType(dealNumber) == v_string) and (dealNumber != ""))
        dealNumber = " № " + dealNumber;
      end;
      dockindname = getDocKindName(dockind);
      if ((valType(dockindname) == v_string) and (dockindname != ""))
        dockindname = "\"" + dockindname + "\"";
      end;
      if  (errcode == 404)
        errmsg = "Контроль лимитов: Сделка " + dockindname + dealNumber + " не найдена в АСУДР.|Выполнять контроль платежа?";
      elif(errcode == 405)
        errmsg = "Контроль лимитов: Сделка " + dockindname + dealNumber + " получена не из АСУДР. Выполнять контроль платежа?";
      elif(errcode == 406)
        errmsg = "Контроль лимитов: параметры сделки " + dockindname + dealNumber + " не найдена в АСУДР. Выполнять контроль платежа?";
      elif(errcode == 999)
        errmsg = "Контроль лимитов для сделки " + dockindname + dealNumber + " не пройден в АСУДР. Выполнять контроль платежа?";
      else
        errmsg = "Контроль лимитов: Сделка " + dockindname + dealNumber + " не прошла контроль лимитов. Выполнять контроль платежа?";
      end;
      return false;
    end;

    return true;
  end;

  // DEF-42283 обертка в транзакцию
  macro ProcessPaymentForUnload(paymentid, acctrnid)
    var  cmd;
    RslDefCon.BeginTrans();

    cmd = RsdCommand("update dpmpaym_dbt set t_userfield2='1', t_userfield3=chr(1) where t_paymentid="+paymentid);
    cmd.Execute();

    //cmd = RsdCommand("update dacctrn_dbt set t_userfield3='1', t_Number_Pack=155 where t_acctrnid="+acctrnid);
    cmd = RsdCommand("update dacctrn_dbt set t_userfield3='1' where t_acctrnid="+acctrnid);
    cmd.Execute();

    cmd = RsdCommand("update dpmdocs_dbt  set t_acctrnid=0 where t_paymentid="+paymentid);
    cmd.Execute();

    if (RslDefCon.IsInTrans())
       RslDefCon.CommitTrans();
