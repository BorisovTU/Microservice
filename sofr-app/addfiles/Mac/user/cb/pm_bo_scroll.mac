        if(f_chk_status == "")
          f_chk_status = " (t_status in ('Для выгрузки', 'Для подтверждения', 'Подтвержден') ";
        else
          f_chk_status = f_chk_status + " or t_status in ('Для выгрузки', 'Для подтверждения', 'Подтвержден') ";
