import pm_setst, cbsttls, PTInter, CashInter, "pm_answerret.mac", "pmfm.mac";

var PaymentObj:RsbPayment;


MACRO ExecuteStep( doc, paymDoc )

  PaymentObj.PaymStatus = 3010/*проконтролирован*/;  
        
  if(УстановитьСтатусыПлатежа(OPR_PAYM_CONTROL, OPR_PAYM_ST_CTRL_CONTROL))
    msgbox("Ошибка при установке сегментов статуса экземпляра операции");
    return 1;
  end;

  return 0;
END;

