/*----------------------------------------------------------------------------
печатает первую строку сообщения, принимает строку формы , референс и SWIFT БИК
получателя
----------------------------------------------------------------------------*/
macro prheader(form, ID_NKS, bicRecv, SESID, ISN)
  var tag3="";

  if(StrLen(bicRecv) == 8)
    bicRecv = bicRecv + "XXXX";
  elif(StrLen(bicRecv) == 11)
    bicRecv = SubStr(bicRecv,1,8) + "X" + SubStr(bicRecv,9);
  end;

  if  (ID_NKS == ID_NKS_RUR6)
       tag3="{3:{113:RUR6}}";
  elif(ID_NKS == ID_NKS_RUS9)
       tag3="{3:{113:RUS9}}";
  end;



  if(ISN)
    if(strlen(tag3) > 0)
      tag3 = StrSubst(tag3, "}}", "}{108:"+String(ISN)+"}}");
    else
      tag3 = "{3:{108:"+String(ISN)+"}}";
    end;
  end;

  if((SESID) and (ISN))
    ISN = String(ISN);

    if(StrLen(ISN) > 6)
      ISN = SubStr(ISN,StrLen(ISN)-5);
    else
      ISN = Padd(ISN,48,6,1);
    end;

    SESID = Padd(int(SESID),48,4,1);
    return "{1:F01"+MySWIFTBic+"AXXX"+SESID+ISN+"}{2:I"+form +bicRecv+"N}"+tag3+"{4:";
  else
    return "{1:F01"+MySWIFTBic+"AXXX0000000000}{2:I"+form +bicRecv+"N}"+tag3+"{4:";
  end;

end;
