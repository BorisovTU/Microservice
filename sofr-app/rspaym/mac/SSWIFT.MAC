macro LoadXMLKvit()

  var Insert_TMP_XML = TRslOraQuery();
  var LoadSPFS;
  /*Помещаем во временную таблицу*/
  Insert_TMP_XML.SqlText = " insert into TMP_XML (VALXML) values(XMLTYPE(:IMP_STRING)) ";
  Insert_TMP_XML.DeclareAndSetLOB("IMP_STRING", tooem(InpStr));

  if(not Insert_TMP_XML.Execute())
    MakeErrorMsg("Шлюз СПФС: загрузка файла " + NameFile + " Ошибка вставки во временную таблицу - "+Insert_TMP_XML.Error);
    return false;
  end;

  /*Вызываем LoadSPFS*/
  LoadSPFS = TRslOraQuery();

  var Ret = "";
  LoadSPFS.SqlText = " begin  :Ret := RSP_SPFS.LoadSPFS(:TMP_TABLE,:NAME_FILE); end;";
  LoadSPFS.DeclareAndSet("Ret",Ret,"TMP_TABLE","TMP_XML","NAME_FILE",NameFile);

  if(not LoadSPFS.Execute)
    MakeErrorMsg("Шлюз СПФС: загрузка файла " + NameFile + " Ошибка в запросе обработки xml файла");
