macro SaveJRNSPFS(NameFile, paym, msgType, isout, ses)
 var JRNObj = TRSLPaydoc(Type_JRNSPFS,0);
 var JRN = JRNObj.JCONFSPFS;

   JRN.ID_PAYM   = paym.ID_PAYM;
   JRN.FILENAME  = NameFile;
   JRN.TYPE      = msgType;
   if(VALTYPE(ses)!=V_UNDEF)
      JRN.SESSIONNUM = ses;
   end;
   if(isout)
       JRN.DIRECTION = "Исх";
       JRN.SWIFT_BIC = paym.RECEIVERBANKID;
       JRN.ID_SCONFSTATE = JRNSent2SPFS;
   else
       JRN.DIRECTION = "Вх";
       JRN.SWIFT_BIC = paym.PAYERBANKID;
       JRN.ID_SCONFSTATE = JRNReceiveSPFS
   end;

   JRN.DEALNUM = paym.NUMREFERDOC;

  if(not JRNObj.Save())
    CommentStr = JRNObj.Error;
    return false;
  end;
  return true;
end;



