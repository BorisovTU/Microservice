   if (not TstObj (qry))
     CommentStr = "Ошибка при cохранении входящего референса ";
     return false;
   end;


   if(index(KvitForm, MesForm))
       var AllRef="";

       qText = " begin  :p1 := RSP_PAYM_API.GETGENERALREFERENCE (:p2); end;";
       qry = ExecQuery (qText, AllRef, pay.ID_PAYM);

       if (not TstObj (qry))
         CommentStr = "Ошибка запроса определения общего референса";
         return false;
       end;

       AllRef=qry.GetVariable("p1");

       if(strlen(AllRef))
           /* Сохранение входящего общего референса */
           qText = "begin RSP_PAYM_API.CRT_PAYMASSOCIATE (:p1,:p2,:p3, :p4,:p5); end;";
           qry = ExecQuery (qText, AllRef, pay.ID_PAYM, ASS_RECV_ALLREF, pay.PAYERBANKID, pay.ID_NKORRINPUT);
           if (not TstObj (qry))
             CommentStr = "Ошибка при cохранении входящего общего референса ";
      return false;
    end;
       else
          CommentStr = "Невозможно определить общий референс";
       end;
   end;

    if((GateId==SPFSIdGate) AND NOT SaveJRNSPFS(NameFile, pay, MesForm, false, pay.ACCCURRAMOUNT))
      return false;
    end;


    PaymID = int(Pay.id_paym);
    Return true;
  END;

  /*
     Тело функции
  */

  while(index(InpStr, MsgBeginMarker))
    if(LoadOneMessage(GateId,@PaymID))
      MsgLoaded = MsgLoaded + 1;
    else
     if (trim (CommentStr) != "")
      MakeErrorMsg ("Шлюз "+GateId+": " + CommentStr);
     end;
     return false;
    end;
    InpStr = Trim(InpStr);
  end;

  AddPtk("Шлюз "+GateId+": загружено " + MsgLoaded + " сообщений из " + NameFile);
