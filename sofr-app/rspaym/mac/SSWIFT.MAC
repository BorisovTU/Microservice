    MakeErrorMsg("Шлюз СПФС: загрузка файла " + NameFile + " Функция LoadSPFS вернула пустую строку");
    return false;
  elif(Index(Ret,"Исходное ЭС не найдено"))
    InsertEmailNotify(EmailGroup, "Шлюз СПФС: Загрузка xml квитанции", "Квитанцию ED " + substr(Ret, Index(Ret, "ED") + 2, 3)+ " в файле " + NameFile + " невозможно сопоставить с выгруженным из СОФР подтверждением.");
  end;

  MakeErrorMsg("Шлюз СПФС: загрузка файла " + NameFile + " "+Ret);
  return true;

end;


macro LoadABSKvit()
  var cmd = TRslOraQuery();
  var upd = TRslOraQuery();
  var updses = TRslOraQuery();


  cmd.sqlText = "select t.id_jconfspfs,   "+
                "       t.versionrec,     "+
                "       t.id_paym         "+
                "  from JCONFSPFS t       "+
                " where t.filename = :FIL "+
                "   and t.id_sconfstate = " + JRNSent2SPFS +
                " order by t.id_jconfspfs desc";

  cmd.DeclareAndSet("FIL", NameFile);

  if(not cmd.Execute)
     MakeErrorMsg( "Шлюз СПФС: загрузка файла " + NameFile + ", ошибка в запросе поиска отправленных сообщений - "+cmd.Error);
     return false;
  end;

  if(cmd.Eof)
     MakeErrorMsg( "Шлюз СПФС: загрузка файла " + NameFile + ", не найдено отправленных сообщений");
     InsertEmailNotify(EmailGroup, "Шлюз СПФС: Загрузка квитанции из АБС", "Квитанцию из АБС в файле " + NameFile + " невозможно сопоставить с выгруженным из СОФР подтверждением.");
  return true;
  end;

  upd.sqlText = " UPDATE JCONFSPFS t ";

  if(substr(InpStr, 1, strlen(BLOCK1_HEAD)) == BLOCK1_HEAD)
     var ses = substr(InpStr, 19, 10);
//     var EdNo = GetSubStr(InpStr, ",", RN+"{");
     var EdNo = GetSubStr(InpStr, ",");

     if(strlen(EdNo))
        upd.sqlText = upd.sqlText +
        " SET t.ID_SCONFSTATE = :STATE, t.COMMENTAR= :COM, t.EDNOED503 = :EDNO, t.SESSIONNUM = :SES, t.EDDATEED503 = Trunc(sysdate)";
        upd.DeclareAndSet("STATE", JRNWaitKvi, "COM", "ожидание транспортной квитанции", "EDNO", EdNo, "SES", ses);
        updses.sqlText = "UPDATE paym p                 "+
                         "   SET p.acccurramount = :SES "+
                         " WHERE p.id_paym = :ID";
        updses.DeclareAndSet("SES", ses, "ID", cmd.Rec.id_paym);
        if(not updses.Execute)
           MakeErrorMsg("Шлюз СПФС: ошибка сохранения в платеже номера сессии " + ses + "- " + updses.Error);
           return false;
        end;
     else
        MakeErrorMsg( "Шлюз СПФС: загрузка файла " + NameFile + ", не определен номер конверта ED503");
        InsertEmailNotify(EmailGroup, "Шлюз СПФС: Загрузка квитанции из АБС", "Считанная техническая квитанция АБС из файла " + NameFile + " не содержит номеров терминальной сессии и/или номера конверта ED 503 или эти номера не соответствуют формату.");
        return true;
     end;
  else //плохая квитанция
      upd.sqlText = upd.sqlText+
                    "    SET t.ID_SCONFSTATE = :STATE, t.COMMENTAR= :COM";
      upd.DeclareAndSet("STATE", JRNErrABS,"COM",substr(InpStr,1,255));
      InsertEmailNotify(EmailGroup, "Шлюз СПФС: Загрузка квитанции из АБС", "Считанная техническая квитанция АБС из файла " + NameFile + " содержит текст ошибки: " + substr(InpStr,1,255) + ".");
  end;

  upd.sqlText = upd.sqlText+"  WHERE t.id_jconfspfs = :ID and t.versionrec = :VR";
  upd.DeclareAndSet("ID",cmd.Rec.id_jconfspfs, "VR",cmd.Rec.versionrec);

  if(not upd.Execute)
     MakeErrorMsg( "Шлюз СПФС: загрузка файла " + NameFile + ", ошибка в запросе обновления отправленных сообщений - "+upd.Error);
  return false;
  end;

  if(upd.RowsProcessed !=1)
         MakeErrorMsg( "Шлюз СПФС: загрузка файла " + NameFile + ", запись отправленного сообщения конкурентно изменена");
  else
         MakeErrorMsg( "Шлюз СПФС: обработана квитанция АБС из файла " + NameFile);
  end;
  return true;
end;



macro LoadMultiMsgFile(fullNameFile,GateId)
  InpStr = LoadFileString(fullNameFile);
  MsgBeginMarker = BLOCK1_HEAD;

  if(GateId==SPFSIdGate)
   if(substr(InpStr,1,5)=="<?xml") // это 201 или 508
    if(not LoadXMLKvit)
      OraTrnRollback();
      return false;
    end;
    OraTrnCommit();
    return true;
   end;

   var iskvit = (strlen(NameFile)== 12 ) AND (strupr(substr(NameFile,9))==".BNK");
   if(iskvit AND (substr(InpStr,1,strlen(MsgBeginMarker))!= MsgBeginMarker))
    ;
   elif(iskvit)
      var tmstr = SubStr(InpStr, index(InpStr,RN)+2, strlen(BLOCK2_HEAD));
//      if(tmstr!=BLOCK2_HEAD)
//        iskvit = false;
//      end;
   end;

   if(iskvit)
    if(not LoadABSKvit())
      OraTrnRollback();
      return false;
    end;
    OraTrnCommit();
    return true;
   end;
  end;

  if(index(InpStr, MsgBeginMarker)) /* Есть хотя бы одно сообщение */
    if(not LoadMultiMsgFileTrn(GateId))
      OraTrnRollback();
      return false;
    end;
    OraTrnCommit();
  elif(GateId==SPFSIdGate)
    InsertEmailNotify(EmailGroup, "Шлюз СПФС: Загрузка файла", "Скопированный с сервера файлового обмена файл " + NameFile + " нечитабелен (некорректное имя/расширение, неизвестная кодировка, неверная структура, отсутствующие или неполные блоки SWIFT/тэги).");
  end;
  return true;
end;


Macro ImpFiles(GateId, ImpDir, ArcDir, ImpFileNameMask)
  NameFile = ImpDir + ImpFileNameMask;
  Var OnceMoreFile = FindFirst(NameFile), newNameFile;

  While(OnceMoreFile)
    newNameFile  = ArcDir+NameFile;
    OrigNameFile = ImpDir+NameFile;
    If (ExistFile (newNameFile) and DeleteFile(newNameFile))
      AddPtk(GateId+":Удалён файл "+newNameFile);
    End;

    If (MoveFile( OrigNameFile, newNameFile))
      AddPtk(GateId+":Перемещен файл "+NameFile+ " из "+ImpDir+" в "+ArcDir);
      If(not LoadMultiMsgFile(newNameFile,GateId))
        /* нужно попытаться перенести файл обратно */
        If (MoveFile(newNameFile, OrigNameFile))
           AddPtk(GateId+":Возвращён файл "+NameFile+" из "+ArcDir+" в "+ImpDir);
        Else
           MakeErrorMsg (GateId+":Ошибка при возврате файла "+NameFile+" из "+ArcDir+" в "+ImpDir);
        End;
      End;
    Else
      MakeErrorMsg (GateId+":Ошибка переноса файла "+NameFile+" из "+ImpDir+" в "+ArcDir);
    End;
    OnceMoreFile = FindNext(NameFile);
  End;
End;

