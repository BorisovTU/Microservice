MACRO TelexHeader (msgType, paym, OrigNameFile)
  return String("", "SFT YZYZ" + RN)+RN+
         String("FROM:", paym.PAYERBANKID) +RN+
         String("     ", "MOSCOW, RUSSIA") +RN+
         String("TO  :", paym.RECEIVERBANKID) +RN+
         String("     ", " ") +RN+
         String("DATE:", Date2SWIFT(date()))+RN+
         String("FILE NAME:", OrigNameFile + RN)+RN+
         String("", "::" + MsgType + RN+RN);

END;
