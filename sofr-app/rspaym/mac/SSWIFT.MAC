MACRO UnloadDocTrn(paymObj)
   var SentReference = "", NameFile, ArcPath = "", OutPath="", paym, mtrec, hdrform = "", tmp, strmsg;
   var ID_NKS = "";
   var fldIdxArr;
   var CurFld, CurFldNum, StrForTranslit;
   var i;
   var TextStart;
   var PrefixStr;
   Array TranslitedArr;
   var StateSent;
   paym  = paymObj.paym;
   if(paym.id_nform == Type_Doc_MTn9X)
     mtrec = paymObj.mtn9x;
   else
     mtrec = paymObj.mtrec;
   end;

   /* определение имени файла */
   if(paym.id_sstate == StateToSWIFT)
      NameFile = GetNewNameFile(paymObj);
      ArcPath = SwiftArcDir + NameFile;
      OutPath = SwiftOutDir + NameFile;
      StateSent = StateSent2SWIFT;
   elif(paym.id_sstate == StateToTELEX)
      NameFile = GetNewNameFile(paymObj, ".txt");
      ArcPath = TelexArcDir + NameFile;
      OutPath = TelexOutDir + NameFile;
      StateSent = StateSent2Telex;
   elif(paym.id_sstate == StateToSPFS)
      NameFile = GetNewNameFile(paymObj,".bnk");
      ArcPath = SPFSArcDir + NameFile;
      OutPath = SPFSOutDir + NameFile;
      StateSent = StateSent2SPFS;
   end;

   /* сохранение типа сообщения */
   hdrform = int(mtrec.type);

   /* подготовка строки сообщения для записи в файл */
   ID_NKS = GetFrnSys(paym.ID_NKSOUTPUT);

   if((hdrform == 599) or (hdrform == 399))
     strmsg = mtrec.text;
   else
     strmsg = mtrec.str;
   end;

   fldIdxArr = Re_SearchStr(RN+strmsg, "(?m)\\xd\\xa:\\d{2}[A-Z]?.*:", 1, 1);

   if (not fldIdxArr.size OR mod (fldIdxArr, 2))
     CommentStr = "Ошибка при разборе полей сообщения ";
     return false;
   end;


   if (not ParceFields2 (RN+strmsg, fldIdxArr))
     CommentStr = ErrMsg;
     return false;
   end;

   CurFldNum = 0;
   while(CurFldNum < CurMsgFields.Size)

     CurFld = CurMsgFields[CurFldNum];
     StrForTranslit = "";

     if(ValType(CurFld) != V_UNDEF)
       if(CurFld.Num == "35")
         var TransName = false;
         i = 0;
         while(i < CurFld.SubFields.size)
           if(   (not index(CurFld.GetSubField(i),"ISIN"))
              and(not index(CurFld.GetSubField(i),"/RU/"))
//              and(not index(CurFld.GetSubField(i),"/XX/CORP/")))/*если пошла информация о ФИ в текстовом виде*/
              and(not index(CurFld.GetSubField(i),"/XX/")))/*если пошла информация о ФИ в текстовом виде*/
              if(StrForTranslit == "")/*если еще не взяли поле*/
                StrForTranslit = Trim(CurFld.GetFldString(i,false,false));
                TextStart = i;/*Запомним, с какой подстроки начинается текст*/
              end;
              CurFld.Clear(i);/*чистим подполе*/
              TransName = true;
           end;
           i = i + 1;
         end;

         if(TransName)
            TranslitStrAndSplit(StrForTranslit,0,ID_NKS,TranslitedArr,35);
            CopyArrToSubfields(CurFld,TranslitedArr,TextStart);
         end;

       elif(   (CurFld.Num == "70")
            or((CurFld.Num == "95")and(CurFld.Option == "Q")))

         StrForTranslit = Trim(CurFld.GetFldString(0,false,false));

         TextStart = 0;
         ClearSubFields(CurFld,TextStart);
         TranslitStrAndSplit(StrForTranslit,0,ID_NKS,TranslitedArr,35);

         CopyArrToSubfields(CurFld,TranslitedArr,TextStart);
       elif   (CurFld.Num == "36")

         StrForTranslit = Trim(CurFld.GetFldString(0,false,false));

         TextStart = 0;
         ClearSubFields(CurFld,TextStart);
         TranslitStrAndSplit(StrForTranslit,0,ID_NKS,TranslitedArr,35);

         CopyArrToSubfields(CurFld,TranslitedArr,TextStart);
       elif    (CurFld.Num == "97")
         StrForTranslit = Trim(CurFld.GetFldString(0,false,false));
         PrefixStr = SubStr(StrForTranslit,1,7);/*первые 5 символов + 2 слеша - не транслитерируем*/
         StrForTranslit = "12345//"+SubStr(StrForTranslit,8);

         TextStart = 0;
         ClearSubFields(CurFld,TextStart);
         TranslitStrAndSplit(StrForTranslit,0,ID_NKS,TranslitedArr,42);
         TranslitedArr(0) = PrefixStr + SubStr(TranslitedArr(0),8);

         CopyArrToSubfields(CurFld,TranslitedArr,TextStart);
       elif   (CurFld.Num == "79")

         StrForTranslit = Trim(CurFld.GetFldString(0,false,false));

         TextStart = 0;
         ClearSubFields(CurFld,TextStart);
         TranslitStrAndSplit(StrForTranslit,0,ID_NKS,TranslitedArr,50," ");

         CopyArrToSubfields(CurFld,TranslitedArr,TextStart);
       elif((CurFld.Num == "77")and(CurFld.Option == "D"))

         StrForTranslit = Trim(CurFld.GetFldString(0,false,false));

         TextStart = 0;
         ClearSubFields(CurFld,TextStart);
         TranslitStrAndSplit(StrForTranslit,0,ID_NKS,TranslitedArr,35);

         CopyArrToSubfields(CurFld,TranslitedArr,TextStart);
       elif((CurFld.Num == "37")and(CurFld.Option == "N"))

         StrForTranslit = Trim(CurFld.GetFldString(0,false,false));

         TextStart = 0;
         ClearSubFields(CurFld,TextStart);
         TranslitStrAndSplit(StrForTranslit,0,ID_NKS,TranslitedArr,35);

         CopyArrToSubfields(CurFld,TranslitedArr,TextStart);
/*
       elif((CurFld.Num == "57")and(CurFld.Option == "D"))
         TransName = false;
         i = 0;
         while(i < CurFld.SubFields.size)
           if(StrForTranslit == "")/*если еще не взяли поле*/
              StrForTranslit = CurFld.GetFldString(0,false,false);
              TextStart = i;/*Запомним, с какой подстроки начинается текст*/
           else
              StrForTranslit = StrForTranslit + CurFld.GetFldString(0,false,false);
           end;
           CurFld.Clear(i);/*чистим подполе*/
           TransName = true;
           i = i + 1;
         end;
         if(TransName)
            TranslitStrAndSplit(StrForTranslit,0,ID_NKS,TranslitedArr,35);
            CopyArrToSubfields(CurFld,TranslitedArr,TextStart);
         end;
       elif((CurFld.Num == "57")and(CurFld.Option == "D"))

         StrForTranslit = Trim(CurFld.GetFldString(0,false,false));

         TextStart = 0;
         ClearSubFields(CurFld,TextStart);
         TranslitStrAndSplit(StrForTranslit,0,ID_NKS,TranslitedArr,35);

         CopyArrToSubfields(CurFld,TranslitedArr,TextStart);
*/
       end;
     end;

     CurFldNum = CurFldNum + 1;
   end;

//Формат SWIFT
/*==========================================================================================================================*/
   if(paym.id_sstate == StateToSPFS)
     tmp = prheader(hdrform, /*SentReference*/ID_NKS, paym.ReceiverBankID, int(paym.Versionrec)/*0*/, int(paym.NodePaymentId)) + RN;
     paym.ACCCURRAMOUNT=substr(tmp,19,10);
   elif(paym.id_sstate == StateToTELEX)
     tmp = TelexHeader (hdrform, paym, NameFile) + RN;
   else
     tmp = prheader(hdrform, /*SentReference*/ID_NKS, paym.ReceiverBankID) + RN;
     paym.ACCCURRAMOUNT=substr(tmp,19,10);
   end;

   i = 0;
   while(i < CurMsgFields.Size)
     CurFld = CurMsgFields[i];
     if(ValType(CurFld) != V_UNDEF)
       tmp = tmp + CurFld.GetFldString(Null, False, True);
     end;
     i = i + 1;
   end;

   if(paym.id_sstate == StateToTELEX)
     tmp = tmp + RN + ":END:" + RN + TlxEndTag;
   else
   tmp = tmp + prend();

     if(paym.id_sstate == StateToSPFS)
       tmp = tmp + "{5:{MAC:00000000}{CHK:00000000}}";
     end;
   end;

   var hash = "";//, ver;

   if((MDG == 1)and(paym.id_sstate != StateToTELEX))
      if(hmacsha(tmp, Key, hash  ) == 0)
             tmp = tmp + "{S:{MDG:"+hash+"}}";
      else
            CommentStr = "Ошибка вычисляния значения Хэша ";
            return false;
      end;
   end;


/*==========================================================================================================================*/

   if (not SaveExp (paymObj, Tmp, Type_Exp_Origin, "Отправленное сообщение", CommentStr))
      return false;
   end;

   /* Сохранение исходящего референса */
   var qText = "begin RSP_PAYM_API.CRT_PAYMASSOCIATE (:p1,:p2,:p3,:p4,:p5); end;";
   var qry = ExecQuery (qText, paym.NUMREFERDOC, paym.ID_PAYM, ASS_SENT_REF, paym.RECEIVERBANKID, paym.ID_NKORROUTPUT);
   if (not TstObj (qry))
     CommentStr = "Ошибка при cохранении исходящего референса ";
     return false;
   end;

   if((paym.id_sstate == StateToSPFS) AND not SaveJRNSPFS(NameFile, paym, hdrform, true))
     return false;
   end;

   var AllRef="";
   qText = " begin  :p1 := RSP_PAYM_API.GETGENERALREFERENCE (:p2); end;";
   qry = ExecQuery (qText, AllRef, paym.ID_PAYM);

   if (not TstObj (qry))
     CommentStr = "Ошибка запроса определения общего референса";
     return false;
   end;

   AllRef = qry.GetVariable("p1");

   if(strlen(AllRef))
       /* Сохранение исходящего общего референса */
       qText = "begin RSP_PAYM_API.CRT_PAYMASSOCIATE (:p1,:p2,:p3,:p4,:p5); end;";
       qry = ExecQuery (qText, AllRef, paym.ID_PAYM, ASS_SENT_ALLREF, paym.RECEIVERBANKID, paym.ID_NKORROUTPUT);
       if (not TstObj (qry))
         CommentStr = "Ошибка при cохранении исходящего общего референса ";
         return false;
       end;
     else
      CommentStr = "Невозможно определить общий референс";
   end;

   if(not paym.STATEKVIOUTPUT and (paym.id_sstate != StateToSPFS) and (index(KvitForm,hdrform)))
     var Kwit = TRslOraQuery();
     var IDKvit = 0;
     Kwit.SQLText = "begin :p1 := RSP_KVITSWIFT.KvitOutSWIFT(:p2); end;";
     Kwit.DeclareAndSet("p1", IDKvit, "p2", paym.id_paym);

     if(not Kwit.Execute)
       MakeErrorMsg("Квитовка SWIFT: " + "KvitInSWIFT: " + Kwit.Error);
       return false;
     end;

     IDKvit = Kwit.GetVariable("p1");

     if(IDKvit)
        AddPtk("Сообщение ИД=" + ToID(paym.id_paym) + " сквитовано с ИД=" + IDKvit);
     end;
   end;

   CommentStr = "";

   if(not paymObj.ChangeState(StateSent, 0,"сформирован файл "+NameFile))
     CommentStr = "ошибка " + paymObj.Error + " при изменении состояния платежа " + ToId(paymObj.id_paym);
     return false;
   end;

   if(StateSent!= StateSent2SPFS)
     InsertDLMesParams(ToID(paym.id_paym), StateSent);
   end;

   return (WriteStrToFile(tmp, ArcPath) AND CloseAndCopy("Отправка в "+strTransPtk+":", ArcPath, OutPath));

END;

