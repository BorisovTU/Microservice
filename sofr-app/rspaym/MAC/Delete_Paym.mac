Macro DeletePaym()

  var DelPaym;
  
  /*Очистка таблиц платежей и ЭПД*/
  DelPaym = TRslOraQuery();

  DelPaym.SqlText = "begin RSP_COMMON.DeletePAYM; end;";

  if(not VclGetTrue("Хотите очистить?"))
    return false;
  end;

  if(not DelPaym.Execute)
    RunError(toansi(DelPaym.Error));
  end;

End;
